            "id": 5303,
            "verse_key": "68:32",
            "text": "عَسٰى رَبُّنَاۤ اَنۡ يُّبۡدِلَـنَا خَيۡرًا مِّنۡهَاۤ اِنَّاۤ اِلٰى رَبِّنَا رٰغِبُوۡنَ‏"
        },
        {
            "id": 5304,
            "verse_key": "68:33",
            "text": "كَذٰلِكَ الۡعَذَابُ​ؕ وَلَعَذَابُ الۡاٰخِرَةِ اَكۡبَرُ ​ۘ لَوۡ كَانُوۡا يَعۡلَمُوۡنَ‏ ﻿﻿"
        },
        {
            "id": 5305,
            "verse_key": "68:34",
            "text": "اِنَّ لِلۡمُتَّقِيۡنَ عِنۡدَ رَبِّهِمۡ جَنّٰتِ النَّعِيۡمِ‏"
        },
        {
            "id": 5306,
            "verse_key": "68:35",
            "text": "اَفَنَجۡعَلُ الۡمُسۡلِمِيۡنَ كَالۡمُجۡرِمِيۡنَؕ‏"
        },
        {
            "id": 5307,
            "verse_key": "68:36",
            "text": "مَا لَـكُمۡ كَيۡفَ تَحۡكُمُوۡنَ​ۚ‏"
        },
        {
            "id": 5308,
            "verse_key": "68:37",
            "text": "اَمۡ لَـكُمۡ كِتٰبٌ فِيۡهِ تَدۡرُسُوۡنَۙ‏"
        },
        {
            "id": 5309,
            "verse_key": "68:38",
            "text": "اِنَّ لَـكُمۡ فِيۡهِ لَمَا تَخَيَّرُوۡنَ​ۚ‏"
        },
        {
            "id": 5310,
            "verse_key": "68:39",
            "text": "اَمۡ لَـكُمۡ اَيۡمَانٌ عَلَيۡنَا بَالِغَةٌ اِلٰى يَوۡمِ الۡقِيٰمَةِ​ ۙ اِنَّ لَـكُمۡ لَمَا تَحۡكُمُوۡنَ​ۚ‏"
        },
        {
            "id": 5311,
            "verse_key": "68:40",
            "text": "سَلۡهُمۡ اَيُّهُمۡ بِذٰلِكَ زَعِيۡمٌ ۛۚ‏"
        },
        {
            "id": 5312,
            "verse_key": "68:41",
            "text": "اَمۡ لَهُمۡ شُرَكَآءُ ۛۚ فَلۡيَاۡتُوۡا بِشُرَكَآٮِٕهِمۡ اِنۡ كَانُوۡا صٰدِقِيۡنَ‏ ﻿﻿"
        },
        {
            "id": 5313,
            "verse_key": "68:42",
            "text": "يَوۡمَ يُكۡشَفُ عَنۡ سَاقٍ وَّيُدۡعَوۡنَ اِلَى السُّجُوۡدِ فَلَا يَسۡتَطِيۡعُوۡنَۙ‏ ﻿﻿"
        },
        {
            "id": 5314,
            "verse_key": "68:43",
            "text": "خَاشِعَةً اَبۡصَارُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٌ ؕ وَقَدۡ كَانُوۡا يُدۡعَوۡنَ اِلَى السُّجُوۡدِ وَهُمۡ سٰلِمُوۡنَ‏"
        },
        {
            "id": 5315,
            "verse_key": "68:44",
            "text": "فَذَرۡنِىۡ وَمَنۡ يُّكَذِّبُ بِهٰذَا الۡحَـدِيۡثِ​ؕ سَنَسۡتَدۡرِجُهُمۡ مِّنۡ حَيۡثُ لَا يَعۡلَمُوۡنَۙ‏"
        },
        {
            "id": 5316,
            "verse_key": "68:45",
            "text": "وَاُمۡلِىۡ لَهُمۡ​ؕ اِنَّ كَيۡدِىۡ مَتِيۡنٌ‏"
        },
        {
            "id": 5317,
            "verse_key": "68:46",
            "text": "اَمۡ تَسۡـَٔـلُهُمۡ اَجۡرًا فَهُمۡ مِّنۡ مَّغۡرَمٍ مُّثۡقَلُوۡنَ​ۚ‏"
        },
        {
            "id": 5318,
            "verse_key": "68:47",
            "text": "اَمۡ عِنۡدَهُمُ الۡغَيۡبُ فَهُمۡ يَكۡتُبُوۡنَ‏"
        },
        {
            "id": 5319,
            "verse_key": "68:48",
            "text": "فَاصۡبِرۡ لِحُكۡمِ رَبِّكَ وَلَا تَكُنۡ كَصَاحِبِ الۡحُوۡتِ​ۘ اِذۡ نَادٰى وَهُوَ مَكۡظُوۡمٌؕ‏"
        },
        {
            "id": 5320,
            "verse_key": "68:49",
            "text": "لَوۡلَاۤ اَنۡ تَدٰرَكَهٗ نِعۡمَةٌ مِّنۡ رَّبِّهٖ لَنُبِذَ بِالۡعَرَآءِ وَهُوَ مَذۡمُوۡمٌ‏"
        },
        {
            "id": 5321,
            "verse_key": "68:50",
            "text": "فَاجۡتَبٰهُ رَبُّهٗ فَجَعَلَهٗ مِنَ الصّٰلِحِيۡنَ‏"
        },
        {
            "id": 5322,
            "verse_key": "68:51",
            "text": "وَاِنۡ يَّكَادُ الَّذِيۡنَ كَفَرُوۡا لَيُزۡلِقُوۡنَكَ بِاَبۡصَارِهِمۡ لَمَّا سَمِعُوا الذِّكۡرَ وَيَقُوۡلُوۡنَ اِنَّهٗ لَمَجۡنُوۡنٌ​ۘ‏"
        },
        {
            "id": 5323,
            "verse_key": "68:52",
            "text": "وَمَا هُوَ اِلَّا ذِكۡرٌ لِّلۡعٰلَمِيۡنَ‏"
        },
        {
            "id": 5324,
            "verse_key": "69:1",
            "text": "اَلۡحَـآقَّةُ ۙ‏"
        },
        {
            "id": 5325,
            "verse_key": "69:2",
            "text": "مَا الۡحَـآقَّةُ​ ۚ‏"
        },
        {
            "id": 5326,
            "verse_key": "69:3",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا الۡحَــآقَّةُ ؕ‏"
        },
        {
            "id": 5327,
            "verse_key": "69:4",
            "text": "كَذَّبَتۡ ثَمُوۡدُ وَعَادٌۢ بِالۡقَارِعَةِ‏"
        },
        {
            "id": 5328,
            "verse_key": "69:5",
            "text": "فَاَمَّا ثَمُوۡدُ فَاُهۡلِكُوۡا بِالطَّاغِيَةِ‏"
        },
        {
            "id": 5329,
            "verse_key": "69:6",
            "text": "وَاَمَّا عَادٌ فَاُهۡلِكُوۡا بِرِيۡحٍ صَرۡصَرٍ عَاتِيَةٍۙ‏"
        },
        {
            "id": 5330,
            "verse_key": "69:7",
            "text": "سَخَّرَهَا عَلَيۡهِمۡ سَبۡعَ لَيَالٍ وَّثَمٰنِيَةَ اَيَّامٍۙ حُسُوۡمًا ۙ فَتَرَى الۡقَوۡمَ فِيۡهَا صَرۡعٰىۙ كَاَنَّهُمۡ اَعۡجَازُ نَخۡلٍ خَاوِيَةٍ​ ۚ‏"
        },
        {
            "id": 5331,
            "verse_key": "69:8",
            "text": "فَهَلۡ تَرٰى لَهُمۡ مِّنۡۢ بَاقِيَةٍ‏"
        },
        {
            "id": 5332,
            "verse_key": "69:9",
            "text": "وَجَآءَ فِرۡعَوۡنُ وَمَنۡ قَبۡلَهٗ وَالۡمُؤۡتَفِكٰتُ بِالۡخَـاطِئَةِ​ۚ‏"
        },
        {
            "id": 5333,
            "verse_key": "69:10",
            "text": "فَعَصَوۡا رَسُوۡلَ رَبِّهِمۡ فَاَخَذَهُمۡ اَخۡذَةً رَّابِيَةً‏"
        },
        {
            "id": 5334,
            "verse_key": "69:11",
            "text": "اِنَّا لَمَّا طَغَا الۡمَآءُ حَمَلۡنٰكُمۡ فِى الۡجَارِيَةِ ۙ‏"
        },
        {
            "id": 5335,
            "verse_key": "69:12",
            "text": "لِنَجۡعَلَهَا لَـكُمۡ تَذۡكِرَةً وَّتَعِيَهَاۤ اُذُنٌ وَّاعِيَةٌ‏"
        },
        {
            "id": 5336,
            "verse_key": "69:13",
            "text": "فَاِذَا نُفِخَ فِى الصُّوۡرِ نَفۡخَةٌ وَّاحِدَةٌ ۙ‏"
        },
        {
            "id": 5337,
            "verse_key": "69:14",
            "text": "وَحُمِلَتِ الۡاَرۡضُ وَ الۡجِبَالُ فَدُكَّتَا دَكَّةً وَّاحِدَةً ۙ‏"
        },
        {
            "id": 5338,
            "verse_key": "69:15",
            "text": "فَيَوۡمَٮِٕذٍ وَّقَعَتِ الۡوَاقِعَةُ ۙ‏"
        },
        {
            "id": 5339,
            "verse_key": "69:16",
            "text": "وَانْشَقَّتِ السَّمَآءُ فَهِىَ يَوۡمَٮِٕذٍ وَّاهِيَةٌ ۙ‏"
        },
        {
            "id": 5340,
            "verse_key": "69:17",
            "text": "وَّالۡمَلَكُ عَلٰٓى اَرۡجَآٮِٕهَا ​ؕ وَيَحۡمِلُ عَرۡشَ رَبِّكَ فَوۡقَهُمۡ يَوۡمَٮِٕذٍ ثَمٰنِيَةٌ ؕ‏"
        },
        {
            "id": 5341,
            "verse_key": "69:18",
            "text": "يَوۡمَٮِٕذٍ تُعۡرَضُوۡنَ لَا تَخۡفٰى مِنۡكُمۡ خَافِيَةٌ‏"
        },
        {
            "id": 5342,
            "verse_key": "69:19",
            "text": "فَاَمَّا مَنۡ اُوۡتِىَ كِتٰبَهٗ بِيَمِيۡنِهٖۙ فَيَقُوۡلُ هَآؤُمُ اقۡرَءُوۡا كِتٰبِيَهۡ​ۚ‏"
        },
        {
            "id": 5343,
            "verse_key": "69:20",
            "text": "اِنِّىۡ ظَنَنۡتُ اَنِّىۡ مُلٰقٍ حِسَابِيَهۡ​ۚ‏"
        },
        {
            "id": 5344,
            "verse_key": "69:21",
            "text": "فَهُوَ فِىۡ عِيۡشَةٍ رَّاضِيَةٍۙ‏"
        },
        {
            "id": 5345,
            "verse_key": "69:22",
            "text": "فِىۡ جَنَّةٍ عَالِيَةٍۙ‏"
        },
        {
            "id": 5346,
            "verse_key": "69:23",
            "text": "قُطُوۡفُهَا دَانِيَةٌ‏"
        },
        {
            "id": 5347,
            "verse_key": "69:24",
            "text": "كُلُوۡا وَاشۡرَبُوۡا هَنِيۡٓـــًٔا ۢ بِمَاۤ اَسۡلَفۡتُمۡ فِى الۡاَيَّامِ الۡخَـالِيَةِ‏ ﻿﻿"
        },
        {
            "id": 5348,
            "verse_key": "69:25",
            "text": "وَاَمَّا مَنۡ اُوۡتِىَ كِتٰبَهٗ بِشِمَالِهٖ  ۙ فَيَقُوۡلُ يٰلَيۡتَنِىۡ لَمۡ اُوۡتَ كِتٰبِيَهۡ​ۚ‏"
        },
        {
            "id": 5349,
            "verse_key": "69:26",
            "text": "وَلَمۡ اَدۡرِ مَا حِسَابِيَهۡ​ۚ‏"
        },
        {
            "id": 5350,
            "verse_key": "69:27",
            "text": "يٰلَيۡتَهَا كَانَتِ الۡقَاضِيَةَ​ ۚ‏"
        },
        {
            "id": 5351,
            "verse_key": "69:28",
            "text": "مَاۤ اَغۡنٰى عَنِّىۡ مَالِيَهۡۚ‏"
        },
        {
            "id": 5352,
            "verse_key": "69:29",
            "text": "هَلَكَ عَنِّىۡ سُلۡطٰنِيَهۡ​ۚ‏"
        },
        {
            "id": 5353,
            "verse_key": "69:30",
            "text": "خُذُوۡهُ فَغُلُّوۡهُ ۙ‏"
        },
        {
            "id": 5354,
            "verse_key": "69:31",
            "text": "ثُمَّ الۡجَحِيۡمَ صَلُّوۡهُ ۙ‏"
        },
        {
            "id": 5355,
            "verse_key": "69:32",
            "text": "ثُمَّ فِىۡ سِلۡسِلَةٍ ذَرۡعُهَا سَبۡعُوۡنَ ذِرَاعًا فَاسۡلُكُوۡهُ ؕ‏"
        },
        {
            "id": 5356,
            "verse_key": "69:33",
            "text": "اِنَّهٗ كَانَ لَا يُؤۡمِنُ بِاللّٰهِ الۡعَظِيۡمِۙ‏"
        },
        {
            "id": 5357,
            "verse_key": "69:34",
            "text": "وَلَا يَحُضُّ عَلٰى طَعَامِ الۡمِسۡكِيۡنِؕ‏"
        },
        {
            "id": 5358,
            "verse_key": "69:35",
            "text": "فَلَيۡسَ لَـهُ الۡيَوۡمَ هٰهُنَا حَمِيۡمٌۙ‏"
        },
        {
            "id": 5359,
            "verse_key": "69:36",
            "text": "وَّلَا طَعَامٌ اِلَّا مِنۡ غِسۡلِيۡنٍۙ‏"
        },
        {
            "id": 5360,
            "verse_key": "69:37",
            "text": "لَّا يَاۡكُلُهٗۤ اِلَّا الۡخٰطِئُوْنَ‏"
        },
        {
            "id": 5361,
            "verse_key": "69:38",
            "text": "فَلَاۤ اُقۡسِمُ بِمَا تُبۡصِرُوۡنَۙ‏"
        },
        {
            "id": 5362,
            "verse_key": "69:39",
            "text": "وَمَا لَا تُبۡصِرُوۡنَۙ‏"
        },
        {
            "id": 5363,
            "verse_key": "69:40",
            "text": "اِنَّهٗ لَقَوۡلُ رَسُوۡلٍ كَرِيۡمٍۚ ۙ‏"
        },
        {
            "id": 5364,
            "verse_key": "69:41",
            "text": "وَّمَا هُوَ بِقَوۡلِ شَاعِرٍ​ؕ قَلِيۡلًا مَّا تُؤۡمِنُوۡنَۙ‏"
        },
        {
            "id": 5365,
            "verse_key": "69:42",
            "text": "وَلَا بِقَوۡلِ كَاهِنٍ​ؕ قَلِيۡلًا مَّا تَذَكَّرُوۡنَؕ‏"
        },
        {
            "id": 5366,
            "verse_key": "69:43",
            "text": "تَنۡزِيۡلٌ مِّنۡ رَّبِّ الۡعٰلَمِيۡنَ‏"
        },
        {
            "id": 5367,
            "verse_key": "69:44",
            "text": "وَلَوۡ تَقَوَّلَ عَلَيۡنَا بَعۡضَ الۡاَقَاوِيۡلِۙ‏"
        },
        {
            "id": 5368,
            "verse_key": "69:45",
            "text": "لَاَخَذۡنَا مِنۡهُ بِالۡيَمِيۡنِۙ‏"
        },
        {
            "id": 5369,
            "verse_key": "69:46",
            "text": "ثُمَّ لَقَطَعۡنَا مِنۡهُ الۡوَتِيۡنَ  ۖ‏"
        },
        {
            "id": 5370,
            "verse_key": "69:47",
            "text": "فَمَا مِنۡكُمۡ مِّنۡ اَحَدٍ عَنۡهُ حَاجِزِيۡنَ‏"
        },
        {
            "id": 5371,
            "verse_key": "69:48",
            "text": "وَاِنَّهٗ لَتَذۡكِرَةٌ لِّلۡمُتَّقِيۡنَ‏"
        },
        {
            "id": 5372,
            "verse_key": "69:49",
            "text": "وَاِنَّا لَنَعۡلَمُ اَنَّ مِنۡكُمۡ مُّكَذِّبِيۡنَ‏"
        },
        {
            "id": 5373,
            "verse_key": "69:50",
            "text": "وَاِنَّهٗ لَحَسۡرَةٌ عَلَى الۡكٰفِرِيۡنَ‏"
        },
        {
            "id": 5374,
            "verse_key": "69:51",
            "text": " وَاِنَّهٗ لَحَـقُّ الۡيَقِيۡنِ‏"
        },
        {
            "id": 5375,
            "verse_key": "69:52",
            "text": "فَسَبِّحۡ بِاسۡمِ رَبِّكَ الۡعَظِيۡمِ‏"
        },
        {
            "id": 5376,
            "verse_key": "70:1",
            "text": "سَاَلَ سَآٮِٕلٌ ۢ بِعَذَابٍ وَّاقِعٍۙ‏"
        },
        {
            "id": 5377,
            "verse_key": "70:2",
            "text": "لِّلۡكٰفِرِيۡنَ لَيۡسَ لَهٗ دَافِعٌ ۙ‏"
        },
        {
            "id": 5378,
            "verse_key": "70:3",
            "text": "مِّنَ اللّٰهِ ذِى الۡمَعَارِجِؕ‏"
        },
        {
            "id": 5379,
            "verse_key": "70:4",
            "text": "تَعۡرُجُ الۡمَلٰٓٮِٕكَةُ وَ الرُّوۡحُ اِلَيۡهِ فِىۡ يَوۡمٍ كَانَ مِقۡدَارُهٗ خَمۡسِيۡنَ اَلۡفَ سَنَةٍ​ۚ‏"
        },
        {
            "id": 5380,
            "verse_key": "70:5",
            "text": "فَاصۡبِرۡ صَبۡرًا جَمِيۡلًا‏"
        },
        {
            "id": 5381,
            "verse_key": "70:6",
            "text": "اِنَّهُمۡ يَرَوۡنَهٗ بَعِيۡدًا ۙ‏"
        },
        {
            "id": 5382,
            "verse_key": "70:7",
            "text": "وَّنَرٰٮهُ قَرِيۡبًا ؕ‏"
        },
        {
            "id": 5383,
            "verse_key": "70:8",
            "text": "يَوۡمَ تَكُوۡنُ السَّمَآءُ كَالۡمُهۡلِۙ‏"
        },
        {
            "id": 5384,
            "verse_key": "70:9",
            "text": "وَتَكُوۡنُ الۡجِبَالُ كَالۡعِهۡنِۙ‏"
        },
        {
            "id": 5385,
            "verse_key": "70:10",
            "text": "وَلَا يَسۡـَٔـلُ حَمِيۡمٌ حَمِيۡمًا ۖۚ‏"
        },
        {
            "id": 5386,
            "verse_key": "70:11",
            "text": "يُّبَصَّرُوۡنَهُمۡ​ؕ يَوَدُّ الۡمُجۡرِمُ لَوۡ يَفۡتَدِىۡ مِنۡ عَذَابِ يَوۡمِٮِٕذٍۢ بِبَنِيۡهِۙ‏"
        },
        {
            "id": 5387,
            "verse_key": "70:12",
            "text": "وَ صَاحِبَتِهٖ وَاَخِيۡهِۙ‏"
        },
        {
            "id": 5388,
            "verse_key": "70:13",
            "text": "وَفَصِيۡلَتِهِ الَّتِىۡ تُــْٔوِيۡهِۙ‏"
        },
        {
            "id": 5389,
            "verse_key": "70:14",
            "text": "وَمَنۡ فِى الۡاَرۡضِ جَمِيۡعًا ۙ ثُمَّ يُنۡجِيۡهِۙ‏"
        },
        {
            "id": 5390,
            "verse_key": "70:15",
            "text": "كَلَّا ؕ اِنَّهَا لَظٰىۙ‏"
        },
        {
            "id": 5391,
            "verse_key": "70:16",
            "text": "نَزَّاعَةً لِّلشَّوٰى​ ۖ​ۚ‏"
        },
        {
            "id": 5392,
            "verse_key": "70:17",
            "text": "تَدۡعُوۡا مَنۡ اَدۡبَرَ وَتَوَلّٰىۙ‏"
        },
        {
            "id": 5393,
            "verse_key": "70:18",
            "text": "وَجَمَعَ فَاَوۡعٰى‏"
        },
        {
            "id": 5394,
            "verse_key": "70:19",
            "text": "اِنَّ الۡاِنۡسَانَ خُلِقَ هَلُوۡعًا ۙ‏"
        },
        {
            "id": 5395,
            "verse_key": "70:20",
            "text": "اِذَا مَسَّهُ الشَّرُّ جَزُوۡعًا ۙ‏"
        },
        {
            "id": 5396,
            "verse_key": "70:21",
            "text": "وَاِذَا مَسَّهُ الۡخَيۡرُ مَنُوۡعًا ۙ‏"
        },
        {
            "id": 5397,
            "verse_key": "70:22",
            "text": "اِلَّا الۡمُصَلِّيۡنَۙ‏"
        },
        {
            "id": 5398,
            "verse_key": "70:23",
            "text": "الَّذِيۡنَ هُمۡ عَلٰى صَلَاتِهِمۡ دَآٮِٕمُوۡنَۙ‏"
        },
        {
            "id": 5399,
            "verse_key": "70:24",
            "text": "وَالَّذِيۡنَ فِىۡۤ اَمۡوَالِهِمۡ حَقٌّ مَّعۡلُوۡمٌۙ‏"
        },
        {
            "id": 5400,
            "verse_key": "70:25",
            "text": "لِّلسَّآٮِٕلِ وَالۡمَحۡرُوۡمِۙ‏"
        },
        {
            "id": 5401,
            "verse_key": "70:26",
            "text": "وَالَّذِيۡنَ يُصَدِّقُوۡنَ بِيَوۡمِ الدِّيۡنِۙ‏"
        },
        {
            "id": 5402,
            "verse_key": "70:27",
            "text": "وَالَّذِيۡنَ هُمۡ مِّنۡ عَذَابِ رَبِّهِمۡ مُّشۡفِقُوۡنَ​ۚ‏"
        },
        {
            "id": 5403,
            "verse_key": "70:28",
            "text": "اِنَّ عَذَابَ رَبِّهِمۡ غَيۡرُ مَاۡمُوۡنٍ‏"
        },
        {
            "id": 5404,
            "verse_key": "70:29",
            "text": "وَالَّذِيۡنَ هُمۡ لِفُرُوۡجِهِمۡ حٰفِظُوۡنَۙ‏"
        },
        {
            "id": 5405,
            "verse_key": "70:30",
            "text": "اِلَّا عَلٰٓى اَزۡوَاجِهِمۡ اَوۡ مَا مَلَـكَتۡ اَيۡمَانُهُمۡ فَاِنَّهُمۡ غَيۡرُ مَلُوۡمِيۡنَ​ۚ‏"
        },
        {
            "id": 5406,
            "verse_key": "70:31",
            "text": "فَمَنِ ابۡتَغٰى وَرَآءَ ذٰلِكَ فَاُولٰٓٮِٕكَ هُمُ الۡعٰدُوۡنَ​ۚ‏"
        },
        {
            "id": 5407,
            "verse_key": "70:32",
            "text": "وَالَّذِيۡنَ هُمۡ لِاَمٰنٰتِهِمۡ وَعَهۡدِهِمۡ رٰعُوۡنَ ۙ‏"
        },
        {
            "id": 5408,
            "verse_key": "70:33",
            "text": "وَالَّذِيۡنَ هُمۡ بِشَهٰدٰتِهِمۡ قَآٮِٕمُوۡنَ ۙ‏"
        },
        {
            "id": 5409,
            "verse_key": "70:34",
            "text": "وَالَّذِيۡنَ هُمۡ عَلٰى صَلَاتِهِمۡ يُحَافِظُوۡنَؕ‏"
        },
        {
            "id": 5410,
            "verse_key": "70:35",
            "text": "اُولٰٓٮِٕكَ فِىۡ جَنّٰتٍ مُّكۡرَمُوۡنَؕ‏"
        },
        {
            "id": 5411,
            "verse_key": "70:36",
            "text": "فَمَالِ الَّذِيۡنَ كَفَرُوۡا قِبَلَكَ مُهۡطِعِيۡنَۙ‏"
        },
        {
            "id": 5412,
            "verse_key": "70:37",
            "text": "عَنِ الۡيَمِيۡنِ وَعَنِ الشِّمَالِ عِزِيۡنَ‏"
        },
        {
            "id": 5413,
            "verse_key": "70:38",
            "text": "اَيَطۡمَعُ كُلُّ امۡرِىءٍ مِّنۡهُمۡ اَنۡ يُّدۡخَلَ جَنَّةَ نَعِيۡمٍۙ‏ ﻿﻿"
        },
        {
            "id": 5414,
            "verse_key": "70:39",
            "text": "كَلَّا ؕ اِنَّا خَلَقۡنٰهُمۡ مِّمَّا يَعۡلَمُوۡنَ‏"
        },
        {
            "id": 5415,
            "verse_key": "70:40",
            "text": "فَلَاۤ اُقۡسِمُ بِرَبِّ الۡمَشٰرِقِ وَالۡمَغٰرِبِ اِنَّا لَقٰدِرُوۡنَۙ‏"
        },
        {
            "id": 5416,
            "verse_key": "70:41",
            "text": "عَلٰٓى اَنۡ نُّبَدِّلَ خَيۡرًا مِّنۡهُمۡۙ وَمَا نَحۡنُ بِمَسۡبُوۡقِيۡنَ‏"
        },
        {
            "id": 5417,
            "verse_key": "70:42",
            "text": "فَذَرۡهُمۡ يَخُوۡضُوۡا وَيَلۡعَبُوۡا حَتّٰى يُلٰقُوۡا يَوۡمَهُمُ الَّذِىۡ يُوۡعَدُوۡنَۙ‏"
        },
        {
            "id": 5418,
            "verse_key": "70:43",
            "text": "يَوۡمَ يَخۡرُجُوۡنَ مِنَ الۡاَجۡدَاثِ سِرَاعًا كَاَنَّهُمۡ اِلٰى نُصُبٍ يُّوۡفِضُوۡنَۙ‏ ﻿﻿"
        },
        {
            "id": 5419,
            "verse_key": "70:44",
            "text": "خَاشِعَةً اَبۡصَارُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٌ ؕ ذٰلِكَ الۡيَوۡمُ الَّذِىۡ كَانُوۡا يُوۡعَدُوۡنَ‏"
        },
        {
            "id": 5420,
            "verse_key": "71:1",
            "text": "اِنَّاۤ اَرۡسَلۡنَا نُوۡحًا اِلٰى قَوۡمِهٖۤ اَنۡ اَنۡذِرۡ قَوۡمَكَ مِنۡ قَبۡلِ اَنۡ يَّاۡتِيَهُمۡ عَذَابٌ اَلِيۡمٌ‏"
        },
        {
            "id": 5421,
            "verse_key": "71:2",
            "text": "قَالَ يٰقَوۡمِ اِنِّىۡ لَـكُمۡ نَذِيۡرٌ مُّبِيۡنٌۙ‏"
        },
        {
            "id": 5422,
            "verse_key": "71:3",
            "text": "اَنِ اعۡبُدُوا اللّٰهَ وَاتَّقُوۡهُ وَاَطِيۡعُوۡنِۙ‏"
        },
        {
            "id": 5423,
            "verse_key": "71:4",
            "text": "يَغۡفِرۡ لَـكُمۡ مِّنۡ ذُنُوۡبِكُمۡ وَيُؤَخِّرۡكُمۡ اِلٰٓى اَجَلٍ مُّسَمًّى​ؕ اِنَّ اَجَلَ اللّٰهِ اِذَا جَآءَ لَا يُؤَخَّرُ​​ۘ لَوۡ كُنۡتُمۡ تَعۡلَمُوۡنَ‏"
        },
        {
            "id": 5424,
            "verse_key": "71:5",
            "text": "قَالَ رَبِّ اِنِّىۡ دَعَوۡتُ قَوۡمِىۡ لَيۡلًا وَّنَهَارًا ۙ‏"
        },
        {
            "id": 5425,
            "verse_key": "71:6",
            "text": "فَلَمۡ يَزِدۡهُمۡ دُعَآءِىۡۤ اِلَّا فِرَارًا‏"
        },
        {
            "id": 5426,
            "verse_key": "71:7",
            "text": "وَاِنِّىۡ كُلَّمَا دَعَوۡتُهُمۡ لِتَغۡفِرَ لَهُمۡ جَعَلُوۡۤا اَصَابِعَهُمۡ فِىۡۤ اٰذَانِهِمۡ وَاسۡتَغۡشَوۡا ثِيَابَهُمۡ وَاَصَرُّوۡا وَاسۡتَكۡبَرُوا اسۡتِكۡبَارًا​ ۚ‏"
        },
        {
            "id": 5427,
            "verse_key": "71:8",
            "text": "ثُمَّ اِنِّىۡ دَعَوۡتُهُمۡ جِهَارًا ۙ‏"
        },
        {
            "id": 5428,
            "verse_key": "71:9",
            "text": "ثُمَّ اِنِّىۡۤ اَعۡلَـنۡتُ لَهُمۡ وَاَسۡرَرۡتُ لَهُمۡ اِسۡرَارًا ۙ‏"
        },
        {
            "id": 5429,
            "verse_key": "71:10",
            "text": "فَقُلۡتُ اسۡتَغۡفِرُوۡا رَبَّكُمۡؕ اِنَّهٗ كَانَ غَفَّارًا ۙ‏"
        },
        {
            "id": 5430,
            "verse_key": "71:11",
            "text": "يُّرۡسِلِ السَّمَآءَ عَلَيۡكُمۡ مِّدۡرَارًا ۙ‏"
        },
        {
            "id": 5431,
            "verse_key": "71:12",
            "text": "وَّيُمۡدِدۡكُمۡ بِاَمۡوَالٍ وَّبَنِيۡنَ وَيَجۡعَلۡ لَّـكُمۡ جَنّٰتٍ وَّيَجۡعَلۡ لَّـكُمۡ اَنۡهٰرًا ؕ‏"
        },
        {
            "id": 5432,
            "verse_key": "71:13",
            "text": "مَا لَـكُمۡ لَا تَرۡجُوۡنَ لِلّٰهِ وَقَارًا​ ۚ‏"
        },
        {
            "id": 5433,
            "verse_key": "71:14",
            "text": "وَقَدۡ خَلَقَكُمۡ اَطۡوَارًا‏"
        },
        {
            "id": 5434,
            "verse_key": "71:15",
            "text": "اَلَمۡ تَرَوۡا كَيۡفَ خَلَقَ اللّٰهُ سَبۡعَ سَمٰوٰتٍ طِبَاقًا ۙ‏"
        },
        {
            "id": 5435,
            "verse_key": "71:16",
            "text": "وَّجَعَلَ الۡقَمَرَ فِيۡهِنَّ نُوۡرًا ۙ وَّجَعَلَ الشَّمۡسَ سِرَاجًا‏"
        },
        {
            "id": 5436,
            "verse_key": "71:17",
            "text": "وَاللّٰهُ اَنۡۢبَتَكُمۡ مِّنَ الۡاَرۡضِ نَبَاتًا ۙ‏"
        },
        {
            "id": 5437,
            "verse_key": "71:18",
            "text": "ثُمَّ يُعِيۡدُكُمۡ فِيۡهَا وَيُخۡرِجُكُمۡ اِخۡرَاجًا‏"
        },
        {
            "id": 5438,
            "verse_key": "71:19",
            "text": "وَاللّٰهُ جَعَلَ لَـكُمُ الۡاَرۡضَ بِسَاطًا ۙ‏"
        },
        {
            "id": 5439,
            "verse_key": "71:20",
            "text": "لِّـتَسۡلُكُوۡا مِنۡهَا سُبُلًا فِجَاجًا‏"
        },
        {
            "id": 5440,
            "verse_key": "71:21",
            "text": "قَالَ نُوۡحٌ رَّبِّ اِنَّهُمۡ عَصَوۡنِىۡ وَاتَّبَعُوۡا مَنۡ لَّمۡ يَزِدۡهُ مَالُهٗ وَوَلَدُهٗۤ اِلَّا خَسَارًا​ ۚ‏"
        },
        {
            "id": 5441,
            "verse_key": "71:22",
            "text": "وَمَكَرُوۡا مَكۡرًا كُبَّارًا​ ۚ‏"
        },
        {
            "id": 5442,
            "verse_key": "71:23",
            "text": "وَ قَالُوۡا لَا تَذَرُنَّ اٰلِهَتَكُمۡ وَلَا تَذَرُنَّ وَدًّا وَّلَا سُوَاعًا  ۙ وَّ لَا يَغُوۡثَ وَيَعُوۡقَ وَنَسۡرًا​ ۚ‏"
        },
        {
            "id": 5443,
            "verse_key": "71:24",
            "text": "وَقَدۡ اَضَلُّوۡا كَثِيۡرًا​ ​ ۚ وَلَا تَزِدِ الظّٰلِمِيۡنَ اِلَّا ضَلٰلًا‏ ﻿﻿"
        },
        {
            "id": 5444,
            "verse_key": "71:25",
            "text": "مِّمَّا خَطِٓيْئٰتِهِمۡ اُغۡرِقُوۡا فَاُدۡخِلُوۡا نَارًا  ۙ فَلَمۡ يَجِدُوۡا لَهُمۡ مِّنۡ دُوۡنِ اللّٰهِ اَنۡصَارًا‏"
        },
        {
            "id": 5445,
            "verse_key": "71:26",
            "text": "وَ قَالَ نُوۡحٌ رَّبِّ لَا تَذَرۡ عَلَى الۡاَرۡضِ مِنَ الۡكٰفِرِيۡنَ دَيَّارًا‏ ﻿﻿"
        },
        {
            "id": 5446,
            "verse_key": "71:27",
            "text": "اِنَّكَ اِنۡ تَذَرۡهُمۡ يُضِلُّوۡا عِبَادَكَ وَلَا يَلِدُوۡۤا اِلَّا فَاجِرًا كَفَّارًا‏"
        },
        {
            "id": 5447,
            "verse_key": "71:28",
            "text": "رَبِّ اغۡفِرۡلِىۡ وَلِـوَالِدَىَّ وَلِمَنۡ دَخَلَ بَيۡتِىَ مُؤۡمِنًا وَّلِلۡمُؤۡمِنِيۡنَ وَالۡمُؤۡمِنٰتِؕ وَلَا تَزِدِ الظّٰلِمِيۡنَ اِلَّا تَبَارًا‏"
        },
        {
            "id": 5448,
            "verse_key": "72:1",
            "text": "قُلۡ اُوۡحِىَ اِلَىَّ اَنَّهُ اسۡتَمَعَ نَفَرٌ مِّنَ الۡجِنِّ فَقَالُوۡۤا اِنَّا سَمِعۡنَا قُرۡاٰنًاعَجَبًا ۙ‏"
        },
        {
            "id": 5449,
            "verse_key": "72:2",
            "text": "يَّهۡدِىۡۤ اِلَى الرُّشۡدِ فَاٰمَنَّا بِهٖ​ ؕ وَلَنۡ نُّشۡرِكَ بِرَبِّنَاۤ اَحَدًا ۙ‏"
        },
        {
            "id": 5450,
            "verse_key": "72:3",
            "text": "وَّاَنَّهٗ تَعٰلٰى جَدُّ رَبِّنَا مَا اتَّخَذَ صَاحِبَةً وَّلَا وَلَدًا ۙ‏ ﻿﻿"
        },
        {
            "id": 5451,
            "verse_key": "72:4",
            "text": "وَّ اَنَّهٗ كَانَ يَقُوۡلُ سَفِيۡهُنَا عَلَى اللّٰهِ شَطَطًا ۙ‏"
        },
        {
            "id": 5452,
            "verse_key": "72:5",
            "text": "وَّاَنَّا ظَنَنَّاۤ اَنۡ لَّنۡ تَقُوۡلَ الۡاِنۡسُ وَالۡجِنُّ عَلَى اللّٰهِ كَذِبًا ۙ‏"
        },
