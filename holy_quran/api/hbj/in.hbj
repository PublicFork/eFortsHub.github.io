{
    "verses": [
        {
            "id": 4847,
            "verse_key": "54:1",
            "text": "اِقۡتَـرَبَتِ السَّاعَةُ وَانْشَقَّ الۡقَمَرُ‏"
        },
        {
            "id": 4848,
            "verse_key": "54:2",
            "text": "وَاِنۡ يَّرَوۡا اٰيَةً يُّعۡرِضُوۡا وَيَقُوۡلُوۡا سِحۡرٌ مُّسۡتَمِرٌّ‏"
        },
        {
            "id": 4849,
            "verse_key": "54:3",
            "text": "وَكَذَّبُوۡا وَاتَّبَعُوۡۤا اَهۡوَآءَهُمۡ​ وَكُلُّ اَمۡرٍ مُّسۡتَقِرٌّ‏"
        },
        {
            "id": 4850,
            "verse_key": "54:4",
            "text": "وَلَقَدۡ جَآءَهُمۡ مِّنَ الۡاَنۡۢبَآءِ مَا فِيۡهِ مُزۡدَجَرٌۙ‏"
        },
        {
            "id": 4851,
            "verse_key": "54:5",
            "text": "حِكۡمَةٌ ۢ بَالِغَةٌ​ فَمَا تُغۡنِ النُّذُرُۙ‏"
        },
        {
            "id": 4852,
            "verse_key": "54:6",
            "text": "فَتَوَلَّ عَنۡهُمۡ​ۘ يَوۡمَ يَدۡعُ الدَّاعِ اِلٰى شَىۡءٍ نُّكُرٍۙ‏"
        },
        {
            "id": 4853,
            "verse_key": "54:7",
            "text": "خُشَّعًا اَبۡصَارُهُمۡ يَخۡرُجُوۡنَ مِنَ الۡاَجۡدَاثِ كَاَنَّهُمۡ جَرَادٌ مُّنۡتَشِرٌۙ‏"
        },
        {
            "id": 4854,
            "verse_key": "54:8",
            "text": "مُّهۡطِعِيۡنَ اِلَى الدَّاعِ​ؕ يَقُوۡلُ الۡكٰفِرُوۡنَ هٰذَا يَوۡمٌ عَسِرٌ‏ ﻿﻿"
        },
        {
            "id": 4855,
            "verse_key": "54:9",
            "text": "كَذَّبَتۡ قَبۡلَهُمۡ قَوۡمُ نُوۡحٍ فَكَذَّبُوۡا عَبۡدَنَا وَقَالُوۡا مَجۡنُوۡنٌ وَّازۡدُجِرَ‏"
        },
        {
            "id": 4856,
            "verse_key": "54:10",
            "text": "فَدَعَا رَبَّهٗۤ اَنِّىۡ مَغۡلُوۡبٌ فَانْـتَصِرۡ‏"
        },
        {
            "id": 4857,
            "verse_key": "54:11",
            "text": "فَفَتَحۡنَاۤ اَبۡوَابَ السَّمَآءِ بِمَآءٍ مُّنۡهَمِرٍ ۖ‏"
        },
        {
            "id": 4858,
            "verse_key": "54:12",
            "text": "وَّفَجَّرۡنَا الۡاَرۡضَ عُيُوۡنًا فَالۡتَقَى الۡمَآءُ عَلٰٓى اَمۡرٍ قَدۡ قُدِرَ​ۚ‏"
        },
        {
            "id": 4859,
            "verse_key": "54:13",
            "text": "وَحَمَلۡنٰهُ عَلٰى ذَاتِ اَلۡوَاحٍ وَّدُسُرٍۙ‏"
        },
        {
            "id": 4860,
            "verse_key": "54:14",
            "text": "تَجۡرِىۡ بِاَعۡيُنِنَا​ۚ جَزَآءً لِّمَنۡ كَانَ كُفِرَ‏"
        },
        {
            "id": 4861,
            "verse_key": "54:15",
            "text": "وَلَقَدْ تَّرَكۡنٰهَاۤ اٰيَةً فَهَلۡ مِنۡ مُّدَّكِرٍ‏"
        },
        {
            "id": 4862,
            "verse_key": "54:16",
            "text": "فَكَيۡفَ كَانَ عَذَابِىۡ وَنُذُرِ‏"
        },
        {
            "id": 4863,
            "verse_key": "54:17",
            "text": "وَلَقَدۡ يَسَّرۡنَا الۡقُرۡاٰنَ لِلذِّكۡرِ فَهَلۡ مِنۡ مُّدَّكِرٍ‏"
        },
        {
            "id": 4864,
            "verse_key": "54:18",
            "text": "كَذَّبَتۡ عَادٌ فَكَيۡفَ كَانَ عَذَابِىۡ وَنُذُرِ‏"
        },
        {
            "id": 4865,
            "verse_key": "54:19",
            "text": "اِنَّاۤ اَرۡسَلۡنَا عَلَيۡهِمۡ رِيۡحًا صَرۡصَرًا فِىۡ يَوۡمِ نَحۡسٍ مُّسۡتَمِرٍّۙ‏ ﻿﻿"
        },
        {
            "id": 4866,
            "verse_key": "54:20",
            "text": "تَنۡزِعُ النَّاسَۙ كَاَنَّهُمۡ اَعۡجَازُ نَخۡلٍ مُّنۡقَعِرٍ‏"
        },
        {
            "id": 4867,
            "verse_key": "54:21",
            "text": "فَكَيۡفَ كَانَ عَذَابِىۡ وَنُذُرِ‏"
        },
        {
            "id": 4868,
            "verse_key": "54:22",
            "text": "وَلَقَدۡ يَسَّرۡنَا الۡقُرۡاٰنَ لِلذِّكۡرِ فَهَلۡ مِنۡ مُّدَّكِرٍ‏"
        },
        {
            "id": 4869,
            "verse_key": "54:23",
            "text": "كَذَّبَتۡ ثَمُوۡدُ بِالنُّذُرِ‏"
        },
        {
            "id": 4870,
            "verse_key": "54:24",
            "text": "فَقَالُـوۡۤا اَبَشَرًا مِّنَّا وَاحِدًا نَّتَّبِعُهٗۤ ۙ اِنَّاۤ اِذًا لَّفِىۡ ضَلٰلٍ وَّسُعُرٍ‏"
        },
        {
            "id": 4871,
            "verse_key": "54:25",
            "text": "ءَاُلۡقِىَ الذِّكۡرُ عَلَيۡهِ مِنۡۢ بَيۡنِنَا بَلۡ هُوَ كَذَّابٌ اَشِرٌ‏"
        },
        {
            "id": 4872,
            "verse_key": "54:26",
            "text": "سَيَعۡلَمُوۡنَ غَدًا مَّنِ الۡكَذَّابُ الۡاَشِرُ‏"
        },
        {
            "id": 4873,
            "verse_key": "54:27",
            "text": "اِنَّا مُرۡسِلُوا النَّاقَةِ فِتۡنَةً لَّهُمۡ فَارۡتَقِبۡهُمۡ وَاصۡطَبِرۡ‏ ﻿﻿"
        },
        {
            "id": 4874,
            "verse_key": "54:28",
            "text": "وَنَبِّئۡهُمۡ اَنَّ الۡمَآءَ قِسۡمَةٌ ۢ بَيۡنَهُمۡ​ۚ كُلُّ شِرۡبٍ مُّحۡتَضَرٌ‏ ﻿﻿"
        },
        {
            "id": 4875,
            "verse_key": "54:29",
            "text": "فَنَادَوۡا صَاحِبَهُمۡ فَتَعَاطٰى فَعَقَرَ‏"
        },
        {
            "id": 4876,
            "verse_key": "54:30",
            "text": "فَكَيۡفَ كَانَ عَذَابِىۡ وَنُذُرِ‏"
        },
        {
            "id": 4877,
            "verse_key": "54:31",
            "text": "اِنَّاۤ اَرۡسَلۡنَا عَلَيۡهِمۡ صَيۡحَةً وَّاحِدَةً فَكَانُوۡا كَهَشِيۡمِ الۡمُحۡتَظِرِ‏ ﻿﻿"
        },
        {
            "id": 4878,
            "verse_key": "54:32",
            "text": "وَلَقَدۡ يَسَّرۡنَا الۡقُرۡاٰنَ لِلذِّكۡرِ فَهَلۡ مِنۡ مُّدَّكِرٍ‏"
        },
        {
            "id": 4879,
            "verse_key": "54:33",
            "text": "كَذَّبَتۡ قَوۡمُ لُوۡطٍ ۢ بِالنُّذُرِ‏"
        },
        {
            "id": 4880,
            "verse_key": "54:34",
            "text": "اِنَّاۤ اَرۡسَلۡنَا عَلَيۡهِمۡ حَاصِبًا اِلَّاۤ اٰلَ لُوۡطٍ​ؕ نَّجَّيۡنٰهُمۡ بِسَحَرٍۙ‏"
        },
        {
            "id": 4881,
            "verse_key": "54:35",
            "text": "نِّعۡمَةً مِّنۡ عِنۡدِنَا​ؕ كَذٰلِكَ نَجۡزِىۡ مَنۡ شَكَرَ‏"
        },
        {
            "id": 4882,
            "verse_key": "54:36",
            "text": "وَلَقَدۡ اَنۡذَرَهُمۡ بَطۡشَتَـنَا فَتَمَارَوۡا بِالنُّذُرِ‏"
        },
        {
            "id": 4883,
            "verse_key": "54:37",
            "text": "وَلَقَدۡ رَاوَدُوۡهُ عَنۡ ضَيۡفِهٖ فَطَمَسۡنَاۤ اَعۡيُنَهُمۡ فَذُوۡقُوۡا عَذَابِىۡ وَنُذُرِ‏"
        },
        {
            "id": 4884,
            "verse_key": "54:38",
            "text": "وَلَقَدۡ صَبَّحَهُمۡ بُكۡرَةً عَذَابٌ مُّسۡتَقِرٌّ​ ۚ‏"
        },
        {
            "id": 4885,
            "verse_key": "54:39",
            "text": "فَذُوۡقُوۡا عَذَابِىۡ وَنُذُرِ‏"
        },
        {
            "id": 4886,
            "verse_key": "54:40",
            "text": "وَلَقَدۡ يَسَّرۡنَا الۡقُرۡاٰنَ لِلذِّكۡرِ فَهَلۡ مِنۡ مُّدَّكِرٍ‏"
        },
        {
            "id": 4887,
            "verse_key": "54:41",
            "text": "وَلَقَدۡ جَآءَ اٰلَ فِرۡعَوۡنَ النُّذُرُ​ۚ‏"
        },
        {
            "id": 4888,
            "verse_key": "54:42",
            "text": "كَذَّبُوۡا بِاٰيٰتِنَا كُلِّهَا فَاَخَذۡنٰهُمۡ اَخۡذَ عَزِيۡزٍ مُّقۡتَدِرٍ‏ ﻿﻿"
        },
        {
            "id": 4889,
            "verse_key": "54:43",
            "text": "اَكُفَّارُكُمۡ خَيۡرٌ مِّنۡ اُولٰٓٮِٕكُمۡ اَمۡ لَكُمۡ بَرَآءَةٌ فِى الزُّبُرِ​ۚ‏ ﻿﻿"
        },
        {
            "id": 4890,
            "verse_key": "54:44",
            "text": "اَمۡ يَقُوۡلُوۡنَ نَحۡنُ جَمِيۡعٌ مُّنۡتَصِرٌ‏"
        },
        {
            "id": 4891,
            "verse_key": "54:45",
            "text": "سَيُهۡزَمُ الۡجَمۡعُ وَيُوَلُّوۡنَ الدُّبُرَ‏"
        },
        {
            "id": 4892,
            "verse_key": "54:46",
            "text": "بَلِ السَّاعَةُ مَوۡعِدُهُمۡ وَالسَّاعَةُ اَدۡهٰى وَاَمَرُّ‏"
        },
        {
            "id": 4893,
            "verse_key": "54:47",
            "text": "اِنَّ الۡمُجۡرِمِيۡنَ فِىۡ ضَلٰلٍ وَّسُعُرٍ​ۘ‏"
        },
        {
            "id": 4894,
            "verse_key": "54:48",
            "text": "يَوۡمَ يُسۡحَبُوۡنَ فِى النَّارِ عَلٰى وُجُوۡهِهِمۡؕ ذُوۡقُوۡا مَسَّ سَقَرَ‏ ﻿﻿"
        },
        {
            "id": 4895,
            "verse_key": "54:49",
            "text": "اِنَّا كُلَّ شَىۡءٍ خَلَقۡنٰهُ بِقَدَرٍ‏"
        },
        {
            "id": 4896,
            "verse_key": "54:50",
            "text": "وَمَاۤ اَمۡرُنَاۤ اِلَّا وَاحِدَةٌ كَلَمۡحٍۢ بِالۡبَصَرِ‏"
        },
        {
            "id": 4897,
            "verse_key": "54:51",
            "text": "وَلَقَدۡ اَهۡلَـكۡنَاۤ اَشۡيَاعَكُمۡ فَهَلۡ مِنۡ مُّدَّكِرٍ‏"
        },
        {
            "id": 4898,
            "verse_key": "54:52",
            "text": "وَكُلُّ شَىۡءٍ فَعَلُوۡهُ فِى الزُّبُرِ‏"
        },
        {
            "id": 4899,
            "verse_key": "54:53",
            "text": "وَ كُلُّ صَغِيۡرٍ وَّكَبِيۡرٍ مُّسۡتَطَرٌ‏"
        },
        {
            "id": 4900,
            "verse_key": "54:54",
            "text": "اِنَّ الۡمُتَّقِيۡنَ فِىۡ جَنّٰتٍ وَّنَهَرٍۙ‏"
        },
        {
            "id": 4901,
            "verse_key": "54:55",
            "text": "  فِىۡ مَقۡعَدِ صِدۡقٍ عِنۡدَ مَلِيۡكٍ مُّقۡتَدِرٍ‏"
        },
        {
            "id": 4902,
            "verse_key": "55:1",
            "text": "اَلرَّحۡمٰنُۙ‏"
        },
        {
            "id": 4903,
            "verse_key": "55:2",
            "text": "عَلَّمَ الۡقُرۡاٰنَؕ‏"
        },
        {
            "id": 4904,
            "verse_key": "55:3",
            "text": "خَلَقَ الۡاِنۡسَانَۙ‏"
        },
        {
            "id": 4905,
            "verse_key": "55:4",
            "text": "عَلَّمَهُ الۡبَيَانَ‏"
        },
        {
            "id": 4906,
            "verse_key": "55:5",
            "text": "اَلشَّمۡسُ وَالۡقَمَرُ بِحُسۡبَانٍ‏"
        },
        {
            "id": 4907,
            "verse_key": "55:6",
            "text": "وَّالنَّجۡمُ وَالشَّجَرُ يَسۡجُدٰنِ‏"
        },
        {
            "id": 4908,
            "verse_key": "55:7",
            "text": "وَالسَّمَآءَ رَفَعَهَا وَوَضَعَ الۡمِيۡزَانَۙ‏"
        },
        {
            "id": 4909,
            "verse_key": "55:8",
            "text": "اَلَّا تَطۡغَوۡا فِى الۡمِيۡزَانِ‏"
        },
        {
            "id": 4910,
            "verse_key": "55:9",
            "text": "وَاَقِيۡمُوا الۡوَزۡنَ بِالۡقِسۡطِ وَلَا تُخۡسِرُوا الۡمِيۡزَانَ‏"
        },
        {
            "id": 4911,
            "verse_key": "55:10",
            "text": "وَالۡاَرۡضَ وَضَعَهَا لِلۡاَنَامِۙ‏"
        },
        {
            "id": 4912,
            "verse_key": "55:11",
            "text": "فِيۡهَا فَاكِهَةٌ  ۙ وَّالنَّخۡلُ ذَاتُ الۡاَكۡمَامِ​ ۖ‏"
        },
        {
            "id": 4913,
            "verse_key": "55:12",
            "text": "وَالۡحَبُّ ذُو الۡعَصۡفِ وَالرَّيۡحَانُ​ۚ‏"
        },
        {
            "id": 4914,
            "verse_key": "55:13",
            "text": "فَبِاَىِّ اٰلَاۤءِ رَبِّكُمَا تُكَذِّبٰنِ‏"
        },
        {
            "id": 4915,
            "verse_key": "55:14",
            "text": "خَلَقَ الۡاِنۡسَانَ مِنۡ صَلۡصَالٍ كَالۡفَخَّارِۙ‏"
        },
        {
            "id": 4916,
            "verse_key": "55:15",
            "text": "وَخَلَقَ الۡجَآنَّ مِنۡ مَّارِجٍ مِّنۡ نَّارٍ​ۚ‏"
        },
        {
            "id": 4917,
            "verse_key": "55:16",
            "text": "فَبِاَىِّ اٰلَاۤءِ رَبِّكُمَا تُكَذِّبٰنِ‏"
        },
        {
            "id": 4918,
            "verse_key": "55:17",
            "text": "رَبُّ الۡمَشۡرِقَيۡنِ وَ رَبُّ الۡمَغۡرِبَيۡنِ​ۚ‏"
        },
        {
            "id": 4919,
            "verse_key": "55:18",
            "text": "فَبِاَىِّ اٰلَاۤءِ رَبِّكُمَا تُكَذِّبٰنِ‏"
        },
        {
            "id": 4920,
            "verse_key": "55:19",
            "text": "مَرَجَ الۡبَحۡرَيۡنِ يَلۡتَقِيٰنِۙ‏"
        },
        {
            "id": 4921,
            "verse_key": "55:20",
            "text": "بَيۡنَهُمَا بَرۡزَخٌ لَّا يَبۡغِيٰنِ​ۚ‏"
        },
        {
            "id": 4922,
            "verse_key": "55:21",
            "text": "فَبِاَىِّ اٰلَاۤءِ رَبِّكُمَا تُكَذِّبٰنِ‏"
        },
        {
            "id": 4923,
            "verse_key": "55:22",
            "text": "يَخۡرُجُ مِنۡهُمَا اللُّـؤۡلُـؤُ وَالۡمَرۡجَانُ​ۚ‏"
        },
        {
            "id": 4924,
            "verse_key": "55:23",
            "text": "فَبِاَىِّ اٰلَاۤءِ رَبِّكُمَا تُكَذِّبٰنِ‏"
        },
        {
            "id": 4925,
            "verse_key": "55:24",
            "text": "وَلَهُ الۡجَوَارِ الۡمُنۡشَئٰتُ فِى الۡبَحۡرِ كَالۡاَعۡلَامِ​ۚ‏"
        },
        {
            "id": 4926,
            "verse_key": "55:25",
