            "verse_key": "56:34",
            "text": "وَّ فُرُشٍ مَّرۡفُوۡعَةٍؕ‏"
        },
        {
            "id": 5014,
            "verse_key": "56:35",
            "text": "اِنَّاۤ اَنۡشَاۡنٰهُنَّ اِنۡشَآءًۙ‏"
        },
        {
            "id": 5015,
            "verse_key": "56:36",
            "text": "فَجَعَلۡنٰهُنَّ اَبۡكَارًاۙ‏"
        },
        {
            "id": 5016,
            "verse_key": "56:37",
            "text": "عُرُبًا اَتۡرَابًاۙ‏"
        },
        {
            "id": 5017,
            "verse_key": "56:38",
            "text": "لِّاَصۡحٰبِ الۡيَمِيۡنِؕ‏"
        },
        {
            "id": 5018,
            "verse_key": "56:39",
            "text": "ثُلَّةٌ مِّنَ الۡاَوَّلِيۡنَۙ‏"
        },
        {
            "id": 5019,
            "verse_key": "56:40",
            "text": "وَثُلَّةٌ مِّنَ الۡاٰخِرِيۡنَؕ‏"
        },
        {
            "id": 5020,
            "verse_key": "56:41",
            "text": "وَاَصۡحٰبُ الشِّمَالِ ۙ مَاۤ اَصۡحٰبُ الشِّمَالِؕ‏"
        },
        {
            "id": 5021,
            "verse_key": "56:42",
            "text": "فِىۡ سَمُوۡمٍ وَّحَمِيۡمٍۙ‏"
        },
        {
            "id": 5022,
            "verse_key": "56:43",
            "text": "وَّظِلٍّ مِّنۡ يَّحۡمُوۡمٍۙ‏"
        },
        {
            "id": 5023,
            "verse_key": "56:44",
            "text": "لَّا بَارِدٍ وَّلَا كَرِيۡمٍ‏"
        },
        {
            "id": 5024,
            "verse_key": "56:45",
            "text": "اِنَّهُمۡ كَانُوۡا قَبۡلَ ذٰ لِكَ مُتۡرَفِيۡنَۚ  ۖ‏"
        },
        {
            "id": 5025,
            "verse_key": "56:46",
            "text": "وَكَانُوۡا يُصِرُّوۡنَ عَلَى الۡحِنۡثِ الۡعَظِيۡمِ​ۚ‏"
        },
        {
            "id": 5026,
            "verse_key": "56:47",
            "text": "وَكَانُوۡا يَقُوۡلُوۡنَ ۙ اَٮِٕذَا مِتۡنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبۡعُوۡثُوۡنَۙ‏"
        },
        {
            "id": 5027,
            "verse_key": "56:48",
            "text": "اَوَاٰبَآؤُنَا الۡاَوَّلُوۡنَ‏"
        },
        {
            "id": 5028,
            "verse_key": "56:49",
            "text": "قُلۡ اِنَّ الۡاَوَّلِيۡنَ وَالۡاٰخِرِيۡنَۙ‏"
        },
        {
            "id": 5029,
            "verse_key": "56:50",
            "text": "لَمَجۡمُوۡعُوۡنَ ۙ اِلٰى مِيۡقَاتِ يَوۡمٍ مَّعۡلُوۡمٍ‏"
        },
        {
            "id": 5030,
            "verse_key": "56:51",
            "text": "ثُمَّ اِنَّكُمۡ اَيُّهَا الضَّآلُّوۡنَ الۡمُكَذِّبُوۡنَۙ‏"
        },
        {
            "id": 5031,
            "verse_key": "56:52",
            "text": "لَاٰكِلُوۡنَ مِنۡ شَجَرٍ مِّنۡ زَقُّوۡمٍۙ‏"
        },
        {
            "id": 5032,
            "verse_key": "56:53",
            "text": "فَمٰلِــُٔوۡنَ مِنۡهَا الۡبُطُوۡنَ​ۚ‏"
        },
        {
            "id": 5033,
            "verse_key": "56:54",
            "text": "فَشٰرِبُوۡنَ عَلَيۡهِ مِنَ الۡحَمِيۡمِ​ۚ‏"
        },
        {
            "id": 5034,
            "verse_key": "56:55",
            "text": "فَشٰرِبُوۡنَ شُرۡبَ الۡهِيۡمِؕ‏"
        },
        {
            "id": 5035,
            "verse_key": "56:56",
            "text": "هٰذَا نُزُلُهُمۡ يَوۡمَ الدِّيۡنِؕ‏"
        },
        {
            "id": 5036,
            "verse_key": "56:57",
            "text": "نَحۡنُ خَلَقۡنٰكُمۡ فَلَوۡلَا تُصَدِّقُوۡنَ‏"
        },
        {
            "id": 5037,
            "verse_key": "56:58",
            "text": "اَفَرَءَيۡتُمۡ مَّا تُمۡنُوۡنَؕ‏"
        },
        {
            "id": 5038,
            "verse_key": "56:59",
            "text": "ءَاَنۡتُمۡ تَخۡلُقُوۡنَهٗۤ اَمۡ نَحۡنُ الۡخٰلِقُوۡنَ‏"
        },
        {
            "id": 5039,
            "verse_key": "56:60",
            "text": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ الۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوۡقِيۡنَۙ‏"
        },
        {
            "id": 5040,
            "verse_key": "56:61",
            "text": "عَلٰٓى اَنۡ نُّبَدِّلَ اَمۡثَالَـكُمۡ وَنُـنۡشِئَكُمۡ فِىۡ مَا لَا تَعۡلَمُوۡنَ‏ ﻿﻿"
        },
        {
            "id": 5041,
            "verse_key": "56:62",
            "text": "وَلَـقَدۡ عَلِمۡتُمُ النَّشۡاَةَ الۡاُوۡلٰى فَلَوۡلَا تَذَكَّرُوۡنَ‏"
        },
        {
            "id": 5042,
            "verse_key": "56:63",
            "text": "اَفَرَءَيۡتُمۡ مَّا تَحۡرُثُوۡنَؕ‏"
        },
        {
            "id": 5043,
            "verse_key": "56:64",
            "text": "ءَاَنۡتُمۡ تَزۡرَعُوۡنَهٗۤ اَمۡ نَحۡنُ الزّٰرِعُوۡنَ‏"
        },
        {
            "id": 5044,
            "verse_key": "56:65",
            "text": "لَوۡ نَشَآءُ لَجَـعَلۡنٰهُ حُطَامًا فَظَلۡتُمۡ تَفَكَّهُوۡنَ‏"
        },
        {
            "id": 5045,
            "verse_key": "56:66",
            "text": "اِنَّا لَمُغۡرَمُوۡنَۙ‏"
        },
        {
            "id": 5046,
            "verse_key": "56:67",
            "text": "بَلۡ نَحۡنُ مَحۡرُوۡمُوۡنَ‏"
        },
        {
            "id": 5047,
            "verse_key": "56:68",
            "text": "اَفَرَءَيۡتُمُ الۡمَآءَ الَّذِىۡ تَشۡرَبُوۡنَؕ‏"
        },
        {
            "id": 5048,
            "verse_key": "56:69",
            "text": "ءَاَنۡـتُمۡ اَنۡزَلۡـتُمُوۡهُ مِنَ الۡمُزۡنِ اَمۡ نَحۡنُ الۡمُنۡزِلُوۡنَ‏ ﻿﻿"
        },
        {
            "id": 5049,
            "verse_key": "56:70",
            "text": "لَوۡ نَشَآءُ جَعَلۡنٰهُ اُجَاجًا فَلَوۡلَا تَشۡكُرُوۡنَ‏"
        },
        {
            "id": 5050,
            "verse_key": "56:71",
            "text": "اَفَرَءَيۡتُمُ النَّارَ الَّتِىۡ تُوۡرُوۡنَؕ‏"
        },
        {
            "id": 5051,
            "verse_key": "56:72",
            "text": "ءَاَنۡتُمۡ اَنۡشَاۡتُمۡ شَجَرَتَهَاۤ اَمۡ نَحۡنُ الۡمُنۡشِـُٔـوۡنَ‏"
        },
        {
            "id": 5052,
            "verse_key": "56:73",
            "text": "نَحۡنُ جَعَلۡنٰهَا تَذۡكِرَةً وَّمَتَاعًا لِّلۡمُقۡوِيۡنَ​ۚ‏"
        },
        {
            "id": 5053,
            "verse_key": "56:74",
            "text": "فَسَبِّحۡ بِاسۡمِ رَبِّكَ الۡعَظِيۡمِ‏"
        },
        {
            "id": 5054,
            "verse_key": "56:75",
            "text": "فَلَاۤ اُقۡسِمُ بِمَوٰقِعِ النُّجُوۡمِۙ‏‏"
        },
        {
            "id": 5055,
            "verse_key": "56:76",
            "text": "وَاِنَّهٗ لَقَسَمٌ لَّوۡ تَعۡلَمُوۡنَ عَظِيۡمٌۙ‏"
        },
        {
            "id": 5056,
            "verse_key": "56:77",
            "text": "اِنَّهٗ لَـقُرۡاٰنٌ كَرِيۡمٌۙ‏"
        },
        {
            "id": 5057,
            "verse_key": "56:78",
            "text": "فِىۡ كِتٰبٍ مَّكۡنُوۡنٍۙ‏"
        },
        {
            "id": 5058,
            "verse_key": "56:79",
            "text": "لَّا يَمَسُّهٗۤ اِلَّا الۡمُطَهَّرُوۡنَؕ‏"
        },
        {
            "id": 5059,
            "verse_key": "56:80",
            "text": "تَنۡزِيۡلٌ مِّنۡ رَّبِّ الۡعٰلَمِيۡنَ‏"
        },
        {
            "id": 5060,
            "verse_key": "56:81",
            "text": "اَفَبِهٰذَا الۡحَـدِيۡثِ اَنۡتُمۡ مُّدۡهِنُوۡنَۙ‏"
        },
        {
            "id": 5061,
            "verse_key": "56:82",
            "text": "وَتَجۡعَلُوۡنَ رِزۡقَكُمۡ اَنَّكُمۡ تُكَذِّبُوۡنَ‏"
        },
        {
            "id": 5062,
            "verse_key": "56:83",
            "text": "فَلَوۡلَاۤ اِذَا بَلَغَتِ الۡحُـلۡقُوۡمَۙ‏"
        },
        {
            "id": 5063,
            "verse_key": "56:84",
            "text": "وَاَنۡتُمۡ حِيۡنَٮِٕذٍ تَـنۡظُرُوۡنَۙ‏"
        },
        {
            "id": 5064,
            "verse_key": "56:85",
            "text": "وَنَحۡنُ اَقۡرَبُ اِلَيۡهِ مِنۡكُمۡ وَلٰـكِنۡ لَّا تُبۡصِرُوۡنَ‏"
        },
        {
            "id": 5065,
            "verse_key": "56:86",
            "text": "فَلَوۡلَاۤ اِنۡ كُنۡتُمۡ غَيۡرَ مَدِيۡنِيۡنَۙ‏"
        },
        {
            "id": 5066,
            "verse_key": "56:87",
            "text": "تَرۡجِعُوۡنَهَاۤ اِنۡ كُنۡتُمۡ صٰدِقِيۡنَ‏"
        },
        {
            "id": 5067,
            "verse_key": "56:88",
            "text": "فَاَمَّاۤ اِنۡ كَانَ مِنَ الۡمُقَرَّبِيۡنَۙ‏"
        },
        {
            "id": 5068,
            "verse_key": "56:89",
            "text": "فَرَوۡحٌ وَّ رَيۡحَانٌ ۙ وَّجَنَّتُ نَعِيۡمٍ‏"
        },
        {
            "id": 5069,
            "verse_key": "56:90",
            "text": "وَاَمَّاۤ اِنۡ كَانَ مِنۡ اَصۡحٰبِ الۡيَمِيۡنِۙ‏"
        },
        {
            "id": 5070,
            "verse_key": "56:91",
            "text": "فَسَلٰمٌ لَّكَ مِنۡ اَصۡحٰبِ الۡيَمِيۡنِؕ‏"
        },
        {
            "id": 5071,
            "verse_key": "56:92",
            "text": "وَاَمَّاۤ اِنۡ كَانَ مِنَ الۡمُكَذِّبِيۡنَ الضَّآلِّيۡنَۙ‏"
        },
        {
            "id": 5072,
            "verse_key": "56:93",
            "text": "فَنُزُلٌ مِّنۡ حَمِيۡمٍۙ‏"
        },
        {
            "id": 5073,
            "verse_key": "56:94",
            "text": "وَّتَصۡلِيَةُ جَحِيۡمٍ‏"
        },
        {
            "id": 5074,
            "verse_key": "56:95",
            "text": "اِنَّ هٰذَا لَهُوَ حَقُّ الۡيَـقِيۡنِۚ‏"
        },
        {
            "id": 5075,
            "verse_key": "56:96",
            "text": "فَسَبِّحۡ بِاسۡمِ رَبِّكَ الۡعَظِيۡمِ‏"
        },
        {
            "id": 5076,
            "verse_key": "57:1",
            "text": "سَبَّحَ لِلّٰهِ مَا فِى السَّمٰوٰتِ وَالۡاَرۡضِ​ۚ وَهُوَ الۡعَزِيۡزُ الۡحَكِيۡمُ‏ ﻿﻿"
        },
        {
            "id": 5077,
            "verse_key": "57:2",
            "text": "لَهٗ مُلۡكُ السَّمٰوٰتِ وَالۡاَرۡضِ​ۚ يُحۡىٖ وَيُمِيۡتُ​ۚ وَهُوَ عَلٰى كُلِّ شَىۡءٍ قَدِيۡرٌ‏"
        },
        {
            "id": 5078,
            "verse_key": "57:3",
            "text": "هُوَ الۡاَوَّلُ وَالۡاٰخِرُ وَالظَّاهِرُ وَالۡبَاطِنُ​ۚ وَهُوَ بِكُلِّ شَىۡءٍ عَلِيۡمٌ‏"
        },
        {
            "id": 5079,
            "verse_key": "57:4",
            "text": "هُوَ الَّذِىۡ خَلَقَ السَّمٰوٰتِ وَالۡاَرۡضَ فِىۡ سِتَّةِ اَيَّامٍ ثُمَّ اسۡتَوٰى عَلَى الۡعَرۡشِ​ؕ يَعۡلَمُ مَا يَلِجُ فِى الۡاَرۡضِ وَمَا يَخۡرُجُ مِنۡهَا وَمَا يَنۡزِلُ مِنَ السَّمَآءِ وَمَا يَعۡرُجُ فِيۡهَاؕ وَهُوَ مَعَكُمۡ اَيۡنَ مَا كُنۡتُمۡ​ؕ وَاللّٰهُ بِمَا تَعۡمَلُوۡنَ بَصِيۡرٌ‏"
        },
        {
            "id": 5080,
            "verse_key": "57:5",
            "text": "لَهٗ مُلۡكُ السَّمٰوٰتِ وَالۡاَرۡضِ​ؕ وَاِلَى اللّٰهِ تُرۡجَعُ الۡاُمُوۡرُ‏ ﻿﻿"
        },
        {
            "id": 5081,
            "verse_key": "57:6",
            "text": "يُوۡلِجُ الَّيۡلَ فِى النَّهَارِ وَيُوۡلِجُ النَّهَارَ فِى الَّيۡلِ​ؕ وَهُوَ عَلِيۡمٌۢ بِذَاتِ الصُّدُوۡرِ‏"
        },
        {
            "id": 5082,
            "verse_key": "57:7",
            "text": "اٰمِنُوۡا بِاللّٰهِ وَرَسُوۡلِهٖ وَاَنۡفِقُوۡا مِمَّا جَعَلَـكُمۡ مُّسۡتَخۡلَفِيۡنَ فِيۡهِ​ؕ فَالَّذِيۡنَ اٰمَنُوۡا مِنۡكُمۡ وَاَنۡفَقُوۡا لَهُمۡ اَجۡرٌ كَبِيۡرٌ‏"
        },
        {
            "id": 5083,
            "verse_key": "57:8",
            "text": "وَمَا لَـكُمۡ لَا تُؤۡمِنُوۡنَ بِاللّٰهِ​ۚ وَالرَّسُوۡلُ يَدۡعُوۡكُمۡ لِتُؤۡمِنُوۡا بِرَبِّكُمۡ وَقَدۡ اَخَذَ مِيۡثَاقَكُمۡ اِنۡ كُنۡتُمۡ مُّؤۡمِنِيۡنَ‏"
        },
        {
            "id": 5084,
            "verse_key": "57:9",
            "text": "هُوَ الَّذِىۡ يُنَزِّلُ عَلٰى عَبۡدِهٖۤ اٰيٰتٍۭ بَيِّنٰتٍ لِّيُخۡرِجَكُمۡ مِّنَ الظُّلُمٰتِ اِلَى النُّوۡرِ​ؕ وَاِنَّ اللّٰهَ بِكُمۡ لَرَءُوۡفٌ رَّحِيۡمٌ‏ ﻿﻿"
        },
        {
            "id": 5085,
            "verse_key": "57:10",
            "text": "وَ مَا لَـكُمۡ اَلَّا تُنۡفِقُوۡا فِىۡ سَبِيۡلِ اللّٰهِ وَلِلّٰهِ مِيۡـرَاثُ السَّمٰوٰتِ وَ الۡاَرۡضِ​ؕ لَا يَسۡتَوِىۡ مِنۡكُمۡ مَّنۡ اَنۡفَقَ مِنۡ قَبۡلِ الۡفَتۡحِ وَقَاتَلَ​ ؕ اُولٰٓٮِٕكَ اَعۡظَمُ دَرَجَةً مِّنَ الَّذِيۡنَ اَنۡفَقُوۡا مِنۡۢ بَعۡدُ وَقَاتَلُوۡا​ ؕ وَكُلًّا وَّعَدَ اللّٰهُ الۡحُسۡنٰى​ؕ وَاللّٰهُ بِمَا تَعۡمَلُوۡنَ خَبِيۡرٌ‏"
        },
        {
