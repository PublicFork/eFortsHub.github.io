        },
        {
            "id": 5521,
            "verse_key": "74:26",
            "text": "سَاُصۡلِيۡهِ سَقَرَ‏"
        },
        {
            "id": 5522,
            "verse_key": "74:27",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا سَقَرُؕ‏"
        },
        {
            "id": 5523,
            "verse_key": "74:28",
            "text": "لَا تُبۡقِىۡ وَ لَا تَذَرُ​ۚ‏"
        },
        {
            "id": 5524,
            "verse_key": "74:29",
            "text": "لَـوَّاحَةٌ لِّلۡبَشَرِ​ۖ​ۚ‏"
        },
        {
            "id": 5525,
            "verse_key": "74:30",
            "text": "عَلَيۡهَا تِسۡعَةَ عَشَرَؕ‏"
        },
        {
            "id": 5526,
            "verse_key": "74:31",
            "text": "وَمَا جَعَلۡنَاۤ اَصۡحٰبَ النَّارِ اِلَّا مَلٰٓٮِٕكَةً​ وَّمَا جَعَلۡنَا عِدَّتَهُمۡ اِلَّا فِتۡنَةً لِّلَّذِيۡنَ كَفَرُوۡا ۙ لِيَسۡتَيۡقِنَ الَّذِيۡنَ اُوۡتُوا الۡكِتٰبَ وَيَزۡدَادَ الَّذِيۡنَ اٰمَنُوۡۤا اِيۡمَانًا​ وَّلَا يَرۡتَابَ الَّذِيۡنَ اُوۡتُوا الۡكِتٰبَ وَالۡمُؤۡمِنُوۡنَ​ۙ وَلِيَقُوۡلَ الَّذِيۡنَ فِىۡ قُلُوۡبِهِمۡ مَّرَضٌ وَّالۡكٰفِرُوۡنَ مَاذَاۤ اَرَادَ اللّٰهُ بِهٰذَا مَثَلًا ​ؕ كَذٰلِكَ يُضِلُّ اللّٰهُ مَنۡ يَّشَآءُ وَيَهۡدِىۡ مَنۡ يَّشَآءُ ​ؕ وَمَا يَعۡلَمُ جُنُوۡدَ رَبِّكَ اِلَّا هُوَ ​ؕ وَمَا هِىَ اِلَّا ذِكۡرٰى لِلۡبَشَرِ‏"
        },
        {
            "id": 5527,
            "verse_key": "74:32",
            "text": "كَلَّا وَالۡقَمَرِۙ‏"
        },
        {
            "id": 5528,
            "verse_key": "74:33",
            "text": "وَالَّيۡلِ اِذۡ اَدۡبَرَۙ‏"
        },
        {
            "id": 5529,
            "verse_key": "74:34",
            "text": "وَالصُّبۡحِ اِذَاۤ اَسۡفَرَۙ‏"
        },
        {
            "id": 5530,
            "verse_key": "74:35",
            "text": "اِنَّهَا لَاِحۡدَى الۡكُبَرِۙ‏"
        },
        {
            "id": 5531,
            "verse_key": "74:36",
            "text": "نَذِيۡرًا لِّلۡبَشَرِۙ‏"
        },
        {
            "id": 5532,
            "verse_key": "74:37",
            "text": "لِمَنۡ شَآءَ مِنۡكُمۡ اَنۡ يَّتَقَدَّمَ اَوۡ يَتَاَخَّرَؕ‏"
        },
        {
            "id": 5533,
            "verse_key": "74:38",
            "text": "كُلُّ نَفۡسٍ ۢ بِمَا كَسَبَتۡ رَهِيۡنَةٌ ۙ‏"
        },
        {
            "id": 5534,
            "verse_key": "74:39",
            "text": "اِلَّاۤ اَصۡحٰبَ الۡيَمِيۡنِۛ ؕ‏"
        },
        {
            "id": 5535,
            "verse_key": "74:40",
            "text": "فِىۡ جَنّٰتٍ ۛ يَتَسَآءَلُوۡنَۙ‏"
        },
        {
            "id": 5536,
            "verse_key": "74:41",
            "text": "عَنِ الۡمُجۡرِمِيۡنَۙ‏"
        },
        {
            "id": 5537,
            "verse_key": "74:42",
            "text": "مَا سَلَـكَكُمۡ فِىۡ سَقَرَ‏"
        },
        {
            "id": 5538,
            "verse_key": "74:43",
            "text": "قَالُوۡا لَمۡ نَكُ مِنَ الۡمُصَلِّيۡنَۙ‏"
        },
        {
            "id": 5539,
            "verse_key": "74:44",
            "text": "وَلَمۡ نَكُ نُطۡعِمُ الۡمِسۡكِيۡنَۙ‏"
        },
        {
            "id": 5540,
            "verse_key": "74:45",
            "text": "وَكُنَّا نَخُوۡضُ مَعَ الۡخَـآٮِٕضِيۡنَۙ‏"
        },
        {
            "id": 5541,
            "verse_key": "74:46",
            "text": "وَ كُنَّا نُكَذِّبُ بِيَوۡمِ الدِّيۡنِۙ‏"
        },
        {
            "id": 5542,
            "verse_key": "74:47",
            "text": "حَتّٰٓى اَتٰٮنَا الۡيَقِيۡنُؕ‏"
        },
        {
            "id": 5543,
            "verse_key": "74:48",
            "text": "فَمَا تَنۡفَعُهُمۡ شَفَاعَةُ الشّٰفِعِيۡنَؕ‏"
        },
        {
            "id": 5544,
            "verse_key": "74:49",
            "text": "فَمَا لَهُمۡ عَنِ التَّذۡكِرَةِ مُعۡرِضِيۡنَۙ‏"
        },
        {
            "id": 5545,
            "verse_key": "74:50",
            "text": "كَاَنَّهُمۡ حُمُرٌ مُّسۡتَنۡفِرَةٌ ۙ‏"
        },
        {
            "id": 5546,
            "verse_key": "74:51",
            "text": "فَرَّتۡ مِنۡ قَسۡوَرَةٍ ؕ‏"
        },
        {
            "id": 5547,
            "verse_key": "74:52",
            "text": "بَلۡ يُرِيۡدُ كُلُّ امۡرِىٴٍ مِّنۡهُمۡ اَنۡ يُّؤۡتٰى صُحُفًا مُّنَشَّرَةً ۙ‏ ﻿﻿"
        },
        {
            "id": 5548,
            "verse_key": "74:53",
            "text": "كَلَّا ​ؕ بَلۡ لَّا يَخَافُوۡنَ الۡاٰخِرَةَ ؕ‏"
        },
        {
            "id": 5549,
            "verse_key": "74:54",
            "text": "كَلَّاۤ اِنَّهٗ تَذۡكِرَةٌ​ ۚ‏"
        },
        {
            "id": 5550,
            "verse_key": "74:55",
            "text": "فَمَنۡ شَآءَ ذَكَرَهٗ ؕ‏"
        },
        {
            "id": 5551,
            "verse_key": "74:56",
            "text": "وَمَا يَذۡكُرُوۡنَ اِلَّاۤ اَنۡ يَّشَآءَ اللّٰهُ​ ؕ هُوَ اَهۡلُ التَّقۡوٰى وَاَهۡلُ الۡمَغۡفِرَةِ‏"
        },
        {
            "id": 5552,
            "verse_key": "75:1",
            "text": "لَاۤ اُقۡسِمُ بِيَوۡمِ الۡقِيٰمَةِۙ‏"
        },
        {
            "id": 5553,
            "verse_key": "75:2",
            "text": "وَلَاۤ اُقۡسِمُ بِالنَّفۡسِ اللَّوَّامَةِؕ‏"
        },
        {
            "id": 5554,
            "verse_key": "75:3",
            "text": "اَيَحۡسَبُ الۡاِنۡسَانُ اَلَّنۡ نَّجۡمَعَ عِظَامَهٗؕ‏"
        },
        {
            "id": 5555,
            "verse_key": "75:4",
            "text": "بَلٰى قٰدِرِيۡنَ عَلٰٓى اَنۡ نُّسَوِّىَ بَنَانَهٗ‏"
        },
        {
            "id": 5556,
            "verse_key": "75:5",
            "text": "بَلۡ يُرِيۡدُ الۡاِنۡسَانُ لِيَفۡجُرَ اَمَامَهٗ​ۚ‏"
        },
        {
            "id": 5557,
            "verse_key": "75:6",
            "text": "يَسۡـَٔـلُ اَيَّانَ يَوۡمُ الۡقِيٰمَةِؕ‏"
        },
        {
            "id": 5558,
            "verse_key": "75:7",
            "text": "فَاِذَا بَرِقَ الۡبَصَرُۙ‏"
        },
        {
            "id": 5559,
            "verse_key": "75:8",
            "text": "وَخَسَفَ الۡقَمَرُۙ‏"
        },
        {
            "id": 5560,
            "verse_key": "75:9",
            "text": "وَجُمِعَ الشَّمۡسُ وَالۡقَمَرُۙ‏"
        },
        {
            "id": 5561,
            "verse_key": "75:10",
            "text": "يَقُوۡلُ الۡاِنۡسَانُ يَوۡمَٮِٕذٍ اَيۡنَ الۡمَفَرُّ​ ۚ‏"
        },
        {
            "id": 5562,
            "verse_key": "75:11",
            "text": "كَلَّا لَا وَزَرَؕ‏"
        },
        {
            "id": 5563,
            "verse_key": "75:12",
            "text": "اِلٰى رَبِّكَ يَوۡمَٮِٕذِ اۨلۡمُسۡتَقَرُّ ؕ‏"
        },
        {
            "id": 5564,
            "verse_key": "75:13",
            "text": "يُنَبَّؤُا الۡاِنۡسَانُ يَوۡمَٮِٕذٍۢ بِمَا قَدَّمَ وَاَخَّرَؕ‏"
        },
        {
            "id": 5565,
            "verse_key": "75:14",
            "text": "بَلِ الۡاِنۡسَانُ عَلٰى نَفۡسِهٖ بَصِيۡرَةٌ ۙ‏"
        },
        {
            "id": 5566,
            "verse_key": "75:15",
            "text": "وَّلَوۡ اَلۡقٰى مَعَاذِيۡرَهٗؕ‏"
        },
        {
            "id": 5567,
            "verse_key": "75:16",
            "text": "لَا تُحَرِّكۡ بِهٖ لِسَانَكَ لِتَعۡجَلَ بِهٖؕ‏"
        },
        {
            "id": 5568,
            "verse_key": "75:17",
            "text": "اِنَّ عَلَيۡنَا جَمۡعَهٗ وَقُرۡاٰنَهٗۚ  ۖ‏"
        },
        {
            "id": 5569,
            "verse_key": "75:18",
            "text": "فَاِذَا قَرَاۡنٰهُ فَاتَّبِعۡ قُرۡاٰنَهٗ​ۚ‏"
        },
        {
            "id": 5570,
            "verse_key": "75:19",
            "text": "ثُمَّ اِنَّ عَلَيۡنَا بَيَانَهٗؕ‏"
        },
        {
            "id": 5571,
            "verse_key": "75:20",
            "text": "كَلَّا بَلۡ تُحِبُّوۡنَ الۡعَاجِلَةَ ۙ‏"
        },
        {
            "id": 5572,
            "verse_key": "75:21",
            "text": "وَتَذَرُوۡنَ الۡاٰخِرَةَ ؕ‏"
        },
        {
            "id": 5573,
            "verse_key": "75:22",
            "text": "وُجُوۡهٌ يَّوۡمَٮِٕذٍ نَّاضِرَةٌ ۙ‏"
        },
        {
            "id": 5574,
            "verse_key": "75:23",
            "text": "اِلٰى رَبِّهَا نَاظِرَةٌ​ ۚ‏"
        },
        {
            "id": 5575,
            "verse_key": "75:24",
            "text": "وَوُجُوۡهٌ يَّوۡمَٮِٕذٍۢ بَاسِرَةٌ ۙ‏"
        },
        {
            "id": 5576,
            "verse_key": "75:25",
            "text": "تَظُنُّ اَنۡ يُّفۡعَلَ بِهَا فَاقِرَةٌ ؕ‏"
        },
        {
            "id": 5577,
            "verse_key": "75:26",
            "text": "كَلَّاۤ اِذَا بَلَغَتِ التَّرَاقِىَۙ‏"
        },
        {
            "id": 5578,
            "verse_key": "75:27",
            "text": "وَقِيۡلَ مَنۡ رَاقٍۙ‏"
        },
        {
            "id": 5579,
            "verse_key": "75:28",
            "text": "وَّظَنَّ اَنَّهُ الۡفِرَاقُۙ‏"
        },
        {
            "id": 5580,
            "verse_key": "75:29",
            "text": "وَالۡتَفَّتِ السَّاقُ بِالسَّاقِۙ‏"
        },
        {
            "id": 5581,
            "verse_key": "75:30",
            "text": "اِلٰى رَبِّكَ يَوۡمَٮِٕذِ اۨلۡمَسَاقُؕ‏"
        },
        {
            "id": 5582,
            "verse_key": "75:31",
            "text": "فَلَا صَدَّقَ وَلَا صَلّٰىۙ‏"
        },
        {
            "id": 5583,
            "verse_key": "75:32",
            "text": "وَلٰڪِنۡ كَذَّبَ وَتَوَلّٰىۙ‏"
        },
        {
            "id": 5584,
            "verse_key": "75:33",
            "text": "ثُمَّ ذَهَبَ اِلٰٓى اَهۡلِهٖ يَتَمَطّٰىؕ‏"
        },
        {
            "id": 5585,
            "verse_key": "75:34",
            "text": "اَوۡلٰى لَكَ فَاَوۡلٰىۙ‏"
        },
        {
            "id": 5586,
            "verse_key": "75:35",
            "text": "ثُمَّ اَوۡلٰى لَكَ فَاَوۡلٰىؕ‏"
        },
        {
            "id": 5587,
            "verse_key": "75:36",
            "text": "اَيَحۡسَبُ الۡاِنۡسَانُ اَنۡ يُّتۡرَكَ سُدًىؕ‏"
        },
        {
            "id": 5588,
            "verse_key": "75:37",
            "text": "اَلَمۡ يَكُ نُطۡفَةً مِّنۡ مَّنِىٍّ يُّمۡنٰىۙ‏"
        },
        {
            "id": 5589,
            "verse_key": "75:38",
            "text": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوّٰىۙ‏"
        },
        {
            "id": 5590,
            "verse_key": "75:39",
            "text": "فَجَعَلَ مِنۡهُ الزَّوۡجَيۡنِ الذَّكَرَ وَالۡاُنۡثٰىؕ‏"
        },
        {
            "id": 5591,
            "verse_key": "75:40",
            "text": "اَلَيۡسَ ذٰلِكَ بِقٰدِرٍ عَلٰٓى اَنۡ يُّحۡـىِۦَ الۡمَوۡتٰى‏"
        },
        {
            "id": 5592,
            "verse_key": "76:1",
            "text": "هَلۡ اَتٰى عَلَى الۡاِنۡسَانِ حِيۡنٌ مِّنَ الدَّهۡرِ لَمۡ يَكُنۡ شَيۡـًٔـا مَّذۡكُوۡرًا‏"
        },
        {
            "id": 5593,
            "verse_key": "76:2",
            "text": "اِنَّا خَلَقۡنَا الۡاِنۡسَانَ مِنۡ نُّطۡفَةٍ اَمۡشَاجٍۖ نَّبۡتَلِيۡهِ فَجَعَلۡنٰهُ سَمِيۡعًۢا بَصِيۡرًا‏"
        },
        {
            "id": 5594,
            "verse_key": "76:3",
            "text": "اِنَّا هَدَيۡنٰهُ السَّبِيۡلَ اِمَّا شَاكِرًا وَّاِمَّا كَفُوۡرًا‏"
        },
        {
            "id": 5595,
            "verse_key": "76:4",
            "text": "اِنَّاۤ اَعۡتَدۡنَا لِلۡكٰفِرِيۡنَ سَلٰسِلَا۟ وَاَغۡلٰلًا وَّسَعِيۡرًا‏"
        },
        {
            "id": 5596,
            "verse_key": "76:5",
            "text": "اِنَّ الۡاَبۡرَارَ يَشۡرَبُوۡنَ مِنۡ كَاۡسٍ كَانَ مِزَاجُهَا كَافُوۡرًا​ۚ‏ ﻿﻿"
        },
        {
            "id": 5597,
            "verse_key": "76:6",
            "text": "عَيۡنًا يَّشۡرَبُ بِهَا عِبَادُ اللّٰهِ يُفَجِّرُوۡنَهَا تَفۡجِيۡرًا‏"
        },
        {
            "id": 5598,
            "verse_key": "76:7",
            "text": "يُوۡفُوۡنَ بِالنَّذۡرِ وَيَخَافُوۡنَ يَوۡمًا كَانَ شَرُّهٗ مُسۡتَطِيۡرًا‏ ﻿﻿"
        },
        {
            "id": 5599,
            "verse_key": "76:8",
            "text": "وَيُطۡعِمُوۡنَ الطَّعَامَ عَلٰى حُبِّهٖ مِسۡكِيۡنًا وَّيَتِيۡمًا وَّاَسِيۡرًا‏ ﻿﻿"
        },
        {
            "id": 5600,
            "verse_key": "76:9",
            "text": "اِنَّمَا نُطۡعِمُكُمۡ لِـوَجۡهِ اللّٰهِ لَا نُرِيۡدُ مِنۡكُمۡ جَزَآءً وَّلَا شُكُوۡرًا‏"
        },
        {
            "id": 5601,
            "verse_key": "76:10",
            "text": "اِنَّا نَخَافُ مِنۡ رَّبِّنَا يَوۡمًا عَبُوۡسًا قَمۡطَرِيۡرًا‏"
        },
        {
            "id": 5602,
            "verse_key": "76:11",
            "text": "فَوَقٰٮهُمُ اللّٰهُ شَرَّ ذٰلِكَ الۡيَوۡمِ وَ لَقّٰٮهُمۡ نَضۡرَةً وَّسُرُوۡرًا​ۚ‏ ﻿﻿"
        },
        {
            "id": 5603,
            "verse_key": "76:12",
            "text": "وَجَزٰٮهُمۡ بِمَا صَبَرُوۡا جَنَّةً وَّحَرِيۡرًا ۙ‏"
        },
        {
            "id": 5604,
            "verse_key": "76:13",
            "text": "مُّتَّكِـِٕـيۡنَ فِيۡهَا عَلَى الۡاَرَآٮِٕكِ​ۚ لَا يَرَوۡنَ فِيۡهَا شَمۡسًا وَّلَا زَمۡهَرِيۡرًا​ۚ‏"
        },
        {
            "id": 5605,
            "verse_key": "76:14",
            "text": "وَدَانِيَةً عَلَيۡهِمۡ ظِلٰلُهَا وَذُلِّلَتۡ قُطُوۡفُهَا تَذۡلِيۡلًا‏"
        },
        {
            "id": 5606,
            "verse_key": "76:15",
            "text": "وَيُطَافُ عَلَيۡهِمۡ بِاٰنِيَةٍ مِّنۡ فِضَّةٍ وَّاَكۡوَابٍ كَانَتۡ قَوَارِيۡرَا۟ ۙ‏ ﻿﻿"
        },
        {
            "id": 5607,
            "verse_key": "76:16",
            "text": "قَوَارِيۡرَا۟ مِنۡ فِضَّةٍ قَدَّرُوۡهَا تَقۡدِيۡرًا‏"
        },
        {
            "id": 5608,
            "verse_key": "76:17",
            "text": "​ ​ وَيُسۡقَوۡنَ فِيۡهَا كَاۡسًا كَانَ مِزَاجُهَا زَنۡجَبِيۡلًا ۚ‏"
        },
        {
            "id": 5609,
            "verse_key": "76:18",
            "text": "عَيۡنًا فِيۡهَا تُسَمّٰى سَلۡسَبِيۡلًا‏"
        },
        {
            "id": 5610,
            "verse_key": "76:19",
            "text": "وَيَطُوۡفُ عَلَيۡهِمۡ وِلۡدَانٌ مُّخَلَّدُوۡنَ​ۚ اِذَا رَاَيۡتَهُمۡ حَسِبۡتَهُمۡ لُـؤۡلُـؤًا مَّنۡثُوۡرًا‏"
        },
        {
            "id": 5611,
            "verse_key": "76:20",
            "text": "وَاِذَا رَاَيۡتَ ثَمَّ رَاَيۡتَ نَعِيۡمًا وَّمُلۡكًا كَبِيۡرًا‏"
        },
        {
            "id": 5612,
            "verse_key": "76:21",
            "text": "عٰلِيَهُمۡ ثِيَابُ سُنۡدُسٍ خُضۡرٌ وَّاِسۡتَبۡرَقٌ​ وَّحُلُّوۡۤا اَسَاوِرَ مِنۡ فِضَّةٍ ​ۚوَسَقٰٮهُمۡ رَبُّهُمۡ شَرَابًا طَهُوۡرًا‏"
        },
        {
            "id": 5613,
            "verse_key": "76:22",
            "text": "اِنَّ هٰذَا كَانَ لَـكُمۡ جَزَآءً وَّكَانَ سَعۡيُكُمۡ مَّشۡكُوۡرًا‏"
        },
        {
            "id": 5614,
            "verse_key": "76:23",
            "text": "اِنَّا نَحۡنُ نَزَّلۡنَا عَلَيۡكَ الۡقُرۡاٰنَ تَنۡزِيۡلًا ۚ‏"
        },
        {
            "id": 5615,
            "verse_key": "76:24",
            "text": "فَاصۡبِرۡ لِحُكۡمِ رَبِّكَ وَلَا تُطِعۡ مِنۡهُمۡ اٰثِمًا اَوۡ كَفُوۡرًا​ۚ‏ ﻿﻿"
        },
        {
            "id": 5616,
            "verse_key": "76:25",
            "text": "وَاذۡكُرِ اسۡمَ رَبِّكَ بُكۡرَةً وَّاَصِيۡلًا ۚ  ۖ‏"
        },
        {
            "id": 5617,
            "verse_key": "76:26",
            "text": "وَمِنَ الَّيۡلِ فَاسۡجُدۡ لَهٗ وَسَبِّحۡهُ لَيۡلًا طَوِيۡلًا‏"
        },
        {
            "id": 5618,
            "verse_key": "76:27",
            "text": "اِنَّ هٰٓؤُلَاۤءِ يُحِبُّوۡنَ الۡعَاجِلَةَ وَيَذَرُوۡنَ وَرَآءَهُمۡ يَوۡمًا ثَقِيۡلًا‏"
        },
        {
            "id": 5619,
            "verse_key": "76:28",
            "text": "نَحۡنُ خَلَقۡنٰهُمۡ وَشَدَدۡنَاۤ اَسۡرَهُمۡ​ۚ وَاِذَا شِئۡنَا بَدَّلۡنَاۤ اَمۡثَالَهُمۡ تَبۡدِيۡلًا‏"
        },
        {
            "id": 5620,
            "verse_key": "76:29",
            "text": "اِنَّ هٰذِهٖ تَذۡكِرَةٌ ​ۚ فَمَنۡ شَآءَ اتَّخَذَ اِلٰى رَبِّهٖ سَبِيۡلًا‏ ﻿﻿"
        },
        {
            "id": 5621,
            "verse_key": "76:30",
            "text": "وَمَا تَشَآءُوۡنَ اِلَّاۤ اَنۡ يَّشَآءَ اللّٰهُ ​ؕ اِنَّ اللّٰهَ كَانَ عَلِيۡمًا حَكِيۡمًا  ۖ‏"
        },
        {
            "id": 5622,
            "verse_key": "76:31",
            "text": "يُّدۡخِلُ مَنۡ يَّشَآءُ فِىۡ رَحۡمَتِهٖ​ؕ وَالظّٰلِمِيۡنَ اَعَدَّ لَهُمۡ عَذَابًا اَلِيۡمًا‏"
        },
        {
            "id": 5623,
            "verse_key": "77:1",
            "text": "وَالۡمُرۡسَلٰتِ عُرۡفًا ۙ‏"
        },
        {
            "id": 5624,
            "verse_key": "77:2",
            "text": "فَالۡعٰصِفٰتِ عَصۡفًا ۙ‏"
        },
        {
            "id": 5625,
            "verse_key": "77:3",
            "text": "وَّالنّٰشِرٰتِ نَشۡرًا ۙ‏"
        },
        {
            "id": 5626,
            "verse_key": "77:4",
            "text": "فَالۡفٰرِقٰتِ فَرۡقًا ۙ‏"
        },
        {
            "id": 5627,
            "verse_key": "77:5",
            "text": "فَالۡمُلۡقِيٰتِ ذِكۡرًا ۙ‏"
        },
        {
            "id": 5628,
            "verse_key": "77:6",
            "text": "عُذۡرًا اَوۡ نُذۡرًا ۙ‏"
        },
        {
            "id": 5629,
            "verse_key": "77:7",
            "text": "اِنَّمَا تُوۡعَدُوۡنَ لَوَاقِعٌ ؕ‏"
        },
        {
            "id": 5630,
            "verse_key": "77:8",
            "text": "فَاِذَا النُّجُوۡمُ طُمِسَتۡۙ‏"
        },
        {
            "id": 5631,
            "verse_key": "77:9",
            "text": "وَ اِذَا السَّمَآءُ فُرِجَتۡۙ‏"
        },
        {
            "id": 5632,
            "verse_key": "77:10",
            "text": "وَاِذَا الۡجِبَالُ نُسِفَتۡۙ‏"
        },
        {
            "id": 5633,
            "verse_key": "77:11",
            "text": "وَاِذَا الرُّسُلُ اُقِّتَتۡؕ‏"
        },
        {
            "id": 5634,
            "verse_key": "77:12",
            "text": "لِاَىِّ يَوۡمٍ اُجِّلَتۡؕ‏"
        },
        {
            "id": 5635,
            "verse_key": "77:13",
            "text": "لِيَوۡمِ الۡفَصۡلِ​ۚ‏"
        },
        {
            "id": 5636,
            "verse_key": "77:14",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا يَوۡمُ الۡفَصۡلِؕ‏"
        },
        {
            "id": 5637,
            "verse_key": "77:15",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5638,
            "verse_key": "77:16",
            "text": "اَلَمۡ نُهۡلِكِ الۡاَوَّلِيۡنَؕ‏"
        },
        {
            "id": 5639,
            "verse_key": "77:17",
            "text": "ثُمَّ نُتۡبِعُهُمُ الۡاٰخِرِيۡنَ‏"
        },
        {
            "id": 5640,
            "verse_key": "77:18",
            "text": "كَذٰلِكَ نَفۡعَلُ بِالۡمُجۡرِمِيۡنَ‏"
        },
        {
            "id": 5641,
            "verse_key": "77:19",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5642,
            "verse_key": "77:20",
            "text": "اَلَمۡ نَخۡلُقۡكُّمۡ مِّنۡ مَّآءٍ مَّهِيۡنٍۙ‏"
        },
        {
            "id": 5643,
            "verse_key": "77:21",
            "text": "فَجَعَلۡنٰهُ فِىۡ قَرَارٍ مَّكِيۡنٍۙ‏"
        },
        {
            "id": 5644,
            "verse_key": "77:22",
            "text": "اِلٰى قَدَرٍ مَّعۡلُوۡمٍۙ‏"
        },
        {
            "id": 5645,
            "verse_key": "77:23",
            "text": "فَقَدَرۡنَا ۖ فَنِعۡمَ الۡقٰدِرُوۡنَ‏"
        },
        {
            "id": 5646,
            "verse_key": "77:24",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5647,
            "verse_key": "77:25",
            "text": "اَلَمۡ نَجۡعَلِ الۡاَرۡضَ كِفَاتًا ۙ‏"
        },
        {
            "id": 5648,
            "verse_key": "77:26",
            "text": "اَحۡيَآءً وَّاَمۡوَاتًا ۙ‏"
        },
        {
            "id": 5649,
            "verse_key": "77:27",
            "text": "وَّجَعَلۡنَا فِيۡهَا رَوَاسِىَ شٰمِخٰتٍ وَّ اَسۡقَيۡنٰكُمۡ مَّآءً فُرَاتًا ؕ‏"
        },
        {
            "id": 5650,
            "verse_key": "77:28",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5651,
            "verse_key": "77:29",
            "text": "اِنْطَلِقُوۡۤا اِلٰى مَا كُنۡتُمۡ بِهٖ تُكَذِّبُوۡنَ​ۚ‏"
        },
        {
            "id": 5652,
            "verse_key": "77:30",
            "text": "اِنْطَلِقُوۡۤا اِلٰى ظِلٍّ ذِىۡ ثَلٰثِ شُعَبٍۙ‏"
        },
        {
            "id": 5653,
            "verse_key": "77:31",
            "text": "لَّا ظَلِيۡلٍ وَّلَا يُغۡنِىۡ مِنَ اللَّهَبِؕ‏"
        },
        {
            "id": 5654,
            "verse_key": "77:32",
            "text": "اِنَّهَا تَرۡمِىۡ بِشَرَرٍ كَالۡقَصۡرِ​ۚ‏"
        },
        {
            "id": 5655,
            "verse_key": "77:33",
            "text": "كَاَنَّهٗ جِمٰلَتٌ صُفۡرٌ ؕ‏"
        },
        {
            "id": 5656,
            "verse_key": "77:34",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5657,
            "verse_key": "77:35",
            "text": "هٰذَا يَوۡمُ لَا يَنۡطِقُوۡنَۙ‏"
        },
        {
            "id": 5658,
            "verse_key": "77:36",
            "text": "وَلَا يُؤۡذَنُ لَهُمۡ فَيَـعۡتَذِرُوۡنَ‏"
        },
        {
            "id": 5659,
            "verse_key": "77:37",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5660,
            "verse_key": "77:38",
            "text": "هٰذَا يَوۡمُ الۡفَصۡلِ​ۚ جَمَعۡنٰكُمۡ وَالۡاَوَّلِيۡنَ‏"
        },
        {
            "id": 5661,
            "verse_key": "77:39",
            "text": "فَاِنۡ كَانَ لَـكُمۡ كَيۡدٌ فَكِيۡدُوۡنِ‏"
        },
        {
            "id": 5662,
            "verse_key": "77:40",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5663,
            "verse_key": "77:41",
            "text": "اِنَّ الۡمُتَّقِيۡنَ فِىۡ ظِلٰلٍ وَّعُيُوۡنٍۙ‏"
        },
        {
            "id": 5664,
            "verse_key": "77:42",
            "text": "وَّفَوَاكِهَ مِمَّا يَشۡتَهُوۡنَؕ‏"
        },
        {
            "id": 5665,
            "verse_key": "77:43",
            "text": "كُلُوۡا وَاشۡرَبُوۡا هَنِيْٓئًا ۢ بِمَا كُنۡتُمۡ تَعۡمَلُوۡنَ‏"
        },
        {
            "id": 5666,
            "verse_key": "77:44",
            "text": "اِنَّا كَذٰلِكَ نَجۡزِى الۡمُحۡسِنِيۡنَ‏"
        },
        {
            "id": 5667,
            "verse_key": "77:45",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5668,
            "verse_key": "77:46",
            "text": "كُلُوۡا وَتَمَتَّعُوۡا قَلِيۡلًا اِنَّكُمۡ مُّجۡرِمُوۡنَ‏"
        },
        {
            "id": 5669,
            "verse_key": "77:47",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5670,
            "verse_key": "77:48",
            "text": "وَاِذَا قِيۡلَ لَهُمُ ارۡكَعُوۡا لَا يَرۡكَعُوۡنَ‏"
        },
        {
            "id": 5671,
            "verse_key": "77:49",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَ‏"
        },
        {
            "id": 5672,
            "verse_key": "77:50",
            "text": "فَبِاَىِّ حَدِيۡثٍۢ بَعۡدَهٗ يُؤۡمِنُوۡنَ‏"
        },
        {
            "id": 5673,
            "verse_key": "78:1",
            "text": "عَمَّ يَتَسَآءَلُوۡنَ​ۚ‏"
        },
        {
            "id": 5674,
            "verse_key": "78:2",
            "text": "عَنِ النَّبَاِ الۡعَظِيۡمِۙ‏"
        },
        {
            "id": 5675,
            "verse_key": "78:3",
            "text": "الَّذِىۡ هُمۡ فِيۡهِ مُخۡتَلِفُوۡنَؕ‏"
        },
        {
            "id": 5676,
            "verse_key": "78:4",
            "text": "كَلَّا سَيَعۡلَمُوۡنَۙ‏"
        },
        {
            "id": 5677,
            "verse_key": "78:5",
            "text": "ثُمَّ كَلَّا سَيَعۡلَمُوۡنَ‏"
        },
        {
            "id": 5678,
            "verse_key": "78:6",
            "text": "اَلَمۡ نَجۡعَلِ الۡاَرۡضَ مِهٰدًا ۙ‏"
        },
        {
            "id": 5679,
            "verse_key": "78:7",
            "text": "وَّالۡجِبَالَ اَوۡتَادًا ۙ‏"
        },
        {
            "id": 5680,
            "verse_key": "78:8",
            "text": "وَّخَلَقۡنٰكُمۡ اَزۡوَاجًا ۙ‏"
        },
        {
            "id": 5681,
            "verse_key": "78:9",
            "text": "وَّجَعَلۡنَا نَوۡمَكُمۡ سُبَاتًا ۙ‏"
        },
        {
            "id": 5682,
            "verse_key": "78:10",
            "text": "وَّجَعَلۡنَا الَّيۡلَ لِبَاسًا ۙ‏"
        },
        {
            "id": 5683,
            "verse_key": "78:11",
            "text": "وَّجَعَلۡنَا النَّهَارَ مَعَاشًا‏"
        },
        {
            "id": 5684,
            "verse_key": "78:12",
            "text": "وَّبَنَيۡنَا فَوۡقَكُمۡ سَبۡعًا شِدَادًا ۙ‏"
        },
        {
            "id": 5685,
            "verse_key": "78:13",
            "text": "وَّ جَعَلۡنَا سِرَاجًا وَّهَّاجًا ۙ‏"
        },
        {
            "id": 5686,
            "verse_key": "78:14",
            "text": "وَّاَنۡزَلۡنَا مِنَ الۡمُعۡصِرٰتِ مَآءً ثَجَّاجًا ۙ‏"
        },
        {
            "id": 5687,
            "verse_key": "78:15",
            "text": "لِّـنُخۡرِجَ بِهٖ حَبًّا وَّنَبَاتًا ۙ‏"
        },
        {
            "id": 5688,
            "verse_key": "78:16",
            "text": "وَّجَنّٰتٍ اَلۡفَافًا ؕ‏"
        },
        {
            "id": 5689,
            "verse_key": "78:17",
            "text": "اِنَّ يَوۡمَ الۡفَصۡلِ كَانَ مِيۡقَاتًا ۙ‏"
        },
        {
            "id": 5690,
            "verse_key": "78:18",
            "text": "يَّوۡمَ يُنۡفَخُ فِى الصُّوۡرِ فَتَاۡتُوۡنَ اَفۡوَاجًا ۙ‏"
        },
        {
            "id": 5691,
            "verse_key": "78:19",
            "text": "وَّفُتِحَتِ السَّمَآءُ فَكَانَتۡ اَبۡوَابًا ۙ‏"
        },
        {
            "id": 5692,
            "verse_key": "78:20",
            "text": "وَّ سُيِّرَتِ الۡجِبَالُ فَكَانَتۡ سَرَابًا ؕ‏"
        },
        {
            "id": 5693,
            "verse_key": "78:21",
            "text": "اِنَّ جَهَنَّمَ كَانَتۡ مِرۡصَادًا ۙ‏"
        },
        {
            "id": 5694,
            "verse_key": "78:22",
            "text": "لِّلطّٰغِيۡنَ مَاٰبًا ۙ‏"
        },
        {
            "id": 5695,
            "verse_key": "78:23",
            "text": "لّٰبِثِيۡنَ فِيۡهَاۤ اَحۡقَابًا​ ۚ‏"
        },
        {
            "id": 5696,
            "verse_key": "78:24",
            "text": "لَا يَذُوۡقُوۡنَ فِيۡهَا بَرۡدًا وَّلَا شَرَابًا ۙ‏"
        },
        {
            "id": 5697,
            "verse_key": "78:25",
            "text": "اِلَّا حَمِيۡمًا وَّغَسَّاقًا ۙ‏"
        },
        {
            "id": 5698,
            "verse_key": "78:26",
            "text": "جَزَآءً وِّفَاقًا ؕ‏"
        },
        {
            "id": 5699,
            "verse_key": "78:27",
            "text": "اِنَّهُمۡ كَانُوۡا لَا يَرۡجُوۡنَ حِسَابًا ۙ‏"
        },
        {
            "id": 5700,
            "verse_key": "78:28",
            "text": "وَّكَذَّبُوۡا بِاٰيٰتِنَا كِذَّابًا ؕ‏"
        },
        {
            "id": 5701,
            "verse_key": "78:29",
            "text": "وَكُلَّ شَىۡءٍ اَحۡصَيۡنٰهُ كِتٰبًا ۙ‏"
        },
        {
            "id": 5702,
            "verse_key": "78:30",
            "text": "فَذُوۡقُوۡا فَلَنۡ نَّزِيۡدَكُمۡ اِلَّا عَذَابًا‏"
        },
        {
            "id": 5703,
            "verse_key": "78:31",
            "text": "اِنَّ لِلۡمُتَّقِيۡنَ مَفَازًا ۙ‏"
        },
        {
            "id": 5704,
            "verse_key": "78:32",
            "text": "حَدَآٮِٕقَ وَاَعۡنَابًا ۙ‏"
        },
        {
            "id": 5705,
            "verse_key": "78:33",
            "text": "وَّكَوَاعِبَ اَتۡرَابًا ۙ‏"
        },
        {
            "id": 5706,
            "verse_key": "78:34",
            "text": "وَّكَاۡسًا دِهَاقًا ؕ‏"
        },
        {
            "id": 5707,
            "verse_key": "78:35",
            "text": "لَا يَسۡمَعُوۡنَ فِيۡهَا لَـغۡوًا وَّلَا كِذّٰبًا​ ۚ‏"
        },
        {
            "id": 5708,
            "verse_key": "78:36",
            "text": "جَزَآءً مِّنۡ رَّبِّكَ عَطَآءً حِسَابًا ۙ‏"
        },
        {
            "id": 5709,
            "verse_key": "78:37",
            "text": "رَّبِّ السَّمٰوٰتِ وَالۡاَرۡضِ وَمَا بَيۡنَهُمَا الرَّحۡمٰنِ​ لَا يَمۡلِكُوۡنَ مِنۡهُ خِطَابًا​ ۚ‏"
        },
        {
            "id": 5710,
            "verse_key": "78:38",
            "text": "يَوۡمَ يَقُوۡمُ الرُّوۡحُ وَالۡمَلٰٓٮِٕكَةُ صَفًّا ؕۙ لَّا يَتَكَلَّمُوۡنَ اِلَّا مَنۡ اَذِنَ لَهُ الرَّحۡمٰنُ وَقَالَ صَوَابًا​‏"
        },
        {
            "id": 5711,
            "verse_key": "78:39",
            "text": "ذٰلِكَ الۡيَوۡمُ الۡحَـقُّ​ ۚ فَمَنۡ شَآءَ اتَّخَذَ اِلٰى رَبِّهٖ مَاٰبًا‏ ﻿﻿"
        },
        {
            "id": 5712,
            "verse_key": "78:40",
            "text": "اِنَّاۤ اَنۡذَرۡنٰـكُمۡ عَذَابًا قَرِيۡبًا ۖۚ  يَّوۡمَ يَنۡظُرُ الۡمَرۡءُ مَا قَدَّمَتۡ يَدٰهُ وَيَقُوۡلُ الۡـكٰفِرُ يٰلَيۡتَنِىۡ كُنۡتُ تُرٰبًا‏"
        },
        {
            "id": 5713,
            "verse_key": "79:1",
            "text": "وَالنّٰزِعٰتِ غَرۡقًا ۙ‏"
        },
        {
            "id": 5714,
            "verse_key": "79:2",
            "text": "وَّالنّٰشِطٰتِ نَشۡطًا ۙ‏"
        },
        {
            "id": 5715,
            "verse_key": "79:3",
            "text": "وَّالسّٰبِحٰتِ سَبۡحًا ۙ‏"
        },
        {
            "id": 5716,
            "verse_key": "79:4",
            "text": "فَالسّٰبِقٰتِ سَبۡقًا ۙ‏"
        },
        {
            "id": 5717,
            "verse_key": "79:5",
            "text": "فَالۡمُدَبِّرٰتِ اَمۡرًا​ ۘ‏"
        },
        {
            "id": 5718,
            "verse_key": "79:6",
            "text": "يَوۡمَ تَرۡجُفُ الرَّاجِفَةُ ۙ‏"
        },
        {
            "id": 5719,
            "verse_key": "79:7",
            "text": "تَتۡبَعُهَا الرَّادِفَةُ ؕ‏"
        },
        {
            "id": 5720,
            "verse_key": "79:8",
            "text": "قُلُوۡبٌ يَّوۡمَٮِٕذٍ وَّاجِفَةٌ ۙ‏"
        },
        {
            "id": 5721,
            "verse_key": "79:9",
            "text": "اَبۡصَارُهَا خَاشِعَةٌ​ ۘ‏"
        },
        {
            "id": 5722,
            "verse_key": "79:10",
            "text": "يَقُوۡلُوۡنَ ءَاِنَّا لَمَرۡدُوۡدُوۡنَ فِى الۡحَـافِرَةِ ؕ‏"
        },
        {
            "id": 5723,
            "verse_key": "79:11",
            "text": "ءَاِذَا كُنَّا عِظَامًا نَّخِرَةً ؕ‏"
        },
        {
            "id": 5724,
            "verse_key": "79:12",
            "text": "قَالُوۡا تِلۡكَ اِذًا كَرَّةٌ خَاسِرَةٌ​ ۘ‏"
        },
        {
            "id": 5725,
            "verse_key": "79:13",
            "text": "فَاِنَّمَا هِىَ زَجۡرَةٌ وَّاحِدَةٌ ۙ‏"
        },
        {
            "id": 5726,
            "verse_key": "79:14",
            "text": "فَاِذَا هُمۡ بِالسَّاهِرَةِ ؕ‏"
        },
        {
            "id": 5727,
            "verse_key": "79:15",
            "text": "هَلۡ اَتٰٮكَ حَدِيۡثُ مُوۡسٰى​ۘ‏"
        },
        {
            "id": 5728,
            "verse_key": "79:16",
            "text": "اِذۡ نَادٰٮهُ رَبُّهٗ بِالۡوَادِ الۡمُقَدَّسِ طُوًى​ۚ‏"
        },
        {
            "id": 5729,
            "verse_key": "79:17",
            "text": "اِذۡهَبۡ اِلٰى فِرۡعَوۡنَ اِنَّهٗ طَغٰى ۖ‏"
        },
        {
            "id": 5730,
            "verse_key": "79:18",
            "text": "فَقُلۡ هَلۡ لَّكَ اِلٰٓى اَنۡ تَزَكّٰى ۙ‏"
        },
        {
            "id": 5731,
            "verse_key": "79:19",
            "text": "وَاَهۡدِيَكَ اِلٰى رَبِّكَ فَتَخۡشٰى​ۚ‏"
        },
        {
            "id": 5732,
            "verse_key": "79:20",
            "text": "فَاَرٰٮهُ الۡاٰيَةَ الۡكُبۡرٰى ۖ‏"
        },
        {
            "id": 5733,
            "verse_key": "79:21",
            "text": "فَكَذَّبَ وَعَصٰى ۖ‏"
        },
        {
            "id": 5734,
            "verse_key": "79:22",
            "text": "ثُمَّ اَدۡبَرَ يَسۡعٰىۖ‏"
        },
        {
            "id": 5735,
            "verse_key": "79:23",
            "text": "فَحَشَرَ فَنَادٰىۖ‏"
        },
        {
            "id": 5736,
            "verse_key": "79:24",
            "text": "فَقَالَ اَنَا رَبُّكُمُ الۡاَعۡلٰى ۖ‏"
        },
        {
            "id": 5737,
            "verse_key": "79:25",
            "text": "فَاَخَذَهُ اللّٰهُ نَڪَالَ الۡاٰخِرَةِ وَالۡاُوۡلٰى ؕ‏"
        },
        {
            "id": 5738,
            "verse_key": "79:26",
            "text": "اِنَّ فِىۡ ذٰلِكَ لَعِبۡرَةً لِّمَنۡ يَّخۡشٰىؕ‏"
        },
        {
            "id": 5739,
            "verse_key": "79:27",
            "text": "ءَاَنۡتُمۡ اَشَدُّ خَلۡقًا اَمِ السَّمَآءُ​ ؕ بَنٰٮهَا‏"
        },
        {
            "id": 5740,
            "verse_key": "79:28",
            "text": "رَفَعَ سَمۡكَهَا فَسَوَّٮهَا ۙ‏"
        },
        {
            "id": 5741,
            "verse_key": "79:29",
            "text": "وَ اَغۡطَشَ لَيۡلَهَا وَاَخۡرَجَ ضُحٰٮهَا‏"
        },
        {
            "id": 5742,
            "verse_key": "79:30",
            "text": "وَالۡاَرۡضَ بَعۡدَ ذٰلِكَ دَحٰٮهَا ؕ‏"
        },
        {
            "id": 5743,
            "verse_key": "79:31",
            "text": "اَخۡرَجَ مِنۡهَا مَآءَهَا وَمَرۡعٰٮهَا‏"
        },
        {
            "id": 5744,
            "verse_key": "79:32",
            "text": "وَالۡجِبَالَ اَرۡسٰٮهَا ۙ‏"
        },
        {
            "id": 5745,
            "verse_key": "79:33",
            "text": "مَتَاعًا لَّـكُمۡ وَلِاَنۡعَامِكُمۡؕ‏"
        },
        {
            "id": 5746,
            "verse_key": "79:34",
            "text": "فَاِذَا جَآءَتِ الطَّآمَّةُ الۡكُبۡرٰى ۖ‏"
        },
        {
            "id": 5747,
            "verse_key": "79:35",
            "text": "يَوۡمَ يَتَذَكَّرُ الۡاِنۡسَانُ مَا سَعٰىۙ‏"
        },
        {
            "id": 5748,
            "verse_key": "79:36",
            "text": "وَبُرِّزَتِ الۡجَحِيۡمُ لِمَنۡ يَّرٰى‏"
        },
        {
            "id": 5749,
            "verse_key": "79:37",
            "text": "فَاَمَّا مَنۡ طَغٰىۙ‏"
        },
        {
            "id": 5750,
            "verse_key": "79:38",
            "text": "وَاٰثَرَ الۡحَيٰوةَ الدُّنۡيَا ۙ‏"
        },
        {
            "id": 5751,
            "verse_key": "79:39",
            "text": "فَاِنَّ الۡجَحِيۡمَ هِىَ الۡمَاۡوٰىؕ‏"
        },
        {
            "id": 5752,
            "verse_key": "79:40",
            "text": "وَاَمَّا مَنۡ خَافَ مَقَامَ رَبِّهٖ وَ نَهَى النَّفۡسَ عَنِ الۡهَوٰىۙ‏"
        },
        {
            "id": 5753,
            "verse_key": "79:41",
            "text": "فَاِنَّ الۡجَـنَّةَ هِىَ الۡمَاۡوٰىؕ‏"
        },
        {
            "id": 5754,
            "verse_key": "79:42",
            "text": "يَسۡـــَٔلُوۡنَكَ عَنِ السَّاعَةِ اَيَّانَ مُرۡسٰٮهَا ؕ‏"
        },
        {
            "id": 5755,
            "verse_key": "79:43",
            "text": "فِيۡمَ اَنۡتَ مِنۡ ذِكۡرٰٮهَاؕ‏"
        },
        {
            "id": 5756,
            "verse_key": "79:44",
            "text": "اِلٰى رَبِّكَ مُنۡتَهٰٮهَاؕ‏"
        },
        {
            "id": 5757,
            "verse_key": "79:45",
            "text": "اِنَّمَاۤ اَنۡتَ مُنۡذِرُ مَنۡ يَّخۡشٰٮهَاؕ‏"
        },
        {
            "id": 5758,
            "verse_key": "79:46",
            "text": "كَاَنَّهُمۡ يَوۡمَ يَرَوۡنَهَا لَمۡ يَلۡبَثُوۡۤا اِلَّا عَشِيَّةً اَوۡ ضُحٰٮهَا‏ ﻿﻿"
        },
        {
            "id": 5759,
            "verse_key": "80:1",
            "text": "عَبَسَ وَتَوَلّٰٓىۙ‏"
        },
        {
            "id": 5760,
            "verse_key": "80:2",
            "text": "اَنۡ جَآءَهُ الۡاَعۡمٰىؕ‏"
        },
        {
            "id": 5761,
            "verse_key": "80:3",
            "text": "وَمَا يُدۡرِيۡكَ لَعَلَّهٗ يَزَّكّٰٓىۙ‏"
        },
        {
            "id": 5762,
            "verse_key": "80:4",
            "text": "اَوۡ يَذَّكَّرُ فَتَنۡفَعَهُ الذِّكۡرٰىؕ‏"
        },
        {
            "id": 5763,
            "verse_key": "80:5",
            "text": "اَمَّا مَنِ اسۡتَغۡنٰىۙ‏"
        },
        {
            "id": 5764,
            "verse_key": "80:6",
            "text": "فَاَنۡتَ لَهٗ تَصَدّٰىؕ‏"
        },
        {
            "id": 5765,
            "verse_key": "80:7",
            "text": "وَمَا عَلَيۡكَ اَلَّا يَزَّكّٰٓىؕ‏"
        },
        {
            "id": 5766,
            "verse_key": "80:8",
            "text": "وَاَمَّا مَنۡ جَآءَكَ يَسۡعٰىۙ‏"
        },
        {
            "id": 5767,
            "verse_key": "80:9",
            "text": "وَهُوَ يَخۡشٰىۙ‏"
        },
        {
            "id": 5768,
            "verse_key": "80:10",
            "text": "فَاَنۡتَ عَنۡهُ تَلَهّٰى​ۚ‏"
        },
        {
            "id": 5769,
            "verse_key": "80:11",
            "text": "كَلَّاۤ اِنَّهَا تَذۡكِرَةٌ ۚ‏"
        },
        {
            "id": 5770,
            "verse_key": "80:12",
            "text": "فَمَنۡ شَآءَ ذَكَرَهٗ​ۘ‏"
        },
        {
            "id": 5771,
            "verse_key": "80:13",
            "text": "فِىۡ صُحُفٍ مُّكَرَّمَةٍۙ‏"
        },
        {
            "id": 5772,
            "verse_key": "80:14",
            "text": "مَّرۡفُوۡعَةٍ مُّطَهَّرَةٍ ۭۙ‏"
        },
        {
            "id": 5773,
            "verse_key": "80:15",
            "text": "بِاَيۡدِىۡ سَفَرَةٍۙ‏"
        },
        {
            "id": 5774,
            "verse_key": "80:16",
            "text": "كِرَامٍۢ بَرَرَةٍؕ‏"
        },
        {
            "id": 5775,
            "verse_key": "80:17",
            "text": "قُتِلَ الۡاِنۡسَانُ مَاۤ اَكۡفَرَهٗؕ‏"
        },
        {
            "id": 5776,
            "verse_key": "80:18",
            "text": "مِنۡ اَىِّ شَىۡءٍ خَلَقَهٗؕ‏"
        },
        {
            "id": 5777,
            "verse_key": "80:19",
            "text": "مِنۡ نُّطۡفَةٍؕ خَلَقَهٗ فَقَدَّرَهٗ ۙ‏"
        },
        {
            "id": 5778,
            "verse_key": "80:20",
            "text": "ثُمَّ السَّبِيۡلَ يَسَّرَهٗۙ‏"
        },
        {
            "id": 5779,
            "verse_key": "80:21",
            "text": "ثُمَّ اَمَاتَهٗ فَاَقۡبَرَهٗۙ‏"
        },
        {
            "id": 5780,
            "verse_key": "80:22",
            "text": "ثُمَّ اِذَا شَآءَ اَنۡشَرَهٗؕ‏"
        },
        {
            "id": 5781,
            "verse_key": "80:23",
            "text": "كَلَّا لَـمَّا يَقۡضِ مَاۤ اَمَرَهٗؕ‏"
        },
        {
            "id": 5782,
            "verse_key": "80:24",
            "text": "فَلۡيَنۡظُرِ الۡاِنۡسَانُ اِلٰى طَعَامِهٖۤۙ‏"
        },
        {
            "id": 5783,
            "verse_key": "80:25",
            "text": "اَنَّا صَبَبۡنَا الۡمَآءَ صَبًّا ۙ‏"
        },
        {
            "id": 5784,
            "verse_key": "80:26",
            "text": "ثُمَّ شَقَقۡنَا الۡاَرۡضَ شَقًّا ۙ‏"
        },
        {
            "id": 5785,
            "verse_key": "80:27",
            "text": "فَاَنۡۢبَتۡنَا فِيۡهَا حَبًّا ۙ‏"
        },
        {
            "id": 5786,
            "verse_key": "80:28",
            "text": "وَّ عِنَبًا وَّقَضۡبًا ۙ‏"
        },
        {
            "id": 5787,
            "verse_key": "80:29",
            "text": "وَّزَيۡتُوۡنًا وَّنَخۡلًا ؕ‏"
        },
        {
            "id": 5788,
            "verse_key": "80:30",
            "text": "وَحَدَآٮِٕقَ غُلۡبًا ۙ‏"
        },
        {
            "id": 5789,
            "verse_key": "80:31",
            "text": "وَّفَاكِهَةً وَّاَبًّا ۙ‏"
        },
        {
            "id": 5790,
            "verse_key": "80:32",
            "text": "مَّتَاعًا لَّـكُمۡ وَلِاَنۡعَامِكُمۡؕ‏"
        },
        {
            "id": 5791,
            "verse_key": "80:33",
            "text": "فَاِذَا جَآءَتِ الصَّآخَّةُ‏"
        },
        {
            "id": 5792,
            "verse_key": "80:34",
            "text": "يَوۡمَ يَفِرُّ الۡمَرۡءُ مِنۡ اَخِيۡهِۙ‏"
        },
        {
            "id": 5793,
            "verse_key": "80:35",
            "text": "وَاُمِّهٖ وَاَبِيۡهِۙ‏"
        },
        {
            "id": 5794,
            "verse_key": "80:36",
            "text": "وَصَاحِبَتِهٖ وَبَنِيۡهِؕ‏"
        },
        {
            "id": 5795,
            "verse_key": "80:37",
            "text": "لِكُلِّ امۡرِیءٍ مِّنۡهُمۡ يَوۡمَٮِٕذٍ شَاۡنٌ يُّغۡنِيۡهِؕ‏"
        },
        {
            "id": 5796,
            "verse_key": "80:38",
            "text": "وُجُوۡهٌ يَّوۡمَٮِٕذٍ مُّسۡفِرَةٌ ۙ‏"
        },
        {
            "id": 5797,
            "verse_key": "80:39",
            "text": "ضَاحِكَةٌ مُّسۡتَبۡشِرَةٌ ۚ‏"
        },
        {
            "id": 5798,
            "verse_key": "80:40",
            "text": "وَوُجُوۡهٌ يَّوۡمَٮِٕذٍ عَلَيۡهَا غَبَرَةٌ ۙ‏"
        },
        {
            "id": 5799,
            "verse_key": "80:41",
            "text": "تَرۡهَقُهَا قَتَرَةٌ ؕ‏"
        },
        {
            "id": 5800,
            "verse_key": "80:42",
            "text": "اُولٰٓٮِٕكَ هُمُ الۡكَفَرَةُ الۡفَجَرَةُ‏"
        },
        {
            "id": 5801,
            "verse_key": "81:1",
            "text": "اِذَا الشَّمۡسُ كُوِّرَتۡۙ‏"
        },
        {
            "id": 5802,
            "verse_key": "81:2",
            "text": "وَاِذَا النُّجُوۡمُ انْكَدَرَتۡۙ‏"
        },
        {
            "id": 5803,
            "verse_key": "81:3",
            "text": "وَاِذَا الۡجِبَالُ سُيِّرَتۡۙ‏"
        },
        {
            "id": 5804,
            "verse_key": "81:4",
            "text": "وَاِذَا الۡعِشَارُ عُطِّلَتۡۙ‏"
        },
        {
            "id": 5805,
            "verse_key": "81:5",
            "text": "وَاِذَا الۡوُحُوۡشُ حُشِرَتۡۙ‏"
        },
        {
            "id": 5806,
            "verse_key": "81:6",
            "text": "وَاِذَا الۡبِحَارُ سُجِّرَتۡۙ‏"
        },
        {
            "id": 5807,
            "verse_key": "81:7",
            "text": "وَاِذَا النُّفُوۡسُ زُوِّجَتۡۙ‏"
        },
        {
            "id": 5808,
            "verse_key": "81:8",
            "text": "وَاِذَا الۡمَوۡءٗدَةُ سُٮِٕلَتۡۙ‏"
        },
        {
            "id": 5809,
            "verse_key": "81:9",
            "text": "بِاَىِّ ذَنۡۢبٍ قُتِلَتۡ​ۚ‏"
        },
        {
            "id": 5810,
            "verse_key": "81:10",
            "text": "وَاِذَا الصُّحُفُ نُشِرَتۡۙ‏"
        },
        {
            "id": 5811,
            "verse_key": "81:11",
            "text": "وَاِذَا السَّمَآءُ كُشِطَتۡۙ‏"
        },
        {
            "id": 5812,
            "verse_key": "81:12",
            "text": "وَاِذَا الۡجَحِيۡمُ سُعِّرَتۡۙ‏"
        },
        {
            "id": 5813,
            "verse_key": "81:13",
            "text": "وَاِذَا الۡجَـنَّةُ اُزۡلِفَتۡۙ‏"
        },
        {
            "id": 5814,
            "verse_key": "81:14",
            "text": "عَلِمَتۡ نَفۡسٌ مَّاۤ اَحۡضَرَتۡؕ‏"
        },
        {
            "id": 5815,
            "verse_key": "81:15",
            "text": "فَلَاۤ اُقۡسِمُ بِالۡخُنَّسِۙ‏"
        },
        {
            "id": 5816,
            "verse_key": "81:16",
            "text": "الۡجَوَارِ الۡكُنَّسِۙ‏"
        },
        {
            "id": 5817,
            "verse_key": "81:17",
            "text": "وَالَّيۡلِ اِذَا عَسۡعَسَۙ‏"
        },
        {
            "id": 5818,
            "verse_key": "81:18",
            "text": "وَالصُّبۡحِ اِذَا تَنَفَّسَۙ‏"
        },
        {
            "id": 5819,
            "verse_key": "81:19",
            "text": "اِنَّهٗ لَقَوۡلُ رَسُوۡلٍ كَرِيۡمٍۙ‏"
        },
        {
            "id": 5820,
            "verse_key": "81:20",
            "text": "ذِىۡ قُوَّةٍ عِنۡدَ ذِى الۡعَرۡشِ مَكِيۡنٍۙ‏"
        },
        {
            "id": 5821,
            "verse_key": "81:21",
            "text": "مُّطَاعٍ ثَمَّ اَمِيۡنٍؕ‏"
        },
        {
            "id": 5822,
            "verse_key": "81:22",
            "text": "وَ مَا صَاحِبُكُمۡ بِمَجۡنُوۡنٍ​ۚ‏"
        },
        {
            "id": 5823,
            "verse_key": "81:23",
            "text": "وَلَقَدۡ رَاٰهُ بِالۡاُفُقِ الۡمُبِيۡنِ​ۚ‏"
        },
        {
            "id": 5824,
            "verse_key": "81:24",
            "text": "وَمَا هُوَ عَلَى الۡغَيۡبِ بِضَنِيۡنٍ​ۚ‏"
        },
        {
            "id": 5825,
            "verse_key": "81:25",
            "text": "وَمَا هُوَ بِقَوۡلِ شَيۡطٰنٍ رَّجِيۡمٍۙ‏"
        },
        {
            "id": 5826,
            "verse_key": "81:26",
            "text": "فَاَيۡنَ تَذۡهَبُوۡنَؕ‏"
        },
        {
            "id": 5827,
            "verse_key": "81:27",
            "text": "اِنۡ هُوَ اِلَّا ذِكۡرٌ لِّلۡعٰلَمِيۡنَۙ‏"
        },
        {
            "id": 5828,
            "verse_key": "81:28",
            "text": "لِمَنۡ شَآءَ مِنۡكُمۡ اَنۡ يَّسۡتَقِيۡمَؕ‏"
        },
        {
            "id": 5829,
            "verse_key": "81:29",
            "text": "وَمَا تَشَآءُوۡنَ اِلَّاۤ اَنۡ يَّشَآءَ اللّٰهُ رَبُّ الۡعٰلَمِيۡنَ‏"
        },
        {
            "id": 5830,
            "verse_key": "82:1",
            "text": "اِذَا السَّمَآءُ انْفَطَرَتۡۙ‏"
        },
        {
            "id": 5831,
            "verse_key": "82:2",
            "text": "وَاِذَا الۡكَوَاكِبُ انْتَثَرَتۡۙ‏"
        },
        {
            "id": 5832,
            "verse_key": "82:3",
            "text": "وَاِذَا الۡبِحَارُ فُجِّرَتۡۙ‏"
        },
        {
            "id": 5833,
            "verse_key": "82:4",
            "text": "وَاِذَا الۡقُبُوۡرُ بُعۡثِرَتۡۙ‏"
        },
        {
            "id": 5834,
            "verse_key": "82:5",
            "text": "عَلِمَتۡ نَفۡسٌ مَّا قَدَّمَتۡ وَاَخَّرَتۡؕ‏"
        },
        {
            "id": 5835,
            "verse_key": "82:6",
            "text": "يٰۤاَيُّهَا الۡاِنۡسَانُ مَا غَرَّكَ بِرَبِّكَ الۡكَرِيۡمِۙ‏"
        },
        {
            "id": 5836,
            "verse_key": "82:7",
            "text": "الَّذِىۡ خَلَقَكَ فَسَوّٰٮكَ فَعَدَلَـكَۙ‏"
        },
        {
            "id": 5837,
            "verse_key": "82:8",
            "text": "فِىۡۤ اَىِّ صُوۡرَةٍ مَّا شَآءَ رَكَّبَكَؕ‏"
        },
        {
            "id": 5838,
            "verse_key": "82:9",
            "text": "كَلَّا بَلۡ تُكَذِّبُوۡنَ بِالدِّيۡنِۙ‏"
        },
        {
            "id": 5839,
            "verse_key": "82:10",
            "text": "وَاِنَّ عَلَيۡكُمۡ لَحٰـفِظِيۡنَۙ‏"
        },
        {
            "id": 5840,
            "verse_key": "82:11",
            "text": "كِرَامًا كَاتِبِيۡنَۙ‏"
        },
        {
            "id": 5841,
            "verse_key": "82:12",
            "text": "يَعۡلَمُوۡنَ مَا تَفۡعَلُوۡنَ‏"
        },
        {
            "id": 5842,
            "verse_key": "82:13",
            "text": "اِنَّ الۡاَبۡرَارَ لَفِىۡ نَعِيۡمٍۚ‏"
        },
        {
            "id": 5843,
            "verse_key": "82:14",
            "text": "وَاِنَّ الۡفُجَّارَ لَفِىۡ جَحِيۡمٍ ۚۖ‏"
        },
        {
            "id": 5844,
            "verse_key": "82:15",
            "text": "يَّصۡلَوۡنَهَا يَوۡمَ الدِّيۡنِ‏"
        },
        {
            "id": 5845,
            "verse_key": "82:16",
            "text": "وَمَا هُمۡ عَنۡهَا بِغَآٮِٕبِيۡنَؕ‏"
        },
        {
            "id": 5846,
            "verse_key": "82:17",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا يَوۡمُ الدِّيۡنِۙ‏"
        },
        {
            "id": 5847,
            "verse_key": "82:18",
            "text": "ثُمَّ مَاۤ اَدۡرٰٮكَ مَا يَوۡمُ الدِّيۡنِؕ‏"
        },
        {
            "id": 5848,
            "verse_key": "82:19",
            "text": "يَوۡمَ لَا تَمۡلِكُ نَفۡسٌ لِّنَفۡسٍ شَيۡـــًٔا​ ؕ وَالۡاَمۡرُ يَوۡمَٮِٕذٍ لِّلَّهِ‏"
        },
        {
            "id": 5849,
            "verse_key": "83:1",
            "text": "وَيۡلٌ لِّلۡمُطَفِّفِيۡنَۙ‏"
        },
        {
            "id": 5850,
            "verse_key": "83:2",
            "text": "الَّذِيۡنَ اِذَا اكۡتَالُوۡا عَلَى النَّاسِ يَسۡتَوۡفُوۡنَۖ‏"
        },
        {
            "id": 5851,
            "verse_key": "83:3",
            "text": "وَاِذَا كَالُوۡهُمۡ اَوْ وَّزَنُوۡهُمۡ يُخۡسِرُوۡنَؕ‏"
        },
        {
            "id": 5852,
            "verse_key": "83:4",
            "text": "اَلَا يَظُنُّ اُولٰٓٮِٕكَ اَنَّهُمۡ مَّبۡعُوۡثُوۡنَۙ‏"
        },
        {
            "id": 5853,
            "verse_key": "83:5",
            "text": "لِيَوۡمٍ عَظِيۡمٍۙ‏"
        },
        {
            "id": 5854,
            "verse_key": "83:6",
            "text": "يَّوۡمَ يَقُوۡمُ النَّاسُ لِرَبِّ الۡعٰلَمِيۡنَؕ‏"
        },
        {
            "id": 5855,
            "verse_key": "83:7",
            "text": "كَلَّاۤ اِنَّ كِتٰبَ الۡفُجَّارِ لَفِىۡ سِجِّيۡنٍؕ‏"
        },
        {
            "id": 5856,
            "verse_key": "83:8",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا سِجِّيۡنٌؕ‏"
        },
        {
            "id": 5857,
            "verse_key": "83:9",
            "text": "كِتٰبٌ مَّرۡقُوۡمٌؕ‏"
        },
        {
            "id": 5858,
            "verse_key": "83:10",
            "text": "وَيۡلٌ يَّوۡمَٮِٕذٍ لِّلۡمُكَذِّبِيۡنَۙ‏"
        },
        {
            "id": 5859,
            "verse_key": "83:11",
            "text": "الَّذِيۡنَ يُكَذِّبُوۡنَ بِيَوۡمِ الدِّيۡنِؕ‏"
        },
        {
            "id": 5860,
            "verse_key": "83:12",
            "text": "وَمَا يُكَذِّبُ بِهٖۤ اِلَّا كُلُّ مُعۡتَدٍ اَثِيۡمٍۙ‏"
        },
        {
            "id": 5861,
            "verse_key": "83:13",
            "text": "اِذَا تُتۡلٰى عَلَيۡهِ اٰيٰتُنَا قَالَ اَسَاطِيۡرُ الۡاَوَّلِيۡنَؕ‏"
        },
        {
            "id": 5862,
            "verse_key": "83:14",
            "text": "كَلَّا​ بَلۡ رَانَ عَلٰى قُلُوۡبِهِمۡ مَّا كَانُوۡا يَكۡسِبُوۡنَ‏"
        },
        {
            "id": 5863,
            "verse_key": "83:15",
            "text": "كَلَّاۤ اِنَّهُمۡ عَنۡ رَّبِّهِمۡ يَوۡمَٮِٕذٍ لَّمَحۡجُوۡبُوۡنَ​ؕ‏"
        },
        {
            "id": 5864,
            "verse_key": "83:16",
            "text": "ثُمَّ اِنَّهُمۡ لَصَالُوا الۡجَحِيۡمِؕ‏"
        },
        {
            "id": 5865,
            "verse_key": "83:17",
            "text": "ثُمَّ يُقَالُ هٰذَا الَّذِىۡ كُنۡتُمۡ بِهٖ تُكَذِّبُوۡنَؕ‏"
        },
        {
            "id": 5866,
            "verse_key": "83:18",
            "text": "كَلَّاۤ اِنَّ كِتٰبَ الۡاَبۡرَارِ لَفِىۡ عِلِّيِّيۡنَؕ‏"
        },
        {
            "id": 5867,
            "verse_key": "83:19",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا عِلِّيُّوۡنَؕ‏"
        },
        {
            "id": 5868,
            "verse_key": "83:20",
            "text": "كِتٰبٌ مَّرۡقُوۡمٌۙ‏"
        },
        {
            "id": 5869,
            "verse_key": "83:21",
            "text": "يَّشۡهَدُهُ الۡمُقَرَّبُوۡنَؕ‏"
        },
        {
            "id": 5870,
            "verse_key": "83:22",
            "text": "اِنَّ الۡاَبۡرَارَ لَفِىۡ نَعِيۡمٍۙ‏"
        },
        {
            "id": 5871,
            "verse_key": "83:23",
            "text": "عَلَى الۡاَرَآٮِٕكِ يَنۡظُرُوۡنَۙ‏"
        },
        {
            "id": 5872,
            "verse_key": "83:24",
            "text": "تَعۡرِفُ فِىۡ وُجُوۡهِهِمۡ نَضۡرَةَ النَّعِيۡمِ​ۚ‏"
        },
        {
            "id": 5873,
            "verse_key": "83:25",
            "text": "يُسۡقَوۡنَ مِنۡ رَّحِيۡقٍ مَّخۡتُوۡمٍۙ‏"
        },
        {
            "id": 5874,
            "verse_key": "83:26",
            "text": "خِتٰمُهٗ مِسۡكٌ ​ؕ وَفِىۡ ذٰلِكَ فَلۡيَتَنَافَسِ الۡمُتَنــَافِسُوۡنَؕ‏"
        },
        {
            "id": 5875,
            "verse_key": "83:27",
            "text": "وَ مِزَاجُهٗ مِنۡ تَسۡنِيۡمٍۙ‏"
        },
        {
            "id": 5876,
            "verse_key": "83:28",
            "text": "عَيۡنًا يَّشۡرَبُ بِهَا الۡمُقَرَّبُوۡنَؕ‏"
        },
        {
            "id": 5877,
            "verse_key": "83:29",
            "text": "اِنَّ الَّذِيۡنَ اَجۡرَمُوۡا كَانُوۡا مِنَ الَّذِيۡنَ اٰمَنُوۡا يَضۡحَكُوۡنَ  ۖ‏"
        },
        {
            "id": 5878,
            "verse_key": "83:30",
            "text": "وَاِذَا مَرُّوۡا بِهِمۡ يَتَغَامَزُوۡنَ  ۖ‏"
        },
        {
            "id": 5879,
            "verse_key": "83:31",
            "text": "وَاِذَا انۡقَلَبُوۡۤا اِلٰٓى اَهۡلِهِمُ انْقَلَبُوۡا فَكِهِيۡنَ  ۖ‏"
        },
        {
            "id": 5880,
            "verse_key": "83:32",
            "text": "وَاِذَا رَاَوۡهُمۡ قَالُوۡۤا اِنَّ هٰٓؤُلَاۤءِ لَـضَآلُّوۡنَۙ‏"
        },
        {
            "id": 5881,
            "verse_key": "83:33",
            "text": "وَمَاۤ اُرۡسِلُوۡا عَلَيۡهِمۡ حٰفِظِيۡنَۙ‏"
        },
        {
            "id": 5882,
            "verse_key": "83:34",
            "text": "فَالۡيَوۡمَ الَّذِيۡنَ اٰمَنُوۡا مِنَ الۡكُفَّارِ يَضۡحَكُوۡنَۙ‏"
        },
        {
            "id": 5883,
            "verse_key": "83:35",
            "text": "عَلَى الۡاَرَآٮِٕكِۙ يَنۡظُرُوۡنَؕ‏"
        },
        {
            "id": 5884,
            "verse_key": "83:36",
            "text": "هَلۡ ثُوِّبَ الۡكُفَّارُ مَا كَانُوۡا يَفۡعَلُوۡنَ‏"
        },
        {
            "id": 5885,
            "verse_key": "84:1",
            "text": "اِذَا السَّمَآءُ انْشَقَّتۡۙ‏"
        },
        {
            "id": 5886,
            "verse_key": "84:2",
            "text": "وَاَذِنَتۡ لِرَبِّهَا وَحُقَّتۡۙ‏"
        },
        {
            "id": 5887,
            "verse_key": "84:3",
            "text": "وَاِذَا الۡاَرۡضُ مُدَّتۡؕ‏"
        },
        {
            "id": 5888,
            "verse_key": "84:4",
            "text": "وَاَلۡقَتۡ مَا فِيۡهَا وَتَخَلَّتۡۙ‏"
        },
        {
            "id": 5889,
            "verse_key": "84:5",
            "text": "وَاَذِنَتۡ لِرَبِّهَا وَحُقَّتۡؕ‏"
        },
        {
            "id": 5890,
            "verse_key": "84:6",
            "text": "يٰۤاَيُّهَا الۡاِنۡسَانُ اِنَّكَ كَادِحٌ اِلٰى رَبِّكَ كَدۡحًا فَمُلٰقِيۡهِ​ۚ‏ ﻿﻿"
        },
        {
            "id": 5891,
            "verse_key": "84:7",
            "text": "فَاَمَّا مَنۡ اُوۡتِىَ كِتٰبَهٗ بِيَمِيۡنِهٖۙ‏"
        },
        {
            "id": 5892,
            "verse_key": "84:8",
            "text": "فَسَوۡفَ يُحَاسَبُ حِسَابًا يَّسِيۡرًا ۙ‏"
        },
        {
            "id": 5893,
            "verse_key": "84:9",
            "text": "وَّيَنۡقَلِبُ اِلٰٓى اَهۡلِهٖ مَسۡرُوۡرًا ؕ‏"
        },
        {
            "id": 5894,
            "verse_key": "84:10",
            "text": "وَاَمَّا مَنۡ اُوۡتِىَ كِتٰبَهٗ وَرَآءَ ظَهۡرِهٖۙ‏"
        },
        {
            "id": 5895,
            "verse_key": "84:11",
            "text": "فَسَوۡفَ يَدۡعُوۡا ثُبُوۡرًا ۙ‏"
        },
        {
            "id": 5896,
            "verse_key": "84:12",
            "text": "وَّيَصۡلٰى سَعِيۡرًا ؕ‏"
        },
        {
            "id": 5897,
            "verse_key": "84:13",
            "text": "اِنَّهٗ كَانَ فِىۡۤ اَهۡلِهٖ مَسۡرُوۡرًا ؕ‏"
        },
        {
            "id": 5898,
            "verse_key": "84:14",
            "text": "اِنَّهٗ ظَنَّ اَنۡ لَّنۡ يَّحُوۡرَ ۛۚ‏"
        },
        {
            "id": 5899,
            "verse_key": "84:15",
            "text": "بَلٰٓى ۛۚ اِنَّ رَبَّهٗ كَانَ بِهٖ بَصِيۡرًا ؕ‏"
        },
        {
            "id": 5900,
            "verse_key": "84:16",
            "text": "فَلَاۤ اُقۡسِمُ بِالشَّفَقِۙ‏"
        },
        {
            "id": 5901,
            "verse_key": "84:17",
            "text": "وَالَّيۡلِ وَمَا وَسَقَۙ‏"
        },
        {
            "id": 5902,
            "verse_key": "84:18",
            "text": "وَالۡقَمَرِ اِذَا اتَّسَقَۙ‏"
        },
        {
            "id": 5903,
            "verse_key": "84:19",
            "text": "لَتَرۡكَبُنَّ طَبَقًا عَنۡ طَبَقٍؕ‏"
        },
        {
            "id": 5904,
            "verse_key": "84:20",
            "text": "فَمَا لَهُمۡ لَا يُؤۡمِنُوۡنَۙ‏"
        },
        {
            "id": 5905,
            "verse_key": "84:21",
            "text": "وَاِذَا قُرِئَ عَلَيۡهِمُ الۡقُرۡاٰنُ لَا يَسۡجُدُوۡنَ ؕ ۩‏"
        },
        {
            "id": 5906,
            "verse_key": "84:22",
            "text": "بَلِ الَّذِيۡنَ كَفَرُوۡا يُكَذِّبُوۡنَ ۖ‏"
        },
        {
            "id": 5907,
            "verse_key": "84:23",
            "text": "وَاللّٰهُ اَعۡلَمُ بِمَا يُوۡعُوۡنَ ۖ‏"
        },
        {
            "id": 5908,
            "verse_key": "84:24",
            "text": "فَبَشِّرۡهُمۡ بِعَذَابٍ اَلِيۡمٍۙ‏"
        },
        {
            "id": 5909,
            "verse_key": "84:25",
            "text": "اِلَّا الَّذِيۡنَ اٰمَنُوۡا وَعَمِلُوا الصّٰلِحٰتِ لَهُمۡ اَجۡرٌ غَيۡرُ مَمۡنُوۡنٍ‏ ﻿﻿"
        },
        {
            "id": 5910,
            "verse_key": "85:1",
            "text": "وَالسَّمَآءِ ذَاتِ الۡبُرُوۡجِۙ‏"
        },
        {
            "id": 5911,
            "verse_key": "85:2",
            "text": "وَالۡيَوۡمِ الۡمَوۡعُوۡدِۙ‏"
        },
        {
            "id": 5912,
            "verse_key": "85:3",
            "text": "وَشَاهِدٍ وَّمَشۡهُوۡدٍؕ‏"
        },
        {
            "id": 5913,
            "verse_key": "85:4",
            "text": "قُتِلَ اَصۡحٰبُ الۡاُخۡدُوۡدِۙ‏"
        },
        {
            "id": 5914,
            "verse_key": "85:5",
            "text": "النَّارِ ذَاتِ الۡوَقُوۡدِۙ‏"
        },
        {
            "id": 5915,
            "verse_key": "85:6",
            "text": "اِذۡ هُمۡ عَلَيۡهَا قُعُوۡدٌ ۙ‏"
        },
        {
            "id": 5916,
            "verse_key": "85:7",
            "text": "وَّهُمۡ عَلٰى مَا يَفۡعَلُوۡنَ بِالۡمُؤۡمِنِيۡنَ شُهُوۡدٌ ؕ‏"
        },
        {
            "id": 5917,
            "verse_key": "85:8",
            "text": "وَمَا نَقَمُوۡا مِنۡهُمۡ اِلَّاۤ اَنۡ يُّؤۡمِنُوۡا بِاللّٰهِ الۡعَزِيۡزِ الۡحَمِيۡدِۙ‏ ﻿﻿"
        },
        {
            "id": 5918,
            "verse_key": "85:9",
            "text": "الَّذِىۡ لَهٗ مُلۡكُ السَّمٰوٰتِ وَالۡاَرۡضِ​ؕ وَ اللّٰهُ عَلٰى كُلِّ شَىۡءٍ شَهِيۡدٌ ؕ‏"
        },
        {
            "id": 5919,
            "verse_key": "85:10",
            "text": "اِنَّ الَّذِيۡنَ فَتَـنُوا الۡمُؤۡمِنِيۡنَ وَ الۡمُؤۡمِنٰتِ ثُمَّ لَمۡ يَتُوۡبُوۡا فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ الۡحَرِيۡقِؕ‏"
        },
        {
            "id": 5920,
            "verse_key": "85:11",
            "text": "اِنَّ الَّذِيۡنَ اٰمَنُوۡا وَعَمِلُوا الصّٰلِحٰتِ لَهُمۡ جَنّٰتٌ تَجۡرِىۡ مِنۡ تَحۡتِهَا الۡاَنۡهٰرُ ؕ ذٰلِكَ الۡفَوۡزُ الۡكَبِيۡرُؕ‏"
        },
        {
            "id": 5921,
            "verse_key": "85:12",
            "text": "اِنَّ بَطۡشَ رَبِّكَ لَشَدِيۡدٌ ؕ‏"
        },
        {
            "id": 5922,
            "verse_key": "85:13",
            "text": "اِنَّهٗ هُوَ يُبۡدِئُ وَيُعِيۡدُ​ ۚ‏"
        },
        {
            "id": 5923,
            "verse_key": "85:14",
            "text": "وَهُوَ الۡغَفُوۡرُ الۡوَدُوۡدُۙ‏"
        },
        {
            "id": 5924,
            "verse_key": "85:15",
            "text": "ذُو الۡعَرۡشِ الۡمَجِيۡدُ ۙ‏"
        },
        {
            "id": 5925,
            "verse_key": "85:16",
            "text": "فَعَّالٌ لِّمَا يُرِيۡدُ ؕ‏"
        },
        {
            "id": 5926,
            "verse_key": "85:17",
            "text": "هَلۡ اَتٰٮكَ حَدِيۡثُ الۡجُـنُوۡدِۙ‏"
        },
        {
            "id": 5927,
            "verse_key": "85:18",
            "text": "فِرۡعَوۡنَ وَثَمُوۡدَؕ‏"
        },
        {
            "id": 5928,
            "verse_key": "85:19",
            "text": "بَلِ الَّذِيۡنَ كَفَرُوۡا فِىۡ تَكۡذِيۡبٍۙ‏"
        },
        {
            "id": 5929,
            "verse_key": "85:20",
            "text": "وَّاللّٰهُ مِنۡ وَّرَآٮِٕهِمۡ مُّحِيۡطٌۚ‏"
        },
        {
            "id": 5930,
            "verse_key": "85:21",
            "text": "بَلۡ هُوَ قُرۡاٰنٌ مَّجِيۡدٌ ۙ‏"
        },
        {
            "id": 5931,
            "verse_key": "85:22",
            "text": "فِىۡ لَوۡحٍ مَّحۡفُوۡظٍ‏"
        },
        {
            "id": 5932,
            "verse_key": "86:1",
            "text": "وَالسَّمَآءِ وَالطَّارِقِۙ‏"
        },
        {
            "id": 5933,
            "verse_key": "86:2",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا الطَّارِقُۙ‏"
        },
        {
            "id": 5934,
            "verse_key": "86:3",
            "text": "النَّجۡمُ الثَّاقِبُۙ‏"
        },
        {
            "id": 5935,
            "verse_key": "86:4",
            "text": "اِنۡ كُلُّ نَفۡسٍ لَّمَّا عَلَيۡهَا حَافِظٌؕ‏"
        },
        {
            "id": 5936,
            "verse_key": "86:5",
            "text": "فَلۡيَنۡظُرِ الۡاِنۡسَانُ مِمَّ خُلِقَؕ‏"
        },
        {
            "id": 5937,
            "verse_key": "86:6",
            "text": "خُلِقَ مِنۡ مَّآءٍ دَافِقٍۙ‏"
        },
        {
            "id": 5938,
            "verse_key": "86:7",
            "text": "يَّخۡرُجُ مِنۡۢ بَيۡنِ الصُّلۡبِ وَالتَّرَآٮِٕبِؕ‏"
        },
        {
            "id": 5939,
            "verse_key": "86:8",
            "text": "اِنَّهٗ عَلٰى رَجۡعِهٖ لَقَادِرٌؕ‏"
        },
        {
            "id": 5940,
            "verse_key": "86:9",
            "text": "يَوۡمَ تُبۡلَى السَّرَآٮِٕرُۙ‏"
        },
        {
            "id": 5941,
            "verse_key": "86:10",
            "text": "فَمَا لَهٗ مِنۡ قُوَّةٍ وَّلَا نَاصِرٍؕ‏"
        },
        {
            "id": 5942,
            "verse_key": "86:11",
            "text": "وَالسَّمَآءِ ذَاتِ الرَّجۡعِۙ‏"
        },
        {
            "id": 5943,
            "verse_key": "86:12",
            "text": "وَالۡاَرۡضِ ذَاتِ الصَّدۡعِۙ‏"
        },
        {
            "id": 5944,
            "verse_key": "86:13",
            "text": "اِنَّهٗ لَقَوۡلٌ فَصۡلٌۙ‏"
        },
        {
            "id": 5945,
            "verse_key": "86:14",
            "text": "وَّمَا هُوَ بِالۡهَزۡلِؕ‏"
        },
        {
            "id": 5946,
            "verse_key": "86:15",
            "text": "اِنَّهُمۡ يَكِيۡدُوۡنَ كَيۡدًا ۙ‏"
        },
        {
            "id": 5947,
            "verse_key": "86:16",
            "text": "وَّاَكِيۡدُ كَيۡدًا ۚۖ‏"
        },
        {
            "id": 5948,
            "verse_key": "86:17",
            "text": "فَمَهِّلِ الۡكٰفِرِيۡنَ اَمۡهِلۡهُمۡ رُوَيۡدًا‏"
        },
        {
            "id": 5949,
            "verse_key": "87:1",
            "text": "سَبِّحِ اسۡمَ رَبِّكَ الۡاَعۡلَىۙ‏"
        },
        {
            "id": 5950,
            "verse_key": "87:2",
            "text": "الَّذِىۡ خَلَقَ فَسَوّٰى ۙ‏"
        },
        {
            "id": 5951,
            "verse_key": "87:3",
            "text": "وَالَّذِىۡ قَدَّرَ فَهَدٰى ۙ‏"
        },
        {
            "id": 5952,
            "verse_key": "87:4",
            "text": "وَالَّذِىۡۤ اَخۡرَجَ الۡمَرۡعٰى ۙ‏"
        },
        {
            "id": 5953,
            "verse_key": "87:5",
            "text": "فَجَعَلَهٗ غُثَآءً اَحۡوٰىؕ‏"
        },
        {
            "id": 5954,
            "verse_key": "87:6",
            "text": "سَنُقۡرِئُكَ فَلَا تَنۡسٰٓىۙ‏"
        },
        {
            "id": 5955,
            "verse_key": "87:7",
            "text": "اِلَّا مَا شَآءَ اللّٰهُ​ؕ اِنَّهٗ يَعۡلَمُ الۡجَهۡرَ وَمَا يَخۡفٰىؕ‏"
        },
        {
            "id": 5956,
            "verse_key": "87:8",
            "text": "وَنُيَسِّرُكَ لِلۡيُسۡرٰى ​ۖ​ۚ‏"
        },
        {
            "id": 5957,
            "verse_key": "87:9",
            "text": "فَذَكِّرۡ اِنۡ نَّفَعَتِ الذِّكۡرٰىؕ‏"
        },
        {
            "id": 5958,
            "verse_key": "87:10",
            "text": "سَيَذَّكَّرُ مَنۡ يَّخۡشٰىۙ‏"
        },
        {
            "id": 5959,
            "verse_key": "87:11",
            "text": "وَيَتَجَنَّبُهَا الۡاَشۡقَىۙ‏"
        },
        {
            "id": 5960,
            "verse_key": "87:12",
            "text": "الَّذِىۡ يَصۡلَى النَّارَ الۡكُبۡرٰى​ۚ‏"
        },
        {
            "id": 5961,
            "verse_key": "87:13",
            "text": "ثُمَّ لَا يَمُوۡتُ فِيۡهَا وَلَا يَحۡيٰىؕ‏"
        },
        {
            "id": 5962,
            "verse_key": "87:14",
            "text": "قَدۡ اَفۡلَحَ مَنۡ تَزَكّٰىۙ‏"
        },
        {
            "id": 5963,
            "verse_key": "87:15",
            "text": "وَذَكَرَ اسۡمَ رَبِّهٖ فَصَلّٰى​ ؕ‏"
        },
        {
            "id": 5964,
            "verse_key": "87:16",
            "text": "بَلۡ تُؤۡثِرُوۡنَ الۡحَيٰوةَ الدُّنۡيَا ۖ‏"
        },
        {
            "id": 5965,
            "verse_key": "87:17",
            "text": "وَالۡاٰخِرَةُ خَيۡرٌ وَّ اَبۡقٰىؕ‏"
        },
        {
            "id": 5966,
            "verse_key": "87:18",
            "text": "اِنَّ هٰذَا لَفِى الصُّحُفِ الۡاُوۡلٰىۙ‏"
        },
        {
            "id": 5967,
            "verse_key": "87:19",
            "text": "صُحُفِ اِبۡرٰهِيۡمَ وَمُوۡسٰى‏"
        },
        {
            "id": 5968,
            "verse_key": "88:1",
            "text": "هَلۡ اَتٰٮكَ حَدِيۡثُ الۡغَاشِيَةِؕ‏"
        },
        {
            "id": 5969,
            "verse_key": "88:2",
            "text": "وُجُوۡهٌ يَّوۡمَٮِٕذٍ خَاشِعَةٌ ۙ‏"
        },
        {
            "id": 5970,
            "verse_key": "88:3",
            "text": "عَامِلَةٌ نَّاصِبَةٌ ۙ‏"
        },
        {
            "id": 5971,
            "verse_key": "88:4",
            "text": "تَصۡلٰى نَارًا حَامِيَةً ۙ‏"
        },
        {
            "id": 5972,
            "verse_key": "88:5",
            "text": "تُسۡقٰى مِنۡ عَيۡنٍ اٰنِيَةٍؕ‏"
        },
        {
            "id": 5973,
            "verse_key": "88:6",
            "text": "لَـيۡسَ لَهُمۡ طَعَامٌ اِلَّا مِنۡ ضَرِيۡعٍۙ‏"
        },
        {
            "id": 5974,
            "verse_key": "88:7",
            "text": "لَّا يُسۡمِنُ وَلَا يُغۡنِىۡ مِنۡ جُوۡعٍؕ‏"
        },
        {
            "id": 5975,
            "verse_key": "88:8",
            "text": "وُجُوۡهٌ يَّوۡمَٮِٕذٍ نَّاعِمَةٌ ۙ‏"
        },
        {
            "id": 5976,
            "verse_key": "88:9",
            "text": "لِّسَعۡيِهَا رَاضِيَةٌ ۙ‏"
        },
        {
            "id": 5977,
            "verse_key": "88:10",
            "text": "فِىۡ جَنَّةٍ عَالِيَةٍۙ‏"
        },
        {
            "id": 5978,
            "verse_key": "88:11",
            "text": "لَّا تَسۡمَعُ فِيۡهَا لَاغِيَةً ؕ‏"
        },
        {
            "id": 5979,
            "verse_key": "88:12",
            "text": "فِيۡهَا عَيۡنٌ جَارِيَةٌ​ ۘ‏"
        },
        {
            "id": 5980,
            "verse_key": "88:13",
            "text": "فِيۡهَا سُرُرٌ مَّرۡفُوۡعَةٌ ۙ‏"
        },
        {
            "id": 5981,
            "verse_key": "88:14",
            "text": "وَّاَكۡوَابٌ مَّوۡضُوۡعَةٌ ۙ‏"
        },
        {
            "id": 5982,
            "verse_key": "88:15",
            "text": "وَّنَمَارِقُ مَصۡفُوۡفَةٌ ۙ‏"
        },
        {
            "id": 5983,
            "verse_key": "88:16",
            "text": "وَّزَرَابِىُّ مَبۡثُوۡثَةٌ ؕ‏"
        },
        {
            "id": 5984,
            "verse_key": "88:17",
            "text": "اَفَلَا يَنۡظُرُوۡنَ اِلَى الۡاِ بِلِ كَيۡفَ خُلِقَتۡ‏"
        },
        {
            "id": 5985,
            "verse_key": "88:18",
            "text": "وَاِلَى السَّمَآءِ كَيۡفَ رُفِعَتۡ‏"
        },
        {
            "id": 5986,
            "verse_key": "88:19",
            "text": "وَاِلَى الۡجِبَالِ كَيۡفَ نُصِبَتۡ‏"
        },
        {
            "id": 5987,
            "verse_key": "88:20",
            "text": "وَاِلَى الۡاَرۡضِ كَيۡفَ سُطِحَتۡ‏"
        },
        {
            "id": 5988,
            "verse_key": "88:21",
            "text": "فَذَكِّرۡ ؕ اِنَّمَاۤ اَنۡتَ مُذَكِّرٌ ؕ‏"
        },
        {
            "id": 5989,
            "verse_key": "88:22",
            "text": "لَـسۡتَ عَلَيۡهِمۡ بِمُصَۜيۡطِرٍۙ‏"
        },
        {
            "id": 5990,
            "verse_key": "88:23",
            "text": "اِلَّا مَنۡ تَوَلّٰى وَكَفَرَۙ‏"
        },
        {
            "id": 5991,
            "verse_key": "88:24",
            "text": "فَيُعَذِّبُهُ اللّٰهُ الۡعَذَابَ الۡاَكۡبَرَؕ‏"
        },
        {
            "id": 5992,
            "verse_key": "88:25",
            "text": "اِنَّ اِلَيۡنَاۤ اِيَابَهُمۡۙ‏"
        },
        {
            "id": 5993,
            "verse_key": "88:26",
            "text": "ثُمَّ اِنَّ عَلَيۡنَا حِسَابَهُمْ‏"
        },
        {
            "id": 5994,
            "verse_key": "89:1",
            "text": "وَالۡفَجۡرِۙ‏"
        },
        {
            "id": 5995,
            "verse_key": "89:2",
            "text": "وَلَيَالٍ عَشۡرٍۙ‏"
        },
        {
            "id": 5996,
            "verse_key": "89:3",
            "text": "وَّالشَّفۡعِ وَالۡوَتۡرِۙ‏"
        },
        {
            "id": 5997,
            "verse_key": "89:4",
            "text": "وَالَّيۡلِ اِذَا يَسۡرِ​ۚ‏"
        },
        {
            "id": 5998,
            "verse_key": "89:5",
            "text": "هَلۡ فِىۡ ذٰلِكَ قَسَمٌ لِّذِىۡ حِجۡرٍؕ‏"
        },
        {
            "id": 5999,
            "verse_key": "89:6",
            "text": "اَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍۙ‏"
        },
        {
            "id": 6000,
            "verse_key": "89:7",
            "text": "اِرَمَ ذَاتِ الۡعِمَادِۙ‏"
        },
        {
            "id": 6001,
            "verse_key": "89:8",
            "text": "الَّتِىۡ لَمۡ يُخۡلَقۡ مِثۡلُهَا فِى الۡبِلَادِۙ‏"
        },
        {
            "id": 6002,
            "verse_key": "89:9",
            "text": "وَثَمُوۡدَ الَّذِيۡنَ جَابُوا الصَّخۡرَ بِالۡوَادِۙ‏"
        },
        {
            "id": 6003,
            "verse_key": "89:10",
            "text": "وَفِرۡعَوۡنَ ذِى الۡاَوۡتَادِۙ‏"
        },
        {
            "id": 6004,
            "verse_key": "89:11",
            "text": "الَّذِيۡنَ طَغَوۡا فِى الۡبِلَادِۙ‏"
        },
        {
            "id": 6005,
            "verse_key": "89:12",
            "text": "فَاَكۡثَرُوۡا فِيۡهَا الۡفَسَادَۙ‏"
        },
        {
            "id": 6006,
            "verse_key": "89:13",
            "text": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ ۙۚ‏"
        },
        {
            "id": 6007,
            "verse_key": "89:14",
            "text": "اِنَّ رَبَّكَ لَبِالۡمِرۡصَادِؕ‏"
        },
        {
            "id": 6008,
            "verse_key": "89:15",
            "text": "فَاَمَّا الۡاِنۡسَانُ اِذَا مَا ابۡتَلٰٮهُ رَبُّهٗ فَاَكۡرَمَهٗ وَنَعَّمَهٗ  ۙ فَيَقُوۡلُ رَبِّىۡۤ اَكۡرَمَنِؕ‏"
        },
        {
            "id": 6009,
            "verse_key": "89:16",
            "text": "وَاَمَّاۤ اِذَا مَا ابۡتَلٰٮهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهٗ ۙ فَيَقُوۡلُ رَبِّىۡۤ اَهَانَنِ​ۚ‏"
        },
        {
            "id": 6010,
            "verse_key": "89:17",
            "text": "كَلَّا​ بَلۡ لَّا تُكۡرِمُوۡنَ الۡيَتِيۡمَۙ‏"
        },
        {
            "id": 6011,
            "verse_key": "89:18",
            "text": "وَلَا تَحٰٓضُّوۡنَ عَلٰى طَعَامِ الۡمِسۡكِيۡنِۙ‏"
        },
        {
            "id": 6012,
            "verse_key": "89:19",
            "text": "وَتَاۡكُلُوۡنَ التُّرَاثَ اَكۡلًا لَّـمًّا ۙ‏"
        },
        {
            "id": 6013,
            "verse_key": "89:20",
            "text": "وَّتُحِبُّوۡنَ الۡمَالَ حُبًّا جَمًّا ؕ‏"
        },
        {
            "id": 6014,
            "verse_key": "89:21",
            "text": "كَلَّاۤ اِذَا دُكَّتِ الۡاَرۡضُ دَكًّا دَكًّا ۙ‏"
        },
        {
            "id": 6015,
            "verse_key": "89:22",
            "text": "وَّجَآءَ رَبُّكَ وَالۡمَلَكُ صَفًّا صَفًّا ۚ‏"
        },
        {
            "id": 6016,
            "verse_key": "89:23",
            "text": "وَجِاىْٓءَ يَوۡمَٮِٕذٍۢ بِجَهَنَّمَ  ۙ​ يَوۡمَٮِٕذٍ يَّتَذَكَّرُ الۡاِنۡسَانُ وَاَنّٰى لَـهُ الذِّكۡرٰىؕ‏"
        },
        {
            "id": 6017,
            "verse_key": "89:24",
            "text": "يَقُوۡلُ يٰلَيۡتَنِىۡ قَدَّمۡتُ لِحَـيَاتِى​ۚ‏"
        },
        {
            "id": 6018,
            "verse_key": "89:25",
            "text": "فَيَوۡمَٮِٕذٍ لَّا يُعَذِّبُ عَذَابَهٗۤ اَحَدٌ ۙ‏"
        },
        {
            "id": 6019,
            "verse_key": "89:26",
            "text": "وَّلَا يُوۡثِقُ وَثَاقَهٗۤ اَحَدٌ ؕ‏"
        },
        {
            "id": 6020,
            "verse_key": "89:27",
            "text": "يٰۤاَيَّتُهَا النَّفۡسُ الۡمُطۡمَٮِٕنَّةُ  ۖ‏"
        },
        {
            "id": 6021,
            "verse_key": "89:28",
            "text": "ارۡجِعِىۡۤ اِلٰى رَبِّكِ رَاضِيَةً مَّرۡضِيَّةً​ ۚ‏"
        },
        {
            "id": 6022,
            "verse_key": "89:29",
            "text": "فَادۡخُلِىۡ فِىۡ عِبٰدِىۙ‏"
        },
        {
            "id": 6023,
            "verse_key": "89:30",
            "text": "وَادۡخُلِىۡ جَنَّتِى‏"
        },
        {
            "id": 6024,
            "verse_key": "90:1",
            "text": "لَاۤ اُقۡسِمُ بِهٰذَا الۡبَلَدِۙ‏"
        },
        {
            "id": 6025,
            "verse_key": "90:2",
            "text": "وَاَنۡتَ حِلٌّ ۢ بِهٰذَا الۡبَلَدِۙ‏"
        },
        {
            "id": 6026,
            "verse_key": "90:3",
            "text": "وَوَالِدٍ وَّمَا وَلَدَ ۙ‏"
        },
        {
            "id": 6027,
            "verse_key": "90:4",
            "text": "لَقَدۡ خَلَقۡنَا الۡاِنۡسَانَ فِىۡ كَبَدٍؕ‏"
        },
        {
            "id": 6028,
            "verse_key": "90:5",
            "text": "اَيَحۡسَبُ اَنۡ لَّنۡ يَّقۡدِرَ عَلَيۡهِ اَحَدٌ​ ۘ‏"
        },
        {
            "id": 6029,
            "verse_key": "90:6",
            "text": "يَقُوۡلُ اَهۡلَكۡتُ مَالًا لُّبَدًا ؕ‏"
        },
        {
            "id": 6030,
            "verse_key": "90:7",
            "text": "اَيَحۡسَبُ اَنۡ لَّمۡ يَرَهٗۤ اَحَدٌ ؕ‏"
        },
        {
            "id": 6031,
            "verse_key": "90:8",
            "text": "اَلَمۡ نَجۡعَلۡ لَّهٗ عَيۡنَيۡنِۙ‏"
        },
        {
            "id": 6032,
            "verse_key": "90:9",
            "text": "وَلِسَانًا وَّشَفَتَيۡنِۙ‏"
        },
        {
            "id": 6033,
            "verse_key": "90:10",
            "text": "وَهَدَيۡنٰهُ النَّجۡدَيۡنِ​ۚ‏"
        },
        {
            "id": 6034,
            "verse_key": "90:11",
            "text": "فَلَا اقۡتَحَمَ الۡعَقَبَةَ ۖ‏"
        },
        {
            "id": 6035,
            "verse_key": "90:12",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا الۡعَقَبَةُ ؕ‏"
        },
        {
            "id": 6036,
            "verse_key": "90:13",
            "text": "فَكُّ رَقَبَةٍ ۙ‏"
        },
        {
            "id": 6037,
            "verse_key": "90:14",
            "text": "اَوۡ اِطۡعٰمٌ فِىۡ يَوۡمٍ ذِىۡ مَسۡغَبَةٍ ۙ‏"
        },
        {
            "id": 6038,
            "verse_key": "90:15",
            "text": "يَّتِيۡمًا ذَا مَقۡرَبَةٍ ۙ‏"
        },
        {
            "id": 6039,
            "verse_key": "90:16",
            "text": "اَوۡ مِسۡكِيۡنًا ذَا مَتۡرَبَةٍ ؕ‏"
        },
        {
            "id": 6040,
            "verse_key": "90:17",
            "text": "ثُمَّ كَانَ مِنَ الَّذِيۡنَ اٰمَنُوۡا وَتَوَاصَوۡا بِالصَّبۡرِ وَتَوَاصَوۡا بِالۡمَرۡحَمَةِ ؕ‏"
        },
        {
            "id": 6041,
            "verse_key": "90:18",
            "text": "اُولٰٓٮِٕكَ اَصۡحٰبُ الۡمَيۡمَنَةِ ؕ‏"
        },
        {
            "id": 6042,
            "verse_key": "90:19",
            "text": "وَالَّذِيۡنَ كَفَرُوۡا بِاٰيٰتِنَا هُمۡ اَصۡحٰبُ الۡمَشۡـَٔـمَةِ ؕ‏"
        },
        {
            "id": 6043,
            "verse_key": "90:20",
            "text": "عَلَيۡهِمۡ نَارٌ مُّؤۡصَدَةٌ‏"
        },
        {
            "id": 6044,
            "verse_key": "91:1",
            "text": "وَالشَّمۡسِ وَضُحٰٮهَا  ۙ‏"
        },
        {
            "id": 6045,
            "verse_key": "91:2",
            "text": "وَالۡقَمَرِ اِذَا تَلٰٮهَا  ۙ‏"
        },
        {
            "id": 6046,
            "verse_key": "91:3",
            "text": "وَالنَّهَارِ اِذَا جَلّٰٮهَا ۙ‏"
        },
        {
            "id": 6047,
            "verse_key": "91:4",
            "text": "وَالَّيۡلِ اِذَا يَغۡشٰٮهَا ۙ‏"
        },
        {
            "id": 6048,
            "verse_key": "91:5",
            "text": "وَالسَّمَآءِ وَمَا بَنٰٮهَا ۙ‏"
        },
        {
            "id": 6049,
            "verse_key": "91:6",
            "text": "وَالۡاَرۡضِ وَمَا طَحٰٮهَا ۙ‏"
        },
        {
            "id": 6050,
            "verse_key": "91:7",
            "text": "وَنَفۡسٍ وَّمَا سَوّٰٮهَا ۙ‏"
        },
        {
            "id": 6051,
            "verse_key": "91:8",
            "text": "فَاَلۡهَمَهَا فُجُوۡرَهَا وَتَقۡوٰٮهَا ۙ‏"
        },
        {
            "id": 6052,
            "verse_key": "91:9",
            "text": "قَدۡ اَفۡلَحَ مَنۡ زَكّٰٮهَا ۙ‏"
        },
        {
            "id": 6053,
            "verse_key": "91:10",
            "text": "وَقَدۡ خَابَ مَنۡ دَسّٰٮهَا ؕ‏"
        },
        {
            "id": 6054,
            "verse_key": "91:11",
            "text": "كَذَّبَتۡ ثَمُوۡدُ بِطَغۡوٰٮهَآ  ۙ‏"
        },
        {
            "id": 6055,
            "verse_key": "91:12",
            "text": "اِذِ انۡۢبَعَثَ اَشۡقٰٮهَا  ۙ‏"
        },
        {
            "id": 6056,
            "verse_key": "91:13",
            "text": "فَقَالَ لَهُمۡ رَسُوۡلُ اللّٰهِ نَاقَةَ اللّٰهِ وَسُقۡيٰهَا ؕ‏"
        },
        {
            "id": 6057,
            "verse_key": "91:14",
            "text": "فَكَذَّبُوۡهُ فَعَقَرُوۡهَا   ۙفَدَمۡدَمَ عَلَيۡهِمۡ رَبُّهُمۡ بِذَنۡۢبِهِمۡ فَسَوّٰٮهَا  ۙ‏"
        },
        {
            "id": 6058,
            "verse_key": "91:15",
            "text": "وَلَا يَخَافُ عُقۡبٰهَا‏"
        },
        {
            "id": 6059,
            "verse_key": "92:1",
            "text": "وَالَّيۡلِ اِذَا يَغۡشٰىۙ‏"
        },
        {
            "id": 6060,
            "verse_key": "92:2",
            "text": "وَالنَّهَارِ اِذَا تَجَلّٰىۙ‏"
        },
        {
            "id": 6061,
            "verse_key": "92:3",
            "text": "وَمَا خَلَقَ الذَّكَرَ وَالۡاُنۡثٰٓىۙ‏"
        },
        {
            "id": 6062,
            "verse_key": "92:4",
            "text": "اِنَّ سَعۡيَكُمۡ لَشَتّٰىؕ‏"
        },
        {
            "id": 6063,
            "verse_key": "92:5",
            "text": "فَاَمَّا مَنۡ اَعۡطٰى وَاتَّقٰىۙ‏"
        },
        {
            "id": 6064,
            "verse_key": "92:6",
            "text": "وَصَدَّقَ بِالۡحُسۡنٰىۙ‏"
        },
        {
            "id": 6065,
            "verse_key": "92:7",
            "text": "فَسَنُيَسِّرُهٗ لِلۡيُسۡرٰىؕ‏"
        },
        {
            "id": 6066,
            "verse_key": "92:8",
            "text": "وَاَمَّا مَنۡۢ بَخِلَ وَاسۡتَغۡنٰىۙ‏"
        },
        {
            "id": 6067,
            "verse_key": "92:9",
            "text": "وَكَذَّبَ بِالۡحُسۡنٰىۙ‏"
        },
        {
            "id": 6068,
            "verse_key": "92:10",
            "text": "فَسَنُيَسِّرُهٗ لِلۡعُسۡرٰىؕ‏"
        },
        {
            "id": 6069,
            "verse_key": "92:11",
            "text": "وَمَا يُغۡنِىۡ عَنۡهُ مَالُهٗۤ اِذَا تَرَدّٰىؕ‏"
        },
        {
            "id": 6070,
            "verse_key": "92:12",
            "text": "اِنَّ عَلَيۡنَا لَـلۡهُدٰىۖ‏"
        },
        {
            "id": 6071,
            "verse_key": "92:13",
            "text": "وَاِنَّ لَـنَا لَـلۡاٰخِرَةَ وَالۡاُوۡلٰى‏"
        },
        {
            "id": 6072,
            "verse_key": "92:14",
            "text": "فَاَنۡذَرۡتُكُمۡ نَارًا تَلَظّٰى​ۚ‏"
        },
        {
            "id": 6073,
            "verse_key": "92:15",
            "text": "لَا يَصۡلٰٮهَاۤ اِلَّا الۡاَشۡقَىۙ‏"
        },
        {
            "id": 6074,
            "verse_key": "92:16",
            "text": "الَّذِىۡ كَذَّبَ وَتَوَلّٰىؕ‏"
        },
        {
            "id": 6075,
            "verse_key": "92:17",
            "text": "وَسَيُجَنَّبُهَا الۡاَتۡقَىۙ‏"
        },
        {
            "id": 6076,
            "verse_key": "92:18",
            "text": "الَّذِىۡ يُؤۡتِىۡ مَالَهٗ يَتَزَكّٰى​ۚ‏"
        },
        {
            "id": 6077,
            "verse_key": "92:19",
            "text": "وَمَا لِاَحَدٍ عِنۡدَهٗ مِنۡ نِّعۡمَةٍ تُجۡزٰٓىۙ‏"
        },
        {
            "id": 6078,
            "verse_key": "92:20",
            "text": "اِلَّا ابۡتِغَآءَ وَجۡهِ رَبِّهِ الۡاَعۡلٰى​ۚ‏"
        },
        {
            "id": 6079,
            "verse_key": "92:21",
            "text": "وَلَسَوۡفَ يَرۡضٰى‏"
        },
        {
            "id": 6080,
            "verse_key": "93:1",
            "text": "وَالضُّحٰىۙ‏"
        },
        {
            "id": 6081,
            "verse_key": "93:2",
            "text": "وَالَّيۡلِ اِذَا سَجٰىۙ‏"
        },
        {
            "id": 6082,
            "verse_key": "93:3",
            "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلٰىؕ‏"
        },
        {
            "id": 6083,
            "verse_key": "93:4",
            "text": "وَلَـلۡاٰخِرَةُ خَيۡرٌ لَّكَ مِنَ الۡاُوۡلٰىؕ‏"
        },
        {
            "id": 6084,
            "verse_key": "93:5",
            "text": "وَلَسَوۡفَ يُعۡطِيۡكَ رَبُّكَ فَتَرۡضٰىؕ‏"
        },
        {
            "id": 6085,
            "verse_key": "93:6",
            "text": "اَلَمۡ يَجِدۡكَ يَتِيۡمًا فَاٰوٰى‏"
        },
        {
            "id": 6086,
            "verse_key": "93:7",
            "text": "وَوَجَدَكَ ضَآ لًّا فَهَدٰى‏"
        },
        {
            "id": 6087,
            "verse_key": "93:8",
            "text": "وَوَجَدَكَ عَآٮِٕلًا فَاَغۡنٰىؕ‏"
        },
        {
            "id": 6088,
            "verse_key": "93:9",
            "text": "فَاَمَّا الۡيَتِيۡمَ فَلَا تَقۡهَرۡؕ‏"
        },
        {
            "id": 6089,
            "verse_key": "93:10",
            "text": "وَاَمَّا السَّآٮِٕلَ فَلَا تَنۡهَرۡؕ‏"
        },
        {
            "id": 6090,
            "verse_key": "93:11",
            "text": "وَاَمَّا بِنِعۡمَةِ رَبِّكَ فَحَدِّثۡ‏"
        },
        {
            "id": 6091,
            "verse_key": "94:1",
            "text": "اَلَمۡ نَشۡرَحۡ لَـكَ صَدۡرَكَۙ‏"
        },
        {
            "id": 6092,
            "verse_key": "94:2",
            "text": "وَوَضَعۡنَا عَنۡكَ وِزۡرَكَۙ‏"
        },
        {
            "id": 6093,
            "verse_key": "94:3",
            "text": "الَّذِىۡۤ اَنۡقَضَ ظَهۡرَكَۙ‏"
        },
        {
            "id": 6094,
            "verse_key": "94:4",
            "text": "وَرَفَعۡنَا لَـكَ ذِكۡرَكَؕ‏"
        },
        {
            "id": 6095,
            "verse_key": "94:5",
            "text": "فَاِنَّ مَعَ الۡعُسۡرِ يُسۡرًا ۙ‏"
        },
        {
            "id": 6096,
            "verse_key": "94:6",
            "text": "اِنَّ مَعَ الۡعُسۡرِ يُسۡرًا ؕ‏"
        },
        {
            "id": 6097,
            "verse_key": "94:7",
            "text": "فَاِذَا فَرَغۡتَ فَانۡصَبۡۙ‏"
        },
        {
            "id": 6098,
            "verse_key": "94:8",
            "text": "وَاِلٰى رَبِّكَ فَارۡغَب‏"
        },
        {
            "id": 6099,
            "verse_key": "95:1",
            "text": "وَالتِّيۡنِ وَالزَّيۡتُوۡنِۙ‏"
        },
        {
            "id": 6100,
            "verse_key": "95:2",
            "text": "وَطُوۡرِ سِيۡنِيۡنَۙ‏"
        },
        {
            "id": 6101,
            "verse_key": "95:3",
            "text": "وَهٰذَا الۡبَلَدِ الۡاَمِيۡنِۙ‏"
        },
        {
            "id": 6102,
            "verse_key": "95:4",
            "text": "لَقَدۡ خَلَقۡنَا الۡاِنۡسَانَ فِىۡۤ اَحۡسَنِ تَقۡوِيۡمٍ‏"
        },
        {
            "id": 6103,
            "verse_key": "95:5",
            "text": "ثُمَّ رَدَدۡنٰهُ اَسۡفَلَ سَافِلِيۡنَۙ‏"
        },
        {
            "id": 6104,
            "verse_key": "95:6",
            "text": "اِلَّا الَّذِيۡنَ اٰمَنُوۡا وَعَمِلُوا الصّٰلِحٰتِ فَلَهُمۡ اَجۡرٌ غَيۡرُ مَمۡنُوۡنٍؕ‏"
        },
        {
            "id": 6105,
            "verse_key": "95:7",
            "text": "فَمَا يُكَذِّبُكَ بَعۡدُ بِالدِّيۡنِ‏"
        },
        {
            "id": 6106,
            "verse_key": "95:8",
            "text": "اَلَيۡسَ اللّٰهُ بِاَحۡكَمِ الۡحٰكِمِيۡنَ‏"
        },
        {
            "id": 6107,
            "verse_key": "96:1",
            "text": "اِقۡرَاۡ بِاسۡمِ رَبِّكَ الَّذِىۡ خَلَقَ​ۚ‏"
        },
        {
            "id": 6108,
            "verse_key": "96:2",
            "text": "خَلَقَ الۡاِنۡسَانَ مِنۡ عَلَقٍ​ۚ‏"
        },
        {
            "id": 6109,
            "verse_key": "96:3",
            "text": "اِقۡرَاۡ وَرَبُّكَ الۡاَكۡرَمُۙ‏"
        },
        {
            "id": 6110,
            "verse_key": "96:4",
            "text": "الَّذِىۡ عَلَّمَ بِالۡقَلَمِۙ‏"
        },
        {
            "id": 6111,
            "verse_key": "96:5",
            "text": "عَلَّمَ الۡاِنۡسَانَ مَا لَمۡ يَعۡلَمۡؕ‏"
        },
        {
            "id": 6112,
            "verse_key": "96:6",
            "text": "كَلَّاۤ اِنَّ الۡاِنۡسَانَ لَيَطۡغٰٓىۙ‏"
        },
        {
            "id": 6113,
            "verse_key": "96:7",
            "text": "اَنۡ رَّاٰهُ اسۡتَغۡنٰىؕ‏"
        },
        {
            "id": 6114,
            "verse_key": "96:8",
            "text": "اِنَّ اِلٰى رَبِّكَ الرُّجۡعٰىؕ‏"
        },
        {
            "id": 6115,
            "verse_key": "96:9",
            "text": "اَرَءَيۡتَ الَّذِىۡ يَنۡهٰىؕ‏"
        },
        {
            "id": 6116,
            "verse_key": "96:10",
            "text": "عَبۡدًا اِذَا صَلّٰىؕ‏"
        },
        {
            "id": 6117,
            "verse_key": "96:11",
            "text": "اَرَءَيۡتَ اِنۡ كَانَ عَلَى الۡهُدٰٓىۙ‏"
        },
        {
            "id": 6118,
            "verse_key": "96:12",
            "text": "اَوۡ اَمَرَ بِالتَّقۡوٰىۙ‏"
        },
        {
            "id": 6119,
            "verse_key": "96:13",
            "text": "اَرَءَيۡتَ اِنۡ كَذَّبَ وَتَوَلّٰىؕ‏"
        },
        {
            "id": 6120,
            "verse_key": "96:14",
            "text": "اَلَمۡ يَعۡلَمۡ بِاَنَّ اللّٰهَ يَرٰىؕ‏"
        },
        {
            "id": 6121,
            "verse_key": "96:15",
            "text": "كَلَّا لَٮِٕنۡ لَّمۡ يَنۡتَهِ  ۙ لَنَسۡفَعًۢا بِالنَّاصِيَةِۙ‏"
        },
        {
            "id": 6122,
            "verse_key": "96:16",
            "text": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ​ ۚ‏"
        },
        {
            "id": 6123,
            "verse_key": "96:17",
            "text": "فَلۡيَدۡعُ نَادِيَهٗ ۙ‏"
        },
        {
            "id": 6124,
            "verse_key": "96:18",
            "text": "سَنَدۡعُ الزَّبَانِيَةَ ۙ‏"
        },
        {
            "id": 6125,
            "verse_key": "96:19",
            "text": "كَلَّا ؕ لَا تُطِعۡهُ وَاسۡجُدۡ وَاقۡتَرِبْ۩‏"
        },
        {
            "id": 6126,
            "verse_key": "97:1",
            "text": "اِنَّاۤ اَنۡزَلۡنٰهُ فِىۡ لَيۡلَةِ الۡقَدۡرِ ۖ ۚ‏"
        },
        {
            "id": 6127,
            "verse_key": "97:2",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا لَيۡلَةُ الۡقَدۡرِؕ‏"
        },
        {
            "id": 6128,
            "verse_key": "97:3",
            "text": "لَيۡلَةُ الۡقَدۡرِ  ۙ خَيۡرٌ مِّنۡ اَلۡفِ شَهۡرٍؕ‏"
        },
        {
            "id": 6129,
            "verse_key": "97:4",
            "text": "تَنَزَّلُ الۡمَلٰٓٮِٕكَةُ وَالرُّوۡحُ فِيۡهَا بِاِذۡنِ رَبِّهِمۡ​ۚ مِّنۡ كُلِّ اَمۡرٍ ۛۙ‏"
        },
        {
            "id": 6130,
            "verse_key": "97:5",
            "text": "سَلٰمٌ   ۛهِىَ حَتّٰى مَطۡلَعِ الۡفَجۡرِ‏"
        },
        {
            "id": 6131,
            "verse_key": "98:1",
            "text": "لَمۡ يَكُنِ الَّذِيۡنَ كَفَرُوۡا مِنۡ اَهۡلِ الۡكِتٰبِ وَالۡمُشۡرِكِيۡنَ مُنۡفَكِّيۡنَ حَتّٰى تَاۡتِيَهُمُ الۡبَيِّنَةُ ۙ‏"
        },
        {
            "id": 6132,
            "verse_key": "98:2",
            "text": "رَسُوۡلٌ مِّنَ اللّٰهِ يَتۡلُوۡا صُحُفًا مُّطَهَّرَةً ۙ‏"
        },
        {
            "id": 6133,
            "verse_key": "98:3",
            "text": "فِيۡهَا كُتُبٌ قَيِّمَةٌ ؕ‏"
        },
        {
            "id": 6134,
            "verse_key": "98:4",
            "text": "وَمَا تَفَرَّقَ الَّذِيۡنَ اُوۡتُوا الۡكِتٰبَ اِلَّا مِنۡۢ بَعۡدِ مَا جَآءَتۡهُمُ الۡبَيِّنَةُ ؕ‏"
        },
        {
            "id": 6135,
            "verse_key": "98:5",
            "text": "وَمَاۤ اُمِرُوۡۤا اِلَّا لِيَعۡبُدُوا اللّٰهَ مُخۡلِصِيۡنَ لَـهُ الدِّيۡنَ  ۙ حُنَفَآءَ وَيُقِيۡمُوا الصَّلٰوةَ وَيُؤۡتُوا الزَّكٰوةَ​ وَذٰلِكَ دِيۡنُ الۡقَيِّمَةِ ؕ‏"
        },
        {
            "id": 6136,
            "verse_key": "98:6",
            "text": "اِنَّ الَّذِيۡنَ كَفَرُوۡا مِنۡ اَهۡلِ الۡكِتٰبِ وَ الۡمُشۡرِكِيۡنَ فِىۡ نَارِ جَهَنَّمَ خٰلِدِيۡنَ فِيۡهَا ​ؕ اُولٰٓٮِٕكَ هُمۡ شَرُّ الۡبَرِيَّةِ ؕ‏"
        },
        {
            "id": 6137,
            "verse_key": "98:7",
            "text": "اِنَّ الَّذِيۡنَ اٰمَنُوۡا وَعَمِلُوا الصّٰلِحٰتِۙ اُولٰٓٮِٕكَ هُمۡ خَيۡرُ الۡبَرِيَّةِ ؕ‏"
        },
        {
            "id": 6138,
            "verse_key": "98:8",
            "text": "جَزَآؤُهُمۡ عِنۡدَ رَبِّهِمۡ جَنّٰتُ عَدۡنٍ تَجۡرِىۡ مِنۡ تَحۡتِهَا الۡاَنۡهٰرُ خٰلِدِيۡنَ فِيۡهَاۤ اَبَدًا ​ؕ رَضِىَ اللّٰهُ عَنۡهُمۡ وَرَضُوۡا عَنۡهُ ​ؕ ذٰلِكَ لِمَنۡ خَشِىَ رَبَّهٗ‏"
        },
        {
            "id": 6139,
            "verse_key": "99:1",
            "text": "اِذَا زُلۡزِلَتِ الۡاَرۡضُ زِلۡزَالَهَا ۙ‏"
        },
        {
            "id": 6140,
            "verse_key": "99:2",
            "text": "وَاَخۡرَجَتِ الۡاَرۡضُ اَثۡقَالَهَا ۙ‏"
        },
        {
            "id": 6141,
            "verse_key": "99:3",
            "text": "وَقَالَ الۡاِنۡسَانُ مَا لَهَا​ ۚ‏"
        },
        {
            "id": 6142,
            "verse_key": "99:4",
            "text": "يَوۡمَٮِٕذٍ تُحَدِّثُ اَخۡبَارَهَا ۙ‏"
        },
        {
            "id": 6143,
            "verse_key": "99:5",
            "text": "بِاَنَّ رَبَّكَ اَوۡحٰى لَهَا ؕ‏"
        },
        {
            "id": 6144,
            "verse_key": "99:6",
            "text": "يَوۡمَٮِٕذٍ يَّصۡدُرُ النَّاسُ اَشۡتَاتًا  ۙ لِّيُرَوۡا اَعۡمَالَهُمۡؕ‏ ﻿﻿"
        },
        {
            "id": 6145,
            "verse_key": "99:7",
            "text": "فَمَنۡ يَّعۡمَلۡ مِثۡقَالَ ذَرَّةٍ خَيۡرًا يَّرَهٗ ؕ‏"
        },
        {
            "id": 6146,
            "verse_key": "99:8",
            "text": "وَمَنۡ يَّعۡمَلۡ مِثۡقَالَ ذَرَّةٍ شَرًّا يَّرَهٗ‏"
        },
        {
            "id": 6147,
            "verse_key": "100:1",
            "text": "وَالۡعٰدِيٰتِ ضَبۡحًا ۙ‏"
        },
        {
            "id": 6148,
            "verse_key": "100:2",
            "text": "فَالۡمُوۡرِيٰتِ قَدۡحًا ۙ‏"
        },
        {
            "id": 6149,
            "verse_key": "100:3",
            "text": "فَالۡمُغِيۡرٰتِ صُبۡحًا ۙ‏"
        },
        {
            "id": 6150,
            "verse_key": "100:4",
            "text": "فَاَثَرۡنَ بِهٖ نَقۡعًا ۙ‏"
        },
        {
            "id": 6151,
            "verse_key": "100:5",
            "text": "فَوَسَطۡنَ بِهٖ جَمۡعًا ۙ‏"
        },
        {
            "id": 6152,
            "verse_key": "100:6",
            "text": "اِنَّ الۡاِنۡسَانَ لِرَبِّهٖ لَـكَنُوۡدٌ ۚ‏"
        },
        {
            "id": 6153,
            "verse_key": "100:7",
            "text": "وَاِنَّهٗ عَلٰى ذٰلِكَ لَشَهِيۡدٌ ۚ‏"
        },
        {
            "id": 6154,
            "verse_key": "100:8",
            "text": "وَاِنَّهٗ لِحُبِّ الۡخَيۡرِ لَشَدِيۡدٌ ؕ‏"
        },
        {
            "id": 6155,
            "verse_key": "100:9",
            "text": "اَفَلَا يَعۡلَمُ اِذَا بُعۡثِرَ مَا فِى الۡقُبُوۡرِۙ‏"
        },
        {
            "id": 6156,
            "verse_key": "100:10",
            "text": "وَحُصِّلَ مَا فِى الصُّدُوۡرِۙ‏"
        },
        {
            "id": 6157,
            "verse_key": "100:11",
            "text": "اِنَّ رَبَّهُمۡ بِهِمۡ يَوۡمَٮِٕذٍ لَّخَبِيۡرٌ‏"
        },
        {
            "id": 6158,
            "verse_key": "101:1",
            "text": "اَلۡقَارِعَةُ ۙ‏"
        },
        {
            "id": 6159,
            "verse_key": "101:2",
            "text": "مَا الۡقَارِعَةُ​ ۚ‏"
        },
        {
            "id": 6160,
            "verse_key": "101:3",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا الۡقَارِعَةُ ؕ‏"
        },
        {
            "id": 6161,
            "verse_key": "101:4",
            "text": "يَوۡمَ يَكُوۡنُ النَّاسُ كَالۡفَرَاشِ الۡمَبۡثُوۡثِۙ‏"
        },
        {
            "id": 6162,
            "verse_key": "101:5",
            "text": "وَتَكُوۡنُ الۡجِبَالُ كَالۡعِهۡنِ الۡمَنۡفُوۡشِؕ‏"
        },
        {
            "id": 6163,
            "verse_key": "101:6",
            "text": "فَاَمَّا مَنۡ ثَقُلَتۡ مَوَازِيۡنُهٗ ۙ‏"
        },
        {
            "id": 6164,
            "verse_key": "101:7",
            "text": "فَهُوَ فِىۡ عِيۡشَةٍ رَّاضِيَةٍ ؕ‏"
        },
        {
            "id": 6165,
            "verse_key": "101:8",
            "text": "وَاَمَّا مَنۡ خَفَّتۡ مَوَازِيۡنُهٗ ۙ‏"
        },
        {
            "id": 6166,
            "verse_key": "101:9",
            "text": "فَاُمُّهٗ هَاوِيَةٌ ؕ‏"
        },
        {
            "id": 6167,
            "verse_key": "101:10",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا هِيَهۡ ؕ‏"
        },
        {
            "id": 6168,
            "verse_key": "101:11",
            "text": "نَارٌ حَامِيَةٌ‏"
        },
        {
            "id": 6169,
            "verse_key": "102:1",
            "text": "اَلۡهٰٮكُمُ التَّكَاثُرُۙ‏"
        },
        {
            "id": 6170,
            "verse_key": "102:2",
            "text": "حَتّٰى زُرۡتُمُ الۡمَقَابِرَؕ‏"
        },
        {
            "id": 6171,
            "verse_key": "102:3",
            "text": "كَلَّا سَوۡفَ تَعۡلَمُوۡنَۙ‏"
        },
        {
            "id": 6172,
            "verse_key": "102:4",
            "text": "ثُمَّ كَلَّا سَوۡفَ تَعۡلَمُوۡنَؕ‏"
        },
        {
            "id": 6173,
            "verse_key": "102:5",
            "text": "كَلَّا لَوۡ تَعۡلَمُوۡنَ عِلۡمَ الۡيَقِيۡنِؕ‏"
        },
        {
            "id": 6174,
            "verse_key": "102:6",
            "text": "لَتَرَوُنَّ الۡجَحِيۡمَۙ‏"
        },
        {
            "id": 6175,
            "verse_key": "102:7",
            "text": "ثُمَّ لَتَرَوُنَّهَا عَيۡنَ الۡيَقِيۡنِۙ‏"
        },
        {
            "id": 6176,
            "verse_key": "102:8",
            "text": "ثُمَّ لَـتُسۡـَٔـلُنَّ يَوۡمَٮِٕذٍ عَنِ النَّعِيۡمِ‏"
        },
        {
            "id": 6177,
            "verse_key": "103:1",
            "text": "وَالۡعَصۡرِۙ‏"
        },
        {
            "id": 6178,
            "verse_key": "103:2",
            "text": "اِنَّ الۡاِنۡسَانَ لَفِىۡ خُسۡرٍۙ‏"
        },
        {
            "id": 6179,
            "verse_key": "103:3",
            "text": "اِلَّا الَّذِيۡنَ اٰمَنُوۡا وَ عَمِلُوا الصّٰلِحٰتِ وَتَوَاصَوۡا بِالۡحَقِّ   ۙ وَتَوَاصَوۡا بِالصَّبۡرِ‏"
        },
        {
            "id": 6180,
            "verse_key": "104:1",
            "text": "وَيۡلٌ لِّـكُلِّ هُمَزَةٍ لُّمَزَةِ ۙ‏"
        },
        {
            "id": 6181,
            "verse_key": "104:2",
            "text": "اۨلَّذِىۡ جَمَعَ مَالًا وَّعَدَّدَهٗ ۙ‏"
        },
        {
            "id": 6182,
            "verse_key": "104:3",
            "text": "يَحۡسَبُ اَنَّ مَالَهٗۤ اَخۡلَدَهٗ​ ۚ‏"
        },
        {
            "id": 6183,
            "verse_key": "104:4",
            "text": "كَلَّا​ لَيُنۡۢبَذَنَّ فِى الۡحُطَمَةِ  ۖ‏"
        },
        {
            "id": 6184,
            "verse_key": "104:5",
            "text": "وَمَاۤ اَدۡرٰٮكَ مَا الۡحُطَمَةُ ؕ‏"
        },
        {
            "id": 6185,
            "verse_key": "104:6",
            "text": "نَارُ اللّٰهِ الۡمُوۡقَدَةُ ۙ‏"
        },
        {
            "id": 6186,
            "verse_key": "104:7",
            "text": "الَّتِىۡ تَطَّلِعُ عَلَى الۡاَفۡـــِٕدَةِ ؕ‏"
        },
        {
            "id": 6187,
            "verse_key": "104:8",
            "text": "اِنَّهَا عَلَيۡهِمۡ مُّؤۡصَدَةٌ ۙ‏"
        },
        {
            "id": 6188,
            "verse_key": "104:9",
            "text": "فِىۡ عَمَدٍ مُّمَدَّدَةٍ‏"
        },
        {
            "id": 6189,
            "verse_key": "105:1",
            "text": "اَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِاَصۡحٰبِ الۡفِيۡلِؕ‏"
        },
        {
            "id": 6190,
            "verse_key": "105:2",
            "text": "اَلَمۡ يَجۡعَلۡ كَيۡدَهُمۡ فِىۡ تَضۡلِيۡلٍۙ‏"
        },
        {
            "id": 6191,
            "verse_key": "105:3",
            "text": "وَّاَرۡسَلَ عَلَيۡهِمۡ طَيۡرًا اَبَابِيۡلَۙ‏"
        },
        {
            "id": 6192,
            "verse_key": "105:4",
            "text": "تَرۡمِيۡهِمۡ بِحِجَارَةٍ مِّنۡ سِجِّيۡلٍۙ‏"
        },
        {
            "id": 6193,
            "verse_key": "105:5",
            "text": "فَجَعَلَهُمۡ كَعَصۡفٍ مَّاۡكُوۡلٍ‏"
        },
        {
            "id": 6194,
            "verse_key": "106:1",
            "text": "لِاِيۡلٰفِ قُرَيۡشٍۙ‏"
        },
        {
            "id": 6195,
            "verse_key": "106:2",
            "text": "اٖلٰفِهِمۡ رِحۡلَةَ الشِّتَآءِ وَالصَّيۡفِ​ۚ‏"
        },
        {
            "id": 6196,
            "verse_key": "106:3",
            "text": "فَلۡيَـعۡبُدُوۡا رَبَّ هٰذَا الۡبَيۡتِۙ‏"
        },
        {
            "id": 6197,
            "verse_key": "106:4",
            "text": "الَّذِىۡۤ اَطۡعَمَهُمۡ مِّنۡ جُوۡعٍ   ۙ وَّاٰمَنَهُمۡ مِّنۡ خَوۡفٍ‏"
        },
        {
            "id": 6198,
            "verse_key": "107:1",
            "text": "اَرَءَيۡتَ الَّذِىۡ يُكَذِّبُ بِالدِّيۡنِؕ‏"
        },
        {
            "id": 6199,
            "verse_key": "107:2",
            "text": "فَذٰلِكَ الَّذِىۡ يَدُعُّ الۡيَتِيۡمَۙ‏"
        },
        {
            "id": 6200,
            "verse_key": "107:3",
            "text": "وَ لَا يَحُضُّ عَلٰى طَعَامِ الۡمِسۡكِيۡنِؕ‏"
        },
        {
            "id": 6201,
            "verse_key": "107:4",
            "text": "فَوَيۡلٌ لِّلۡمُصَلِّيۡنَۙ‏"
        },
        {
            "id": 6202,
            "verse_key": "107:5",
            "text": "الَّذِيۡنَ هُمۡ عَنۡ صَلَاتِهِمۡ سَاهُوۡنَۙ‏"
        },
        {
            "id": 6203,
            "verse_key": "107:6",
            "text": "الَّذِيۡنَ هُمۡ يُرَآءُوۡنَۙ‏"
        },
        {
            "id": 6204,
            "verse_key": "107:7",
            "text": "وَيَمۡنَعُوۡنَ الۡمَاعُوۡنَ‏"
        },
        {
            "id": 6205,
            "verse_key": "108:1",
            "text": "اِنَّاۤ اَعۡطَيۡنٰكَ الۡكَوۡثَرَؕ‏"
        },
        {
            "id": 6206,
            "verse_key": "108:2",
            "text": "فَصَلِّ لِرَبِّكَ وَانۡحَرۡ ؕ‏"
        },
        {
            "id": 6207,
            "verse_key": "108:3",
            "text": "اِنَّ شَانِئَكَ هُوَ الۡاَبۡتَرُ‏"
        },
        {
            "id": 6208,
            "verse_key": "109:1",
            "text": "قُلۡ يٰۤاَيُّهَا الۡكٰفِرُوۡنَۙ‏"
        },
        {
            "id": 6209,
            "verse_key": "109:2",
            "text": "لَاۤ اَعۡبُدُ مَا تَعۡبُدُوۡنَۙ‏"
        },
        {
            "id": 6210,
            "verse_key": "109:3",
            "text": "وَلَاۤ اَنۡـتُمۡ عٰبِدُوۡنَ مَاۤ اَعۡبُدُ​ ۚ‏"
        },
        {
            "id": 6211,
            "verse_key": "109:4",
            "text": "وَلَاۤ اَنَا عَابِدٌ مَّا عَبَدۡتُّمۡۙ‏"
        },
        {
            "id": 6212,
            "verse_key": "109:5",
            "text": "وَ لَاۤ اَنۡـتُمۡ عٰبِدُوۡنَ مَاۤ اَعۡبُدُ ؕ‏"
        },
        {
            "id": 6213,
            "verse_key": "109:6",
            "text": "لَـكُمۡ دِيۡنُكُمۡ وَلِىَ دِيۡنِ‏"
        },
        {
            "id": 6214,
            "verse_key": "110:1",
            "text": "اِذَا جَآءَ نَصۡرُ اللّٰهِ وَالۡفَتۡحُۙ‏"
        },
        {
            "id": 6215,
            "verse_key": "110:2",
            "text": "وَرَاَيۡتَ النَّاسَ يَدۡخُلُوۡنَ فِىۡ دِيۡنِ اللّٰهِ اَفۡوَاجًا ۙ‏"
        },
        {
            "id": 6216,
            "verse_key": "110:3",
            "text": "فَسَبِّحۡ بِحَمۡدِ رَبِّكَ وَاسۡتَغۡفِرۡهُ​ ؕ اِنَّهٗ كَانَ تَوَّابًا‏"
        },
        {
            "id": 6217,
            "verse_key": "111:1",
            "text": "تَبَّتۡ يَدَاۤ اَبِىۡ لَهَبٍ وَّتَبَّؕ‏"
        },
        {
            "id": 6218,
            "verse_key": "111:2",
            "text": "مَاۤ اَغۡنٰى عَنۡهُ مَالُهٗ وَمَا كَسَبَؕ‏"
        },
        {
            "id": 6219,
            "verse_key": "111:3",
            "text": "سَيَصۡلٰى نَارًا ذَاتَ لَهَبٍ ۖۚ‏"
        },
        {
            "id": 6220,
            "verse_key": "111:4",
            "text": "وَّامۡرَاَ تُهٗ ؕ حَمَّالَةَ الۡحَطَبِ​ۚ‏"
        },
        {
            "id": 6221,
            "verse_key": "111:5",
            "text": "فِىۡ جِيۡدِهَا حَبۡلٌ مِّنۡ مَّسَدٍ‏"
        },
        {
            "id": 6222,
            "verse_key": "112:1",
            "text": "قُلۡ هُوَ اللّٰهُ اَحَدٌ​ ۚ‏"
        },
        {
            "id": 6223,
            "verse_key": "112:2",
            "text": "اَللّٰهُ الصَّمَدُ​ ۚ‏"
        },
        {
            "id": 6224,
            "verse_key": "112:3",
            "text": "لَمۡ يَلِدۡ   ۙ وَلَمۡ يُوۡلَدۡ ۙ‏"
        },
        {
            "id": 6225,
            "verse_key": "112:4",
            "text": "وَلَمۡ يَكُنۡ لَّهٗ كُفُوًا اَحَدٌ‏"
        },
        {
            "id": 6226,
            "verse_key": "113:1",
            "text": "قُلۡ اَعُوۡذُ بِرَبِّ الۡفَلَقِۙ‏"
        },
        {
            "id": 6227,
            "verse_key": "113:2",
            "text": "مِنۡ شَرِّ مَا خَلَقَۙ‏"
        },
        {
            "id": 6228,
            "verse_key": "113:3",
            "text": "وَمِنۡ شَرِّ غَاسِقٍ اِذَا وَقَبَۙ‏"
        },
        {
            "id": 6229,
            "verse_key": "113:4",
            "text": "وَمِنۡ شَرِّ النَّفّٰثٰتِ فِى الۡعُقَدِۙ‏"
        },
        {
            "id": 6230,
            "verse_key": "113:5",
            "text": "وَمِنۡ شَرِّ حَاسِدٍ اِذَا حَسَدَ‏"
        },
        {
            "id": 6231,
            "verse_key": "114:1",
            "text": "قُلۡ اَعُوۡذُ بِرَبِّ النَّاسِۙ‏"
        },
        {
            "id": 6232,
            "verse_key": "114:2",
            "text": "مَلِكِ النَّاسِۙ‏"
        },
        {
            "id": 6233,
            "verse_key": "114:3",
            "text": "اِلٰهِ النَّاسِۙ‏"
        },
        {
            "id": 6234,
            "verse_key": "114:4",
            "text": "مِنۡ شَرِّ الۡوَسۡوَاسِ  ۙ الۡخَـنَّاسِ ۙ‏"
        },
        {
            "id": 6235,
            "verse_key": "114:5",
            "text": "الَّذِىۡ يُوَسۡوِسُ فِىۡ صُدُوۡرِ النَّاسِۙ‏"
        },
        {
            "id": 6236,
            "verse_key": "114:6",
            "text": "مِنَ الۡجِنَّةِ وَالنَّاسِ‏"
        }
    ]
}